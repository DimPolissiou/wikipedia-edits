cription = Dadhi Mati Mata Temple \n| location = [[Nagaur]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image =  \n}}\n{{ASI Monument row\n| number = S-RJ-201 \n| description = Sun Temple \n| location = [[Pali]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n|image=Ranakpurtemple.jpg\n}}\n{{ASI Monument row\n| number = S-RJ-202 \n| description = Group of Cenotaph \n| location = [[Rajasamand]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image =  \n}}\n{{ASI Monument row\n| number = S-RJ-203 \n| description = Charbhuja Temple \n| location = [[Rajasamand]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image =  \n}}\n{{ASI Monument row\n| number = S-RJ-204 \n| description = Durga Temple \n| location = [[Rajasamand]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image =  \n}}\n{{ASI Monument row\n| number = S-RJ-205 \n| description = Nadol \n| location = [[Rajasamand]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image =  \n}}\n{{ASI Monument row\n| number = S-RJ-206 \n| description = Makar Mandi Mata \n| location = [[Rajasamand]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image =  \n}}\n{{ASI Monument row\n| number = S-RJ-207 \n| description = Fort of Fatehpur \n| location = [[Sikar]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image =  \n}}\n{{ASI Monument row\n| number = S-RJ-208 \n| description = Sun Temple \n| location = [[Sirohi]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image =  \n}}\n{{ASI Monument row\n| number = S-RJ-209 \n| description = Sunahari Kothi \n| location = [[Tonk, India|Tonk]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image =  \n}}\n{{ASI Monument row\n| number = S-RJ-210 \n| description = [[Jagdish Temple, Udaipur|Jagdish Temple]]\n| location = [[Udaipur]] \n| address =  \n| district =  \n| lon =  \n| lat =  \n| image = Jagdish Mandir.JPG\n|commonscat=Jagdish Temple\n}}\n{{ASI Monument row\n| number = S-RJ-211 \n| descriptio