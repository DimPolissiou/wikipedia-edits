eds source -->\n|-\n!scope=row|{{sortname|Terry|Cooper|dab=footballer, born 1950}}\n|DF||{{sort|1971\u20131979|}}{{ublist|1971\u20131972|1972\u20131979|list_style=text-align:center}}||270||12||299||13||||<ref group=L>[[Notts County]]</ref>\n|-\n!scope=row|{{sortname|Phil|Hubbard|dab=footballer}}\n|MF||{{sort|1966\u20131979|}}{{ublist|1966\u20131971|1976\u20131979|list_style=text-align:center}}||261||53||295||60||||\n|-\n!scope=row|{{sortname|John|Schofield|dab=footballer}}\n|MF||{{sort|1988\u20132001|}}{{ublist|1988\u20131994|2000\u20132001|list_style=text-align:center}}||250||11||293||16||||\n|-\n!scope=row|{{sortname|Roy|Finch|dab=footballer}}\n|FW||1949\u20131958||275||56||291||58||||\n|-\n!scope=row|{{sortname|Andy|Graver}}\n|FW||{{sort|1950\u20131961|}}{{ublist|1950\u20131954|1955|1958\u20131961|list_style=text-align:center}}||274||143||289||150||||\n|-\n!scope=row|{{sortname|Bert|Linnecor}}\n|HB||1957\u20131964||264||52||287||55||||\n|-\n!scope=row|{{sortname|Phil|Turner|Phil Turner (footballer, born 1962)}}\n|MF||1980\u20131986||239||18||283||20||||\n|-\n!scope=row|{{sortname|Jim|Grummett, Jr.}}\n|DF||1963\u20131971||251||19||279||21||style=text-align:left|{{fb|ENG}} youth\n|<ref>{{cite news |title=England's Youth team |newspaper=The Guardian |date=6 May 1963 |page=12}}</ref>\n|-\n!scope=row|{{sortname|John|Kennedy|dab=Northern Irish footballer}}\n|GK||1967\u20131974||251||0||278||0||||\n|-\n!scope=row|{{sortname|George|Shipley|dab=footballer}}\n|MF||1980\u20131985||230||42||274||52||||\n|-\n!scope=row|{{sortname|John|Ward|dab=footballer, born 1951}}\n|FW||{{sort|1971\u20131982|}}{{ublist|1971\u20131979|1982|list_style=text-align:center}}||241||90||263||99||||\n|-\n!scope=row|{{sortname|Peter|Grotier}}\n|GK||1974\u20131979||233||0||263||0||||\n|-\n!scope=row|{{sortname|Peter|Gain}}\n|MF||1999\u20132005||228||21||263||22||style=text-align:left|{{fbu|21|IRL|name=Republic of Ireland U21}}\n|<ref>{{cite news |url=https://www.independent.co.uk/sport/football-unwan