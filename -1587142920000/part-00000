\" | 8\n| ALIGN=\"center\" | 18\n|- ALIGN=\"center\"\n| ALIGN=\"center\" | 1989\u201390\n| ALIGN=\"center\" | [[Jokipojat|JoKP]]\n| ALIGN=\"center\" | SM-l\n| ALIGN=\"center\" | 44\n| ALIGN=\"center\" | 18\n| ALIGN=\"center\" | 14\n| ALIGN=\"center\" | 32\n| ALIGN=\"center\" | 30\n| ALIGN=\"center\" | \u2014\n| ALIGN=\"center\" | \u2014\n| ALIGN=\"center\" | \u2014\n| ALIGN=\"center\" | \u2014\n| ALIGN=\"center\" | \u2014\n|- ALIGN=\"center\" bgcolor=\"#f0f0f0\"\n| ALIGN=\"center\" | 1990\u201391\n| ALIGN=\"center\" | [[SaiPa]]\n| ALIGN=\"center\" | SM-l\n| ALIGN=\"center\" | 44\n| ALIGN=\"center\" | 18\n| ALIGN=\"center\" | 22\n| ALIGN=\"center\" | 40\n| ALIGN=\"center\" | 28\n| ALIGN=\"center\" | \u2014\n| ALIGN=\"center\" | \u2014\n| ALIGN=\"center\" | \u2014\n| ALIGN=\"center\" | \u2014\n| ALIGN=\"center\" | \u2014\n|- ALIGN=\"center\"\n| ALIGN=\"center\" | 1991\u201392\n| ALIGN=\"center\" | [[Lukko]]\n| ALIGN=\"center\" | SM-l\n| ALIGN=\"center\" | 44\n| ALIGN=\"center\" | 15\n| ALIGN=\"center\" | 8\n| ALIGN=\"center\" | 23\n| ALIGN=\"center\" | 38\n| ALIGN=\"center\" | 2\n| ALIGN=\"center\" | 0\n| ALIGN=\"center\" | 0\n| ALIGN=\"center\" | 0\n| ALIGN=\"center\" | 0\n|- ALIGN=\"center\" bgcolor=\"#f0f0f0\"\n| ALIGN=\"center\" | 1992\u201393\n| ALIGN=\"center\" | Lukko\n| ALIGN=\"center\" | SM-l\n| ALIGN=\"center\" | 48\n| ALIGN=\"center\" | 12\n| ALIGN=\"center\" | 17\n| ALIGN=\"center\" | 29\n| ALIGN=\"center\" | 40\n| ALIGN=\"center\" | 3\n| ALIGN=\"center\" | 1\n| ALIGN=\"center\" | 0\n| ALIGN=\"center\" | 1\n| ALIGN=\"center\" | 2\n|- ALIGN=\"center\"\n| ALIGN=\"center\" | 1993\u201394\n| ALIGN=\"center\" | Lukko\n| ALIGN=\"center\" | SM-l\n| ALIGN=\"center\" | 47\n| ALIGN=\"center\" | 19\n| ALIGN=\"center\" | 23\n| ALIGN=\"center\" | 42\n| ALIGN=\"center\" | 52\n| ALIGN=\"center\" | 9\n| ALIGN=\"center\" | 1\n| ALIGN=\"center\" | 4\n| ALIGN=\"center\" | 5\n| ALIGN=\"center\" | 10\n|- ALIGN=\"center\" bgcolor=\"#f0f0f0\"\n| ALIGN=\"center\" | 1994\u201395\n