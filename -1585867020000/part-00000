3 nifs.no] {{in lang|no}}\n| update = complete |start_date= 2 May 1987\n| a_note = \n| matches_style = FBR\n| team1=BRA | team2=BRY | team3=HAM | team4=KON | team5=LIL | team6=MIF  | team7=MOL | team8=MOS | team9=ROS | team10=IKS | team11=TRO | team12=V\u00c5L\n\n| name_BRA = [[SK Brann|Brann]]\n| name_BRY = [[Bryne FK|Bryne]]\n| name_HAM = [[HamKam]]\n| name_KON = [[Kongsvinger IL Toppfotball|Kongsvinger]]\n| name_LIL = [[Lillestr\u00f8m SK|Lillestr\u00f8m]]\n| name_MIF = [[Mj\u00f8ndalen IF|Mj\u00f8ndalen]]\n| name_MOL = [[Molde FK|Molde]]\n| name_MOS = [[Moss FK|Moss]]\n| name_ROS = [[Rosenborg BK|Rosenborg]]\n| name_IKS = [[IK Start|Start]]\n| name_TRO = [[Troms\u00f8 IL|Troms\u00f8]]\n| name_V\u00c5L = [[V\u00e5lerenga Fotball|V\u00e5lerengen]]\n\n| match_BRA_BRY = 1-3\n| match_BRA_HAM = 0-0<sup>(5\u20134)</sup>\n| match_BRA_KON = 2-1\n| match_BRA_LIL = 0-1\n| match_BRA_MIF = 1-1<sup>(11\u201312)</sup>\n| match_BRA_MOL = 0-2\n| match_BRA_MOS = 3-1\n| match_BRA_ROS = 1-1<sup>(5\u20136)</sup>\n| match_BRA_IKS = 1-0\n| match_BRA_TRO = 5-0\n| match_BRA_V\u00c5L = 1-0\n\n| match_BRY_BRA = 1-2\n| match_BRY_HAM = 4-2\n| match_BRY_KON = 4-1\n| match_BRY_LIL = 1-0\n| match_BRY_MIF = 3-0\n| match_BRY_MOL = 2-1\n| match_BRY_MOS = 1-2\n| match_BRY_ROS = 0-2\n| match_BRY_IKS = 0-1\n| match_BRY_TRO = 0-1\n| match_BRY_V\u00c5L = 2-1\n\n| match_HAM_BRA = 4-1\n| match_HAM_BRY = 1-0\n| match_HAM_KON = 2-2<sup>(5\u20133)</sup>\n| match_HAM_LIL = 2-1\n| match_HAM_MIF = 1-1<sup>(4\u20133)</sup>\n| match_HAM_MOL = 0-2\n| match_HAM_MOS = 2-0\n| match_HAM_ROS = 3-0\n| match_HAM_IKS = 3-0\n| match_HAM_TRO = 1-1<sup>(4\u20135)</sup>\n| match_HAM_V\u00c5L = 3-5\n\n| match_KON_BRA = 2-0\n| match_KON_BRY = 4-0\n| match_KON_HAM = 0-1\n| match_KON_LIL = 1-0\n| match_KON_MIF = 1-0\n| match_KON_MOL = 1-0\n| match_KON_MOS = 0-1\n| match_KON_ROS = 1-1<sup>(2\u20134)</sup>\n| match_KON_IKS = 3-4\n| match_KON_TRO = 0-0<sup>(4\u20135)</sup>\n| match_KON_V\u00c5L = 2-2<sup>(4\u20132)</sup>\n\n| matc