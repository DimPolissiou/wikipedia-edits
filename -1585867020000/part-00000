]])<ref>{{cite web|url=http://www.vglista.no/topplister/topp-20-single-2020-01/|title=VG-lista \u2013 Topp 20 Single uke 1, 2020|publisher=[[VG-lista]]|accessdate=January 4, 2020}}</ref>\n| 1\n|-\n{{single chart|Poland|1|chartid=3316|year=2020|rowheader=true|accessdate=February 17, 2020}}\n|-\n{{single chart|Portugal|1|artist=The Weeknd|song=Blinding Lights|rowheader=true|accessdate=March 5, 2020}}\n|-\n! scope=\"row\"| Romania ([[Airplay 100 (Romania)|Airplay 100]])<!--Please do not change this to Romanian Top 100 or Media Forest. The Airplay 100 is Romania's national singles chart from 2012 onwards. For further information, see WP:GOODCHARTS--><ref>{{cite web|url=http://www.kissfm.ro/podcasturi/6855|title=Airplay 100 \u2013 22 martie 2020|publisher=[[Kiss FM (Romania)|Kiss FM]]|language=Romanian|accessdate=March 25, 2020|date=March 22, 2020}}</ref>\n| 10\n|-\n{{single chart|Russia|2|rowheader=true|date=February 14, 2020|accessdate=February 21, 2020|refname=\"russianairplaychart\"}}\n|-\n{{single chart|Scotland|1|date=20200124|rowheader=true|accessdate=January 25, 2020}}\n|-\n! scope=\"row\"| Singapore ([[Recording Industry Association (Singapore)|RIAS]])<ref>{{cite web|url=https://www.rias.org.sg/rias-top-charts/|title=RIAS International Top Charts Week 10|publisher=Recording Industry Association (Singapore)|archive-url=https://web.archive.org/web/20200313063525/https://www.rias.org.sg/rias-top-charts/|archive-date=March 13, 2020}}</ref>\n| 6\n|-\n{{single chart|Slovakia|4|year=2020|week=10|rowheader=true|accessdate=March 10, 2020}}\n|-\n{{single chart|Slovakdigital|1|year=2020|week=09|rowheader=true|accessdate=March 3, 2020}}\n|-\n! scope=\"row\"| Slovenia ([[SloTop50]])<ref>{{cite web|url=https://www.slotop50.si/Glasbene-lestvice/Tedenske-lestvice/?year=2020&week=12|title=SloTop50 \u2013 Slovenian official singles chart|publisher=slotop50.si|accessdate=March 23, 2020}}</ref>\n| 1\n|-\n! scope=\"row\"| Spain ([[Productores de M\u00fasica de Espa\u00f1a|PROMUSICAE]])<ref>{{cite web|url=https://www.elportaldemusica.es/lists/top-100-canciones/2020/9|title=Top 100 Canciones: Semana 9|publisher=[[Productores de M\u00fasica de Espa\u00f1a]]|accessdate=March 4, 2020}}</ref>\n| 16\n|-\n! scope=\"row\"| Sweden ([[Sverigetopplistan]])<ref>{{cite web|url=https://www.sverigetopplistan.se/chart/41?dspy=2020&dspp=2|title=Veckolista Singlar, vecka 2|publisher=[[Sverigetopplistan]]|accessdate=January 11, 2020}}</ref>\n| 1\n|-\n{{single chart|Switzerland|1|artist=The Weeknd|song=Blinding Lights|rowheader=true|accessdate=February 10, 2020}}\n|-\n{{single chart|Ukraine|40|rowheader=true|date=March 20, 2020|accessdate=March 27, 2020}}\n|-\n{{single chart|UK|1|date=20200207|rowheader=true|accessdate=February 8, 2020}}\n|-\n{{single chart|UKrandb|1|date=20200206|rowheader=true|accessdate February 2, 2019}}\n|-\n{{single chart|Billboardhot100|1|artist=The Weeknd|rowheader=true|accessdate=March 31, 2020|note=<ref name=\"BH100 Peak\"/>}}\n|-\n{{single chart|Billboardadultpopsongs|9|artist=The Weeknd|rowheader=true|accessdate=March 31, 2020}}\n|-\n{{single chart|Billboarddanceairplay|5|artist=The Weeknd|rowheader=true|accessdate=March 16, 2020}}\n|-\n{{single chart|Billboarddanceclubplay|44|artist=The Weeknd|rowheader=true|accessdate=March 20, 2020}}\n|-\n{{single chart|Billboardrandbhiphop|1|artist=The Weeknd|rowheader=true|accessdate=March 31, 2020}}\n|-\n{{single chart|Billboardpopsongs|4|artist=The Weeknd|rowheader=true|accessdate=March 31, 2020}}\n|-\n{{single chart|Billboardrhythmic|6|artist=The Weeknd|rowheader=true|accessdate=March 31, 2020}}\n|-\n! scope=\"row\"| US [[Rolling Stone Top 100|''Rolling Stone'' Top 100]]<ref name=\"RS 100\">{{Cite web|url=https://www.rollingstone.com/charts/songs/2020-3-26/|title=Top 100 Songs, March 20 \u2013 March 26, 2020|work=[[Rolling Stone]]|date=March 26, 2020|accessdate=March 31, 2020}}</ref>\n| 1\n|}\n\n==Certifications==\n{{Certification Table Top}}\n{{Certification Table Entry|region=Australia|type=single|award=Platinum|number=2|relyear=2019|certyear=2020|certref=<ref>{{cite web|url=http://www.ariacharts.com.au/charts/singles-chart|title=ARIA Australian Top 50 Singles|publisher=[[Australian Recording Industry Association]]|date=March 9, 2020|accessdate=March 7, 2020}}</ref>}}\n{{Certification Table Entry|region=Belgium|award=Platinum|type=single|relyear=2019|certyear=2020|accessdate=February 28, 2020}}\n{{Certification Table Entry|region=Canada|artist=The Weeknd|title=Blinding Lights|award=Platinum|number=2|type=single|relyear=2019|certyear=2020|accessdate=February 25, 2020}}\n{{Certification Table Entry|region=Denmark|artist=The Weeknd|title=Blinding Lights|award=Platinum|type=single|relyear=2019|certyear=2020|id=9064|accessdate=March 24, 2020}}\n{{Certification Table Entry|region=France|artist=The Weeknd|title=Blinding Lights|award=Platinum|type=single|relyear=2019|certyear=2020|accessdate=February 18, 2020}}\n{{Certification Table Entry|region=Germany|artist=The Weeknd|title=Blinding Lights|award=Gold|type=single|relyear=2019|certyear=2020|accessdate=February 13, 2020}}\n{{Certification Table Entry|region=Italy|artist=The Weeknd|title=Blinding Lights|award=Platinum|type=single|relyear=2019|certyear=2020|salesamount=70,000|accessdate=March 16, 2020}}\n{{Certification Table Entry|region=Mexico|award=Platinum+Gold|type=single|relyear=2019|certyear=2020|accessdate=February 29, 2020}}\n{{Certification Table Entry|region=New Zealand|artist=The Weeknd|title=Blinding Lights|award=Platinum|type=single|relyear=2019|relmonth=11|recent=website|id=4828|accessdate=March 7, 2020}}\n{{Certification Table Entry|region=Norway|artist=The Weeknd|title=Blinding Lights|award=Platinum|type=single|relyear=2019|certyear=2020|salesamount=60,000|accessdate=January 29, 2020}}\n{{Certification Table Entry|region=Spain|artist=The Weeknd|title=Blinding Lights|award=Gold|type=single|relyear=2019|certyear=2020|id=the-weeknd-blinding-lights|accessdate=March 5, 2020}}\n{{Certification Table Entry|region=United Kingdom|artist=The Weeknd|title=Blinding Lights|award=Platinum|type=single|relyear=2019|certyear=2020|id=16157-4005-1|accessdate=March 6, 2020}}\n{{Certification Table Entry|region=United States|artist=The Weeknd|title=Blinding Lights|award=Platinum|type=single|relyear=2019|certyear=2020|accessdate=March 20, 2020}}\n{{Certification Table Bottom|streaming=true}}\n\n==Release history==\n{|class=\"wikitable plainrowheaders\"\n|-\n! scope=\"col\"| Region\n! scope=\"col\"| Date\n! scope=\"col\"| Format\n! scope=\"col\"| Label(s)\n! scope=\"col\"| {{abbr|Ref.|Reference}}\n|-\n! scope=\"row\"| Various\n| December 6, 2019\n| {{hlist|[[Music download|Digital download]]|[[Streaming media|streaming]]}}\n| {{hlist|[[XO (record label)|XO]]|[[Republic Records|Republic]]}}\n| <ref>{{cite web|url=https://hiphop-n-more.com/2019/11/the-weeknd-to-release-2-new-singles-blinding-lights-heartless-this-week/|title=The Weeknd to Release 2 New Singles 'Blinding Lights' & 'Heartless' This Week|website=Hiphop-n-more|accessdate=December 2, 2019}}</ref>\n|-\n! scope=\"row\"| Italy\n| December 13, 2019\n| rowspan=\"2\"| [[Contemporary hit radio]]\n| [[Universal Music Group|Universal]]\n| <ref>{{cite web|url=http://radiodate.it/radio-date/the-weeknd-blinding-lights-190109-13-12-2019-radiodate/|title=Blinding Lights THE WEEKND|website=Radio Date|accessdate=December 11, 2019}}</ref>\n|-\n! scope=\"row\"| United Kingdom\n| December 20, 2019\n| rowspan=\"5\"| {{hlist|XO|Republic}}\n| <ref>{{cite web|url=https://www.bbc.co.uk/programmes/articles/3tqPdBWF9yMbTrfjWvfKV8t/radio-1-playlist|archiveurl=https://web.archive.org/web/20191220184733/https://www.bbc.co.uk/programmes/articles/3tqPdBWF9yMbTrfjWvfKV8t/radio-1-playlist|archivedate=December 20, 2019|title= Radio 1 Playlist|website=[[BBC]]|accessdate=December 20, 2019}}</ref>\n|-\n!scope=\"row\" rowspan=\"2\"|United States\n|January 6, 2020\n|[[Hot adult contemporary|Hot adult contemporary radio]]\n|<ref>{{cite web |title=Hot Modern 