ey|last=Coleman}}\n{{American football roster/Player|num=2 |pos=WR|class=FR|link=n|first=Blake|last=Lynch}}\n{{American football roster/Player|num=3 |pos=QB|class=FR|link=y|first=Jarrett|last=Stidham}}\n{{American football roster/Player|num=4 |pos=WR|class=SR|rs=y|link=n|first=Jay|last=Lee}}\n{{American football roster/Player|num=5 |pos=RB|class=SO|rs=y|link=n|first=Johnny|last=Jefferson}}\n{{American football roster/Player|num=6 |pos=RB|class=FR|link=n|first=JaMycal|last=Hasty}}\n{{American football roster/Player|num=7 |pos=WR|class=JR|rs=y|link=n|first=Lynx|last=Hawthorne}}\n{{American football roster/Player|num=8 |pos=WR|class=FR|link=y|rs=y|first=Ishmael|last=Zamora}}\n{{American football roster/Player|num=9 |pos=WR|class=SO|link=y|first=K. D.|last=Cannon}}\n{{American football roster/Player|num=11|pos=WR|class=FR|link=n|first=Pooh|last=Stricklin}}\n{{American football roster/Player|num=12|pos=WR|class=SO|rs=y|link=n|first=Quan|last=Jones}}\n{{American football roster/Player|num=13|pos=QB|class=SO|rs=y|link=n|first=Chris|last=Johnson}}\n{{American football roster/Player|num=14|pos=WR|class=FR|rs=y|first=Chris|last=Platt}}\n{{American football roster/Player|num=15|pos=TE|class=SR|rs=y|link=n|first=Gus|last=Penning}}\n{{American football roster/Player|num=16|pos=WR|class=SO|link=n|first=Davion|last=Hall}}\n{{American football roster/Player|num=17|pos=QB|class=JR|rs=y|link=y|first=Seth|last=Russell}}\n{{American football roster/Player|num=18|pos=QB|class=FR|link=n|first=Zack|last=Bennema}}\n{{American football roster/Player|num=20|pos=RB|class=SR|rs=y|link=n|first=Anthony|last=Webb}}\n{{American football roster/Player|num=22|pos=RB|class=FR|rs=y|first=Terence|last=Williams}}\n{{American football roster/Player|num=25|pos=RB|class=FR|link=n|first=Dwyer|last=Bucey}}\n{{American football roster/Player|num=28|pos=RB|class=JR|rs=y|link=n|first=Devin|last=Chafin}}\n{{American football roster/Player|num=30|pos=WR|class=FR|rs=y|link=n|first=Tanner|last=Ritchey}}\n{{American