ine]] || style=\"background:blue; color:white\" | || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  ||style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" | || style=\"background:blue; color:white\" | || style=\"background:blue; color:white\" | || style=\"background:blue; color:white\" | || style=\"background:blue; color:white\" | || style=\"background:blue; color:white\" | || style=\"background:blue; color:white\" | ||style=\"background:blue; color:white\" | ||style=\"background:blue; color:white\" | ||style=\"background:blue; color:white\" | ||style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" |  || style=\"background:blue; color:white\" | \n|-\n| 41 || [[United Kingdom]] || style=\"background:grey; color:white\" | || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" |  || style=\"background:grey; color:white\" | || style=\"background:grey; color:white\" | || style=\"background:grey; color:white\" | || style=\"background:grey; color:white\" | || style=\"background:orange; color:white\" | || style=\"background:grey; color:white\" | || 