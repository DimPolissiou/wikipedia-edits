dt|pos=OL|link=y}}\n{{American football roster/Player|num=55|class=FR|rs=y|first=Jake|last=Heinrich|pos=OL}}\n{{American football roster/Player|num=57|class=FR|first=Shane|last=Clenin|pos=OL}}\n{{American football roster/Player|num=60|class=JR|first=Brian|last=Wallace|pos=OL}}\n{{American football roster/Player|num=61|class=SO|first=Cooper|last=Sone|pos=OL}}\n{{American football roster/Player|num=62|class=JR|first=Johnny|last=Gibson Jr.|pos=OL}}\n{{American football roster/Player|num=64|class=FR|first=Audry|last=Horn|pos=OL}}\n{{American football roster/Player|num=66|class=FR|first=Ty|last=Clary|pos=OL}}\n{{American football roster/Player|num=67|class=FR|rs=y|first=Tyler|last=Hall|pos=OL}}\n{{American football roster/Player|num=68|class=FR|first=Kirby|last=Adcock|pos=OL}}\n{{American football roster/Player|num=69|class=FR|rs=y|first=Dylan|last=Hays|pos=OL}}\n{{American football roster/Player|num=71|class=JR|first=Jackson|last=Hannah|pos=OL}}\n{{American football roster/Player|num=72|class=SR|first=Frank|last=Ragnow|pos=OL|link=y|note=C}}\n{{American football roster/Player|num=73|class=JR|first=Deion|last=Malone|pos=OL}}\n{{American football roster/Player|num=74|class=SO|first=Colton|last=Jackson|pos=OL}}\n{{American football roster/Player|num=75|class=JR|first=Zach|last=Rogers|pos=OL}}\n{{American football roster/Player|num=76|class=SR|first=Paul|last=Ramirez|pos=OL}}\n{{American football roster/Player|num=78|class=FR|first=Dalton|last=Wagner|pos=OL|inj=y}}\n{{American football roster/Player|num=79|class=SO|first=Jalen|last=Merrick|pos=OL}}\n{{American football roster/Player|num=80|class=SO|first=Will|last=Gragg|pos=TE}}\n{{American football roster/Player|num=81|class=FR|first=Jarrod|last=Barnes|pos=WR}}\n{{American football roster/Player|num=82|class=JR|first=Gary|last=Cross|pos=WR}}\n{{American football roster/Player|num=83|class=FR|first=De'Shawn|last=Gulledge|pos=WR}}\n{{American football roster/Player|num=84|class=FR|first=Tyson|last=Morris|pos=WR}}\n{{American