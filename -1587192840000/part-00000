ues = offset:2\n | xAxisTitle = Date\n | xAxisAngle = -50\n | type = rect\n | x = 24/1, 25/1, 26/1, 27/1, 28/1, ..., 3/2, 4/2, 5/2, 6/2, 7/2, 8/2, 9/2, ...., 12/2, 13/2, 14/2, 15/2, 16/2, 17/2, 18/2, 19/2, ....., 23/2, 24/2, ......, 27/2, 28/2, 29/2, 1/3, 2/3, 3/3, 4/3, 5/3, 6/3, 7/3, 8/3, 9/3, 10/3, 11/3, 12/3, 13/3, 14/3, 15/3, 16/3, 17/3, 18/3, 19/3, 20/3, 21/3, 22/3, 23/3, 24/3, 25/3, 26/3, 27/3, 28/3, 29/3, 30/3, 31/3, 1/4, 2/4, 3/4, 4/4, 5/4, 6/4, 7/4, 8/4, 9/4, 10/4, 11/4, 12/4, 13/4, 14/4, 15/4, 16/4, 17/4\n | yAxisTitle = No. of active cases\n | legend = Legend\n | y1 = 3, 4, 4, 6, 8, 8, 9, 11, 13, 13, 14, 14, 15, 15, 16, 16, 14, 14, 14, 13, 9, 5, 5, 4, 2, 2, 2, 3, 7, 7, 11, 14, 28, 33, 60, 68, 74, 92, 104, 122, 125, 167, 206, 386, 511, 622, 728, 823, 940, 1061, 1156, 1345, 1425, 1577, 1792, 1876, 1973, 2047, 2110, 2186, 2218, 2299, 2453, 2511, 2596, 2490, 2579, 2567, 2553, 2446, 2462, 2499, 2464, 2427, 2342, 2332, 2198\n | y1Title = Active cases\n | xGrid = \n | yGrid =\n}}\n</center>\n<br />\n<center>\n{{Graph:Chart\n | width = 900\n | height = 250\n | colors = Orange\n | showValues = offset:2\n | xAxisTitle = Date\n | xAxisAngle = -50\n | type = rect\n | x = 24/1, 25/1, 26/1, 27/1, 28/1, ..., 3/2, 4/2, 5/2, 6/2, 7/2, 8/2, 9/2, ...., 12/2, 13/2, 14/2, 15/2, 16/2, ....., 27/2, 28/2, 29/2, 1/3, 2/3, 3/3, 4/3, 5/3, 6/3, 7/3, 8/3, 9/3, 10/3, 11/3, 12/3, 13/3, 14/3, 15/3, 16/3, 17/3, 18/3, 19/3, 20/3, 21/3, 22/3, 23/3, 24/3, 25/3, 26/3, 27/3, 28/3, 29/3, 30/3, 31/3, 1/4, 2/4, 3/4, 4/4, 5/4, 6/4, 7/4, 8/4, 9/4, 10/4, 11/4, 12/4, 13/4, 14/4, 15/4, 16/4, 17/4\n | yAxisTitle = No. of new cases\n | legend = Legend\n | y1 = 3, 1, 0, 2, 2, 0, 1, 3, 2, 0, 1, 1, 2, 0, 1, 0, 2, 0, 1, 0, 2, 1, 4, 0, 4, 3, 14, 5, 28, 10, 6, 19, 12, 19, 9, 42, 42, 186, 125, 120, 117, 110, 130, 153, 123, 212, 106, 172, 235, 130, 159, 150, 156, 140, 142, 208, 217, 150, 179, 131, 170, 156, 109, 118, 184, 153, 134, 170, 85, 110, 69\n | y1Title = New cases per day\n | xGrid = \n | yGrid =\n}}\n