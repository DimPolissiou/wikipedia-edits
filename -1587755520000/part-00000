ef name = \"Matsumura_1999\">{{cite journal | vauthors = Matsumura I, Kitamura T, Wakao H, Tanaka H, Hashimoto K, Albanese C, Downward J, Pestell RG, Kanakura Y | display-authors = 6 | title = Transcriptional regulation of the cyclin D1 promoter by STAT5: its involvement in cytokine-dependent growth of hematopoietic cells | journal = The EMBO Journal | volume = 18 | issue = 5 | pages = 1367\u201377 | date = March 1999 | pmid = 10064602 | pmc = 1171226 | doi = 10.1093/emboj/18.5.1367 }}</ref>\n\n<ref name = \"Merrick_2011\">{{cite journal | vauthors = Merrick KA, Wohlbold L, Zhang C, Allen JJ, Horiuchi D, Huskey NE, Goga A, Shokat KM, Fisher RP | display-authors = 6 | title = Switching Cdk2 on or off with small molecules to reveal requirements in human cell proliferation | journal = Molecular Cell | volume = 42 | issue = 5 | pages = 624\u201336 | date = June 2011 | pmid = 21658603 | doi = 10.1016/j.molcel.2011.03.031 | doi-access = free }}</ref>\n\n<ref name = \"Morgan_2007\">{{cite book | vauthors = Morgan D | title = Cell Cycle: Principles of Control | year = 2007 | location = London | publisher = New Science Press | isbn = 978-0-87893-508-6 }}</ref>\n\n<ref name = \"Narasimha_2014\">{{cite journal | vauthors = Narasimha AM, Kaulich M, Shapiro GS, Choi YJ, Sicinski P, Dowdy SF | title = Cyclin D activates the Rb tumor suppressor by mono-phosphorylation | journal = eLife | volume = 3 | date = June 2014 | pmid = 24876129 | doi = 10.7554/eLife.02872 | doi-access = free }}</ref>\n\n<ref name = \"NCBI_protein_cyclin_D\">{{cite web | url = https://www.ncbi.nlm.nih.gov/protein?term=cyclin%20D | title = cyclin D - Protein | work = NCBI }}</ref>\n\n<ref name = \"Pestell_2013\">{{cite journal | vauthors = Pestell RG | title = New roles of cyclin D1 | journal = The American Journal of Pathology | volume = 183 | issue = 1 | pages = 3\u20139 | date = July 2013 | pmid = 23790801 | pmc = 3702737 | doi = 10.1016/j.ajpath.2013.03.001 }}</ref>\n\n<ref name = \"Resnitzky_1995\">{{cite journal | vauthors = Resnitzky D, Reed SI | title = Different roles for cyclins D1 and E in regulation of the G1-to-S transition | journal = Molecular and Cellular Biology | volume = 15 | issue = 7 | pages = 3463\u20139 | date = July 1995 | pmid = 7791752 | pmc = 230582 | doi = 10.1128/MCB.15.7.3463 }}</ref>\n\n<ref name = \"Rosenwald_1995\">{{cite journal | vauthors = Rosenwald IB, Kaspar R, Rousseau D, Gehrke L, Leboulch P, Chen JJ, Schmidt EV, Sonenberg N, London IM | display-authors = 6 | title = Eukaryotic translation initiation factor 4E regulates expression of cyclin D1 at transcriptional and post-transcriptional levels | journal = The Journal of Biological Chemistry | volume = 270 | issue = 36 | pages = 21176\u201380 | date = September 1995 | pmid = 7673150 | doi = 10.1074/jbc.270.36.21176 | doi-access = free }}</ref>\n\n<ref name = \"Sack_2018\">{{cite journal | vauthors = Sack LM, Davoli T, Li MZ, Li Y, Xu Q, Naxerova K, Wooten EC, Bernardi RJ, Martin TD, Chen T, Leng Y, Liang AC, Scorsone KA, Westbrook TF, Wong KK, Elledge SJ | display-authors = 6 | title = Profound Tissue Specificity in Proliferation Control Underlies Cancer Drivers and Aneuploidy Patterns | journal = Cell | volume = 173 | issue = 2 | pages = 499\u2013514.e23 | date = April 2018 | pmid = 29576454 | doi = 10.1016/j.cell.2018.02.037 | doi-access = free }}</ref>\n\n<ref name = \"Sanes\">{{cite book | vauthors = Sanes DH, Reh TA, Harris WA | year = 2005 | title = Development of the Nervous System | publisher = Elsevier Ltd | location = Oxford | edition = 2nd | pages =  | pmid =  | isbn = 978-0-12-618621-5 }}</ref>\n\n<ref name = \"Shtutman_1999\">{{cite journal | vauthors = Shtutman M, Zhurinsky J, Simcha I, Albanese C, D'Amico M, Pestell R, Ben-Ze'ev A | title = The cyclin D1 gene is a target of the beta-catenin/LEF-1 pathway | journal = Proceedings of the National Academy of Sciences of the United States of America | volume = 96 | issue = 10 | pages = 5522\u20137 | date = May 1999 | pmid