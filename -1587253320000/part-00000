ings<!--also Business District-->}}\n|notes=interchange; west end of freeway\n}}\n{{ARint|exit\n|type=concur\n|exit=2\n|mile=96.60\n|road={{jct|state=AR|US|70|dir1=west|US|70B|dab2=Hot Springs|dir2=east|name2=Airport Road|city1=Glenwood<!--signed where US 270 west hops on the bypass-->}}\n|notes=west end of US 70 overlap\n}}\n{{ARint|exit\n|exit=3\n|mile=98.46\n|road=McLeod Street\n}}\n{{ARint|exit\n|exit=4\n|mile=99.53\n|road={{jct|state=AR|AR|88|name1=Higdon Ferry Road}}\n}}\n{{ARint|exit\n|exit=5\n|mile=100.35\n|road={{jct|state=AR|AR|7|name1=Central Avenue}}\n|notes=signed as exits 5A (south) and 5B (north)\n}}\n{{ARint|exit\n|exit=7\n|mile=102.61\n|road={{jct|state=AR|AR|128|name1=Carpenter Dam Road}}\n}}\n{{ARint|exit\n|type=concur\n|mile=104.57\n|road={{jct|state=AR|US|70|dir1=east|US|270B|dab2=Hot Springs|dir2=west|name2=Malvern Avenue|city1=Little Rock}}\n|notes=interchange; east end of freeway; east end of US 70 overlap; US 270 east follows exit 9\n}}\n{{ARint|exit\n|espan=16\n|location=none\n|mile=108.17\n|road={{jct|state=AR|AR|171|dir1=north|name1=Lonsdale Road}}\n}}\n{{ARint\n|county=Hot Spring|cspan=5\n|location=Magnet Cove\n|mile=110.34\n|road={{jct|state=AR|AR|51|dir1=south|city1=Butterfield}}\n}}\n{{ARint\n|location=none\n|mile=116.73\n|road={{jct|state=AR|AR|51|dir1=north|city1=Butterfield}}\n}}\n{{ARint\n|type=concur\n|location=Rockport|lspan=2\n|mile=117.76\n|road={{jct|state=AR|I|30|dir1=west|US|270B|dab2=Malvern|dir2=east|city1=Malvern|city2=Texarkana|location3=[[Dallas, TX|Dallas]]<!--signed at the other end of the overlap-->}}\n|notes=west end of I-30 overlap; US 270 west follows exit 98B\n}}\n{{ARint\n|type=concur\n|road={{jct|state=AR|I|30|dir1=east|city1=Benton|city2=Little Rock}}\n|notes=east end of I-30 overlap; US 270 east follows exit 99\n}}\n{{ARint\n|location=Malvern\n|road={{jct|state=AR|US|67|US|270B|dab2=Malvern|dir2=west|city1=Glen Rose|city2=Perla|city3=Malvern}}\n}}\n{{ARint\n|county=Grant|cspan=8\n|location=Poyen\n|road={{jct