gn=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| 1972\n| \"Sundown\"\n|align=\"center\"| 54\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| 1973\n| \"Lonesome Mary\"\n|align=\"center\"| [[Chilliwack (1971 album)|Chilliwack]]''\n|align=\"center\"| 9\n|align=\"center\"| &mdash;\n|align=\"center\"| 75\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| rowspan=\"2\"|1974\n| \"Groundhog\"\n|align=\"center\"| ''[[All Over You (album)|All Over You]]''\n|align=\"center\"| 41\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| \"Crazy Talk\"\n|align=\"center\" rowspan=\"2\"| ''[[Riding High (Chilliwack album)|Riding High]]''\n|align=\"center\"| 10\n|align=\"center\"| 13\n|align=\"center\"| 98\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| 1975\n| \"There's Something I Like About That\"\n|align=\"center\"| 85\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| 1975\n| \"Last Day of December\"\n|align=\"center\"| ''[[Rockerbox]]''\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| 109\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| 1976\n| \"Come on Over\"\n|align=\"center\"| ''[[Riding High (Chilliwack album)|Riding High]]''\n|align=\"center\"| 54\n|align=\"center\"| 12\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| rowspan=\"2\"|1976\n| \"California Girl\"\n|align=\"center\" rowspan=\"4\"| ''[[Dreams, Dreams, Dreams]]''\n|align=\"center\"| 19\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| \"Fly at Night\"\n|align=\"center\"| 7\n|align=\"center\"| 16\n|align=\"center\"| 75\n|align=\"center\"| &mdash;\n|align=\"center\"| &mdash;\n|-\n| rowspan=\"3\"|1978\n| \"Something Better\"\n|align=