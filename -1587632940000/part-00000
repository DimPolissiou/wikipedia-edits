dix |title=Max Weber |publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend |url-access=registration |pages=[https://archive.org/details/maxweberintellec0000bend/page/200 200]\u201301 |isbn=978-0-520-03194-4 |year=1977}}</ref>\n\n<ref name=\"Bendix204\">{{cite book |last=Bendix |title=Max Weber |publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend |url-access=registration |pages=[https://archive.org/details/maxweberintellec0000bend/page/204 204]\u201305 |isbn=978-0-520-03194-4 |year=1977}}</ref>\n\n<ref name=\"Bendix285\">{{cite book |last=Bendix |title=Max Weber |publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend |url-access=registration |page=[https://archive.org/details/maxweberintellec0000bend/page/285 285] |isbn=978-0-520-03194-4 |year=1977 }}</ref>\n\n<ref name=\"Bendix296\">{{cite book |last=Bendix |title=Max Weber|publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend|url-access=registration |page=[https://archive.org/details/maxweberintellec0000bend/page/296 296] |isbn=978-0-520-03194-4 |year=1977}}</ref>\n\n<ref name=\"Bendix3\">{{cite book |last=Bendix |title=Max Weber |publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend |url-access=registration |page=[https://archive.org/details/maxweberintellec0000bend/page/3 3] |isbn=978-0-520-03194-4 |year=1977 }}</ref>\n\n<ref name=\"Bendix303\">{{cite book |last=Bendix |title=Max Weber|url=https://books.google.com/?id=C_j_2nOUIpcC&pg=PA303&lpg=PA303|pages=303\u201305 |isbn=978-0-415-17453-4 |date=1998}}</ref>\n\n<ref name=\"Bendix49\">{{cite book |last=Bendix |title=Max Weber |publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend |url-access=registration |page=[https://archive.org/details/maxweberintellec0000bend/page/n47 49] |isbn=978-0-520-03194-4 |year=1977}}</ref>\n\n<ref name=\"Bendix54\">{{cite book |last=Bendix |title=Max Weber |url=|page=54}}</ref>\n\n<ref name=\"Bendix57\">{{cite book |last=Bendix |title=Max Weber |publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend|url-access=registration |page=[https://archive.org/details/maxweberintellec0000bend/page/n114 57] |isbn=978-0-520-03194-4 |year=1977}}</ref>\n\n<ref name=\"Bendix60\">{{cite book |last=Bendix |title=Max Weber |publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend |url-access=registration |pages=[https://archive.org/details/maxweberintellec0000bend/page/n117 60]\u201361 |isbn=978-0-520-03194-4 |year=1977 }}</ref>\n\n<ref name=\"Bendix85\">{{cite book |last=Bendix |title=Max Weber |publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend |url-access=registration |pages=[https://archive.org/details/maxweberintellec0000bend/page/n142 85]\u201387 |isbn=978-0-520-03194-4 |year=1977}}</ref>\n\n<ref name=\"Bendix90\">{{cite book|author=Reinhard Bendix|title=Max Weber: an intellectual portrait|url=https://archive.org/details/maxweberintellec0000bend|url-access=registration|accessdate=5 April 2011|year=1977|publisher=University of California Press|isbn=978-0-520-03194-4|page=[https://archive.org/details/maxweberintellec0000bend/page/n147 90]}}</ref>\n\n<ref name=\"Bendix98\">{{cite book |last=Bendix |title=Max Weber |publisher=University of California Press |url=https://archive.org/details/maxweberintellec0000bend |url-access=registration |pages=[https://archive.org/details/maxweberintellec0000bend/page/n155 98]\u201399 |isbn=978-0-520-03194-4 |year=1977}}</ref>\n\n<ref name=\"BendixChapter9\">{{cite book |last=Bendix |title=Max Weber |chapter-url=https://books.google.com/?id=63sC9uaYqQsC&pg=PA285&lpg=PA285 |chapter=IX: Basic Concepts of Political Sociology |nopp=true |isbn=978-0-520-03194-4 |year=1977 }}</ref>\n\n