u\n |- \n | 712 || {{lang|ja|[[wikt:\u640d|\u640d]]}} || 13 || loss || son || soko-neru\n |- \n | 713 || {{lang|ja|[[wikt:\u652f|\u652f]]}} || 4 || support || shi || sasa-eru\n |- \n | 714 || {{lang|ja|[[wikt:\u653f|\u653f]]}} || 9 || politics || sei || matsurigoto\n |- \n | 715 || {{lang|ja|[[wikt:\u6545|\u6545]]}} || 9 || circumstances || ko || yue\n |- \n | 716 || {{lang|ja|[[wikt:\u6575|\u6575]]}} || 15 || enemy || teki || kataki\n |- \n | 717 || {{lang|ja|[[wikt:\u65ad|\u65ad]]}} || 11 || cut off || dan || ta-tsu, kotowa-ru\n |- \n | 718 || {{lang|ja|[[wikt:\u65e7|\u65e7]]}} || 5 || old times || ky\u016b ||\n |- \n | 719 || {{lang|ja|[[wikt:\u6613|\u6613]]}} || 8 || easy || eki || yasa-shii\n |- \n | 720 || {{lang|ja|[[wikt:\u66b4|\u66b4]]}} || 15 || outburst || b\u014d || aba-ku\n |- \n | 721 || {{lang|ja|[[wikt:\u6761|\u6761]]}} || 7 || clause || j\u014d || kudari\n |- \n | 722 || {{lang|ja|[[wikt:\u679d|\u679d]]}} || 8 || branch || shi || eda\n |- \n | 723 || {{lang|ja|[[wikt:\u67fb|\u67fb]]}} || 9 || investigate || sa ||\n |- \n | 724 || {{lang|ja|[[wikt:\u683c|\u683c]]}} || 10 || status || kaku ||\n |- \n | 725 || {{lang|ja|[[wikt:\u685c|\u685c]]}} || 10 || cherry || \u014d || sakura\n |- \n | 726 || {{lang|ja|[[wikt:\u691c|\u691c]]}} || 12 || examine || ken ||\n |- \n | 727 || {{lang|ja|[[wikt:\u69cb|\u69cb]]}} || 14 || construct || k\u014d || kama-eru\n |- \n | 728 || {{lang|ja|[[wikt:\u6b66|\u6b66]]}} || 8 || military || bu, mu ||\n |- \n | 729 || {{lang|ja|[[wikt:\u6bd4|\u6bd4]]}} || 4 || compare || hi || kura-beru\n |- \n | 730 || {{lang|ja|[[wikt:\u6c38|\u6c38]]}} || 5 || eternity || ei || naga-i\n |- \n | 731 || {{lang|ja|[[wikt:\u6cb3|\u6cb3]]}} || 8 || stream || ka || kawa\n |- \n | 732 || {{lang|ja|[[wikt:\u6db2|\u6db2]]}} || 11 || fluid || eki ||\n |- \n | 733 || {{lang|ja|[[wikt:\u6df7|\u6df7]]}} || 11 || mix || kon || ma-zaru\n |- \n | 734 || {{lang|ja|[[wikt:\u6e1b|\u6e1b]]}} || 12 || decrease || gen || he-ru\n |- \n | 735 || {{lang|ja|[[wikt