 || rui ||\n |- \n | 637 || {{lang|ja|[[wikt:\u98db|\u98db]]}} || 9 || fly || hi || to-bu\n |- \n | 638 || {{lang|ja|[[wikt:\u98ef|\u98ef]]}} || 12 || meal || han || meshi\n |- \n | 639 || {{lang|ja|[[wikt:\u990a|\u990a]]}} || 15 || foster || y\u014d || yashina-u\n |- \n | 640 || {{lang|ja|[[wikt:\u9a13|\u9a13]]}} || 18 || verify || ken ||\n|}\n\n===Fifth grade (185 kanji)===\n\n {| class=\"wikitable sortable mw-collapsible\"\n ! # !! '''Kanji''' !! Strokes !! '''Meaning''' !! '''[[Kanji#On.27yomi .28Sino-Japanese reading.29|On]]''' !! '''[[Kanji#Kun.27yomi .28native reading.29|Kun]]'''\n |- \n | 641 || {{lang|ja|[[wikt:\u4e45|\u4e45]]}} || 3 || long time || ky\u016b || hisa\n |- \n | 642 || {{lang|ja|[[wikt:\u4ecf|\u4ecf]]}} || 4 || Buddha || futsu, butsu || hotoke\n |- \n | 643 || {{lang|ja|[[wikt:\u4eee|\u4eee]]}} || 6 || sham || ka, ke || kari\n |- \n | 644 || {{lang|ja|[[wikt:\u4ef6|\u4ef6]]}} || 6 || affair || ken ||\n |- \n | 645 || {{lang|ja|[[wikt:\u4efb|\u4efb]]}} || 6 || responsibility || nin || maka-seru\n |- \n | 646 || {{lang|ja|[[wikt:\u4f3c|\u4f3c]]}} || 7 || becoming || ji || ni-ru\n |- \n | 647 || {{lang|ja|[[wikt:\u4f59|\u4f59]]}} || 7 || surplus || yo || ama-ru\n |- \n | 648 || {{lang|ja|[[wikt:\u4fa1|\u4fa1]]}} || 8 || value || ka || atai\n |- \n | 649 || {{lang|ja|[[wikt:\u4fdd|\u4fdd]]}} || 9 || preserve || ho || tamo-tsu\n |- \n | 650 || {{lang|ja|[[wikt:\u4fee|\u4fee]]}} || 10 || discipline || sh\u016b || osa-meru\n |- \n | 651 || {{lang|ja|[[wikt:\u4ff5|\u4ff5]]}} || 10 || straw bag || hy\u014d || tawara\n |- \n | 652 || {{lang|ja|[[wikt:\u500b|\u500b]]}} || 10 || individual || ko ||\n |- \n | 653 || {{lang|ja|[[wikt:\u5099|\u5099]]}} || 12 || provide || bi || sona-eru\n |- \n | 654 || {{lang|ja|[[wikt:\u50cf|\u50cf]]}} || 14 || statue || z\u014d ||\n |- \n | 655 || {{lang|ja|[[wikt:\u518d|\u518d]]}} || 6 || again || sai, sa || futata-bi\n |- \n | 656 || {{lang|ja|[[wikt:\u520a|\u520a]]}} || 5 || publish || kan ||\n |- \n | 657 || {{lang|ja