26|\u5426]]}} || 7 || negate || hi || ina, iya\n |- \n | 850 || {{lang|ja|[[wikt:\u5438|\u5438]]}} || 6 || suck || ky\u016b || su-u\n |- \n | 851 || {{lang|ja|[[wikt:\u547c|\u547c]]}} || 8 || call || ko || yo-bu\n |- \n | 852 || {{lang|ja|[[wikt:\u5584|\u5584]]}} || 12 || virtue || zen || yo-i\n |- \n | 853 || {{lang|ja|[[wikt:\u56f0|\u56f0]]}} || 7 || quandary || kon || koma-ru\n |- \n | 854 || {{lang|ja|[[wikt:\u5782|\u5782]]}} || 8 || droop || sui || ta-reru\n |- \n | 855 || {{lang|ja|[[wikt:\u57ce|\u57ce]]}} || 9 || castle || j\u014d || shiro\n |- \n | 856 || {{lang|ja|[[wikt:\u57df|\u57df]]}} || 11 || range || iki ||\n |- \n | 857 || {{lang|ja|[[wikt:\u594f|\u594f]]}} || 9 || play music || s\u014d || kana-deru\n |- \n | 858 || {{lang|ja|[[wikt:\u596e|\u596e]]}} || 16 || stirred up || fun || furu-u\n |- \n | 859 || {{lang|ja|[[wikt:\u59ff|\u59ff]]}} || 9 || shape || shi || sugata\n |- \n | 860 || {{lang|ja|[[wikt:\u5b58|\u5b58]]}} || 6 || suppose || son ||\n |- \n | 861 || {{lang|ja|[[wikt:\u5b5d|\u5b5d]]}} || 7 || filial piety || k\u014d ||\n |- \n | 862 || {{lang|ja|[[wikt:\u5b85|\u5b85]]}} || 6 || home || taku || ie <!-- not a typo -->\n |- \n |- \n | 863 || {{lang|ja|[[wikt:\u5b87|\u5b87]]}} || 6 || eaves || u ||\n |- \n | 864 || {{lang|ja|[[wikt:\u5b97|\u5b97]]}} || 8 || religion || sh\u016b || s\u014d\n |- \n | 865 || {{lang|ja|[[wikt:\u5b99|\u5b99]]}} || 8 || mid-air || ch\u016b ||\n |- \n | 866 || {{lang|ja|[[wikt:\u5b9d|\u5b9d]]}} || 8 || treasure || h\u014d || takara\n |- \n | 867 || {{lang|ja|[[wikt:\u5ba3|\u5ba3]]}} || 9 || proclaim || sen || notama-u\n |- \n | 868 || {{lang|ja|[[wikt:\u5bc6|\u5bc6]]}} || 11 || secrecy || mitsu ||\n |- \n | 869 || {{lang|ja|[[wikt:\u5bf8|\u5bf8]]}} || 3 || measurement || sun ||\n |- \n | 870 || {{lang|ja|[[wikt:\u5c02|\u5c02]]}} || 9 || specialty || sen || moppa-ra\n |- \n | 871 || {{lang|ja|[[wikt:\u5c04|\u5c04]]}} || 10 || shoot || sha || i-ru\n |- \n | 872 || {{lang|ja|[[wikt:\u5c06|\u5c06]]}} || 10 || leader || s