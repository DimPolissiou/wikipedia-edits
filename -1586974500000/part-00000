ry|last=Marchese|pos=WR}}\n{{American football roster/Player|num=5|class=SO|first=Oliver|last=Martin|pos=WR}}\n{{American football roster/Player|num=89|class=FR|first=Nico|last=Ragaini|pos=WR|rs=y}}\n{{American football roster/Player|num=29|class=FR|first=Jackson|last=Ritter|pos=WR}}\n{{American football roster/Player|num=12|class=JR|first=Brandon|last=Smith|pos=WR}}\n{{American football roster/Player|num=6|class=JR|first=Ihmir|last=Smith-Marsette|pos=WR}}\n{{American football roster/Player|num=3|class=FR|first=Tyrone|last=Tracy|pos=WR|rs=y}}\n\n{{American football roster/Player|num=42|class=JR|first=Shaun|last=Beyer|pos=TE}}\n{{American football roster/Player|num=18|class=SR|first=Drew|last=Cook|pos=TE}}\n{{American football roster/Player|num=86|class=SO|first=Noah|last=Feldman|pos=TE}}\n{{American football roster/Player|num=88|class=FR|first=Jackson|last=Frericks|pos=TE}}\n{{American football roster/Player|num=46|class=SO|first=Tommy|last=Kujawa|pos=TE}}\n{{American football roster/Player|num=84|class=FR|first=Sam|last=Laporta|pos=TE}}\n{{American football roster/Player|num=48|class=SO|first=Bryce|last=Schulte|pos=TE}}\n{{American football roster/Player|num=39|class=SR|first=Nate|last=Wieting|pos=TE}}\n\n{{American football roster/Player|num=61|class=JR|first=Cole|last=Banwart|pos=OL}}\n{{American football roster/Player|num=63|class=FR|first=Justin|last=Britt|pos=OL}}\n{{American football roster/Player|num=56|class=FR|first=Nick|last=DeJong|pos=OL}}\n{{American football roster/Player|num=55|class=FR|first=Luke|last=Empen|pos=OL|rs=y}}\n{{American football roster/Player|num=69|class=FR|first=Tyler|last=Endres|pos=OL}}\n{{American football roster/Player|num=54|class=FR|first=Matt|last=Fagan|pos=OL|rs=y}}\n{{American football roster/Player|num=60|class=FR|first=Noah|last=Fenske|pos=OL}}\n{{American football roster/Player|num=58|class=FR|first=Taylor|last=Fox|pos=OL}}\n{{American football roster/Player|num=73|class=FR|first=Cody|last=Ince|pos=OL|rs=y}}\n{{American foo