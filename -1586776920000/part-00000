ow\n| position = RB\n| hometown = Memphis, Tennessee\n| highschool = Memphis East High School\n| feet   = 5\n| inches = 8\n| weight = 170\n| 40 = \n| commitdate = July 1, 2011\n| scout stars  = 4\n| rivals stars = 4\n| espn grade = 80\n}}\n{{College Athlete Recruit Entry\n| recruit  = Torey Agee\n| position = DL\n| hometown = Opelika, Alabama\n| highschool = Opelika High School \n| feet   = 6\n| inches = 3\n| weight = 250\n| 40 = \n| commitdate = September 14, 2011\n| scout stars  = 3\n| rivals stars = 3\n| espn grade = 77\n}}\n{{College Athlete Recruit Entry\n| recruit  = Darreon Herring\n| position = LB\n| hometown = Stone Mountain, Georgia\n| highschool = Stephenson High School\n| feet   = 6\n| inches = 1\n| weight = 220\n| 40 = \n| commitdate = May 19, 2011\n| scout stars  = 3\n| rivals stars = 3\n| espn grade = 79\n}}\n{{College Athlete Recruit Entry\n| recruit  = Barrett Gouger\n| position = OL\n| hometown = Soddy-Daisy, Tennessee\n| highschool = Baylor School\n| feet   = 6\n| inches = 4\n| weight = 280\n| 40 = \n| commitdate = May 15, 2011\n| scout stars  = 3\n| rivals stars = 3\n| espn grade = 78\n}}\n{{College Athlete Recruit Entry\n| recruit  = Blake Fromang\n| position = OL\n| hometown = Orlando, Florida\n| highschool = Lake Highland Preparatory\n| feet   = 6\n| inches = 7\n| weight = 285\n| 40 = \n| commitdate = September 3, 2011\n| scout stars  = 3\n| rivals stars = 3\n| espn grade = 78\n}}\n{{College Athlete Recruit Entry\n| recruit  = Patton Robinette\n| position = QB\n| hometown = Maryville, Tennessee\n| highschool = Maryville, HS\n| feet   = 6\n| inches = 4\n| weight = 200\n| 40 = \n| commitdate = January 8, 2012\n| scout stars  = 3\n| rivals stars = 3\n| espn grade = 79\n}}\n{{College Athlete Recruit Entry\n| recruit  = Will Holden\n| position = OL\n| hometown = Green Cove Springs, Florida\n| highschool = Clay High School \n| feet   = 6\n| inches = 6\n| weight = 280\n| 40 = \n| commitdate = October 31, 2011\n| scout stars  = 2\n| rivals stars = 3\n