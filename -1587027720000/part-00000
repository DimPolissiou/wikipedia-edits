e book |last1=Strid |first1=Jan Paul |author-link1= |translator-last1= |translator-first1= |translator-link1= |year=2011 |chapter=Retracing the Goths |trans-chapter= |chapter-url= |editor1-last=Kaliff |editor1-first=Anders |editor1-link=:sv:Anders Kaliff |editor2-last=Munkhammar |editor2-first=Lars |editor2-link= |title=Wulfila 311-2011 |trans-title= |url=http://www.diva-portal.org/smash/get/diva2:668706/FULLTEXT01.pdf |url-status= |series= |language= |volume= |edition= |location= |publisher=[[:sv:Acta Universitatis Upsaliensis|Acta Universitatis Upsaliensis]] |pages=41\u201354 |doi= |isbn=9789155486648 |archive-url=https://web.archive.org/web/20200305084737/http://www.diva-portal.org/smash/get/diva2:668706/FULLTEXT01.pdf |archive-date=March 5, 2020 |access-date= |via= |quote= |ref=harv}}\n* {{cite journal |last1=S\u00f8nnesyn |first1=Sigbj\u00f8rn |author-link1=|year=2004  |title=Arne S\u00f8by Christensen, Cassiodorus, Jordanes and the History of the Goths  |journal=[[Scandinavian Journal of History]]  |publisher=[[Taylor & Francis]] |publication-date= |volume=29 |issue=3\u20134 |pages=306\u2013308  |doi=10.1080/03468750410005719 |ref=harv}}\n* {{cite web |url=https://www.britannica.com/biography/Theodoric-king-of-Italy |url-access= |title=Theodoric |last1=Thompson |first1=E. A. |author-link1=Edward Arthur Thompson |date= |year= |orig-year= |editor-last1= |editor-first1= |editor1= |editor-link1= |editor2-last= |editor2-first= |editor2-link= |editors= |department= |website=[[Encyclop\u00e6dia Britannica Online]] |series= |publisher= |agency= |location= |page= |pages= |at= |language= |trans-title= |type= |format= |bibcode= |doi= |isbn= |jstor= |archive-url= |archive-date= |url-status= |access-date=September 19, 2019 |via= |quote= |ref=harv }}\n* {{cite book |last1=Thompson |first1=Edward Arthur |author-link1=Edward Arthur Thompson |year=1973 |chapter=Goths |editor-last= |editor-first= |editor-link= |title=Encyclopaedia Britannica |url=https://books.google.com/?id=IOdMAQAAIAAJ |series= |language= |volume=10 |edition= |location= |publisher=[[Encyclop\u00e6dia Britannica, Inc.]] |pages=606\u2013609 |isbn=0852291736 |archive-url= |archive-date= |access-date= |via= |quote= |ref=harv}}\n* {{citation|first=Dieter |last=Timpe |year=1989 |editor-last1=Beck |editor-first1=Heinrich |title=Reallexikon der Germanischen Altertumskunde |volume= 7 |pages=307\u2013391 |url=https://books.google.com/?id=3QmORVr1IfMC|chapter=Entdeckungsgeschichte|isbn=9783110114454 }}\n* {{cite book |last=Tucker |first=Spencer |author-link=Spencer C. Tucker |year=2009 |chapter= |editor-last= |editor-first= |editor-link= |title=A Global Chronology of Conflict |url=https://books.google.com/?id=h5_tSnygvbIC |series= |language= |volume= |edition= |location= |publisher=[[ABC-CLIO]] |page= |pages= |isbn=978-1851096725 |archive-url= |archive-date= |access-date= |via= |quote= |ref=harv}}\n* {{cite book |last=Vasiliev |first=Alexander A. |author-link=Alexander Vasiliev (historian) |year=1936 |chapter= |editor-last= |editor-first= |editor-link= |title=The Goths in Crimea |url=https://archive.org/details/Vasiliev1936Goths/page/n3 |series= |language= |volume= |edition= |location= |publisher=[[Medieval Academy of America]] |page= |pages= |isbn= |archive-url= |archive-date= |access-date= |via= |quote= |ref=harv}}\n* {{cite book |last1=Waldman |first1=Carl |author-link1= |last2=Mason |first2=Catherine |author-link2= |year=2006 |chapter= |editor-last= |editor-first= |editor-link= |title=Encyclopedia of European Peoples |url=https://books.google.com/?id=kfv6HKXErqAC |series= |language= |volume= |edition= |location= |publisher=[[Infobase Publishing]] |page= |pages= |isbn=1438129181 |archive-url= |archive-date= |access-date= |via= |quote= |ref=harv}}\n* {{cite web |url=https://www.britannica.com/place/Italy/History |url-access= |title=Italy: History |last1=Wickham |first1=Christopher John |author-link1=Christopher Wickham |last2=Foot |first2=John |author-link2=John Foot (historian)