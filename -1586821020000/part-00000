&Itemid=203 MakFudbal.com]\n\n<!--Update team positions here-->\n|team1=RUF |team2=LJU |team3=GRA |team4=REC |team5=RNE |team6=LIR |team7=RCA |team8=VAR |team9=ARS |team10=FLA |team11=GOC |team12=ZAJ |team13=SAR \n<!--Update team qualifications here (defined below)-->\n|result1=CFL |result7=WIT  |result13=WIT  \n\n<!--Update team results here and then (if needed positions above). Dont forget to update the date (update parameter)-->\n|win_ARS=8 |draw_ARS=2 |loss_ARS=12|gf_ARS=30|ga_ARS=41<!-- Arsimi -->\n|win_VAR=7 |draw_VAR=6 |loss_VAR=9 |gf_VAR=44|ga_VAR=40<!-- Vardari -->\n|win_GOC=6 |draw_GOC=2 |loss_GOC=13|gf_GOC=34|ga_GOC=51<!-- Goce Delchev -->\n|win_GRA=13|draw_GRA=4 |loss_GRA=6 |gf_GRA=59|ga_GRA=36<!-- Gradec -->\n|win_ZAJ=4 |draw_ZAJ=1 |loss_ZAJ=16|gf_ZAJ=24|ga_ZAJ=64<!-- Zajazi -->\n|win_LIR=9 |draw_LIR=2 |loss_LIR=12|gf_LIR=37|ga_LIR=41<!-- Lirija -->\n|win_LJU=14|draw_LJU=5 |loss_LJU=4 |gf_LJU=53|ga_LJU=17<!-- Ljuboten -->\n|win_REC=10|draw_REC=2 |loss_REC=10|gf_REC=37|ga_REC=40<!-- Rechica -->\n|win_RNE=9 |draw_RNE=4 |loss_RNE=8 |gf_RNE=33|ga_RNE=38<!-- Rinia (N) -->\n|win_RCA=9 |draw_RCA=2 |loss_RCA=12|gf_RCA=32|ga_RCA=48<!-- Rinia (Ch) -->\n|win_RUF=20|draw_RUF=2 |loss_RUF=1 |gf_RUF=73|ga_RUF=19<!-- Rufeja -->\n|win_FLA=8 |draw_FLA=1 |loss_FLA=13|gf_FLA=32|ga_FLA=48<!-- Flamurtari -->\n|win_SAR=5 |draw_SAR=1 |loss_SAR=6 |gf_SAR=17|ga_SAR=22<!-- Sharri -->\n|\n<!--Point adjustment-->\n\n<!--Team notes-->\n|status_RUF=C,P\n|status_RCA=R\n|status_SAR=R\n\n<!--Team definitions (wikilinks in table)-->\n|name_ARS = [[KF Arsimi|Arsimi]]\n|name_VAR = [[KF Vardari Forin\u00eb|Vardari Forino]]\n|name_GOC = [[FK Goce Del\u010dev Drugovo|Goce Delchev Drugovo]]\n|name_GRA = [[KF Gradec|Gradec]]\n|name_ZAJ = [[KF Zajazi|Zajazi]]\n|name_LIR = [[KF Liria Zhelin\u00eb|Liria Zhelino]]\n|name_LJU = [[FK Ljuboten|Ljuboten]]\n|name_REC = [[KF Re\u00e7ica|Rechica]]\n|name_RNE = [[KF Rinia Negotin\u00eb|Rinia Negotino]]\n|name_RCA = [[KF Rinia \u00c7ajl\u00eb|Rinia Chajle]]