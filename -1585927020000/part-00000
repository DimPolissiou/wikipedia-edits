 rules |publisher=KNVB |accessdate=3 July 2019}}</ref>\n\n<!--Qualification and relegation column definitions-->\n|res_col_header=PQR\n|col_3D=green1 |text_3D=Promotion to [[2020-21 Derde Divisie|Derde Divisie]]\n|col_PO=blue1  |text_PO={{nowrap|Qualification to promotion play-offs}}\n|col_RPO=red2  |text_RPO=Qualification to relegation play-offs\n|col_1K=red1   |text_1K=Relegation to Eerste Klasse\n}}</onlyinclude>\n\n=== Fixtures/results ===\n<onlyinclude>{{#invoke:Sports results|main|matches_style=FBR\n|section=Hoofdklasse Saturday A results\n|source=[https://www.soccerway.com/national/netherlands/hoofdklasse/20192020/zaterdag-a/r53671/matches/ Soccerway]\n|solid_cell=grey\n|team_nowrap=y\n|update=7 March 2020\n\n<!--Team list order-->\n| team1 =A29\n| team2 =ACH\n| team3 =ARC\n| team4 =AZS\n| team5 =CAP\n| team6 =DHS\n| team7 =DUN\n| team8 =FEY\n| team9 =GRA\n| team10=JOD\n| team11=RVO\n| team12=RSO\n| team13=SMI\n| team14=SPI\n| team15=SPO\n| team16=ZWA\n\n<!--Team definitions (wikilinks in table)-->\n| name_A29 = [[Achilles '29]]\n| name_ACH = [[Achilles Veen]]\n| name_ARC = [[SV ARC|ARC]]\n| name_AZS = [[AZSV]]\n| name_CAP = [[vv Capelle|Capelle]]\n| name_DHS = [[DHSC]]\n| name_DUN = [[VV DUNO|DUNO]]\n| name_FEY = [[SC Feyenoord]]\n| name_GRA = [[FC 's-Gravenzande|'s-Gravenzande]]\n| name_JOD = [[CVV de Jodan Boys|Jodan Boys]]\n| name_RVO = [[FC Rijnvogels|Rijnvogels]]\n| name_RSO = [[VV Rijsoord|Rijsoord]]\n| name_SMI = [[VV Smitshoek|Smitshoek]]\n| name_SPI = [[VV Spijkenisse|Spijkenisse]]\n| name_SPO = [[Sportlust '46]]\n| name_ZWA = [[VV Zwaluwen|Zwaluwen]]\n\n<!--Match results-->\n| match_A29_ACH = 0\u20135\n| match_A29_ARC = {{small|23 May}}\n| match_A29_AZS =\n| match_A29_CAP = 1\u20134\n| match_A29_DHS = 2\u20133\n| match_A29_DUN = {{small|9 May}}\n| match_A29_FEY = 1\u20136\n| match_A29_GRA = {{small|18 Apr}}\n| match_A29_JOD = 1\u20132\n| match_A29_RVO = 1\u20134\n| match_A29_RSO = 0\u20132\n| match_A29_SMI = 0\u20134\n| match_A29_SPI = 0\u20130\n| 