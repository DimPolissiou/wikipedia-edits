y 1955/56 ||align=center| 1 || \n|-\n|align=center| 97 || Alderman George Rastall || 28 May 1956/57 ||align=center| 1 || \n|-\n|align=center| 98 || Alderman Mrs. Ruby Florence Ilsley|| 20 May 1957&ndash;1958 ||align=center| 1 || \n|-\n|align=center| 99 || John Clement Horner || 1958/59 ||align=center| 1 || \n|-\n|align=center|100 || Norman Frank Bagley || 1959/60 ||align=center| 1 || \n|-\n|align=center| 101 || Harold Marsh || 1960/61 ||align=center| 1 || \n|-\n|align=center| 102 || Charles Herbert Davies || 1961/62 ||align=center| 1 ||  \n|-\n|align=center| 103 || Malcolm Pascal Birch || 1962/63 ||align=center| 1 || \n|-\n|align=center| 104 || Herbert Preece || 1963/64 ||align=center| 1 || \n|-\n|align=center| 105 || Harry Bagley || 1964/65 ||align=center| 1 || \n|-\n|align=center| 106 || Frank Clapham || 1965/66 ||align=center| 1 || \n|-\n|align=center| 107 || Frank Stanley Smith || 1966/67 ||align=center| 1 || \n|-\n|align=center| 108 || Alderman Edward (Ted) Yardley Fullwood || 1967/68 ||align=center| 1 || \n|-\n|align=center| 109 || Robert Campbell || 1968/69 ||align=center| 1 || \n|-\n|align=center| 110 || Stanley Steventon Tatem || 1969/70 ||align=center| 1 || \n|-\n|align=center| 111 || Gerald Anthony Guy || 1970/71 ||align=center| 1 || \n|-\n|align=center| 112 || Francis Victor Law || 1971/72 ||align=center| 1 || \n|-\n|align=center| 113 || Arthur Storer || 1972/73 ||align=center| 1 || \n|-\n|align=center| 114 || Dennis Arthur Birch || 1973/74 ||align=center| 1 || \n|-\n|align=center| 115 || Samuel Herbert Reynolds || 1974/75 ||align=center| 1 || \n|-\n|align=center| 116 || Edward Mitchell || 1975/76 ||align=center| 1 || \n|-\n|align=center| 117 || Arthur Ernest Steventon || 1976/77 ||align=center| 1 || \n|-\n|align=center| 118 || Jessie Emma Beddoes || 1977/78 ||align=center| 1 || \n|-\n|align=center| 119 || Violet Amy Fletcher || 1978/79 ||align=center| 1 || \n|-\n|align=center| 120 || Ian George Claymore || 1979/80 ||align=center| 1 || \n|-\n|align=cente