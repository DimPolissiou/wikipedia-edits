u]] || || || 1 || 1 || 1\n|-\n|[[Betong District, Sarawak|Betong]] || 16 || 17 || 16 || 16 || 16\n|-\n|[[Bintulu District|Bintulu]] || 9 || 9 || 9 || 9 || 11\n|-\n|[[Kuching District|Kuching]] || 255 || 258 || 257 || 260 || 264\n|-\n|[[Lawas District|Lawas]] || 1 || 1 || 1 || 1 || 1\n|-\n|[[Limbang District|Limbang]] || 8 || 8 || 8 || 8 || 8\n|-\n|[[Matu District|Matu]] || 1 || 1 || 1 || 1 || 1\n|-\n|[[Miri District|Miri]] || 17 || 19 || 19 || 19 || 19\n|-\n|[[Mukah District|Mukah]] || 1 || 1 || 1 || 1 || 1\n|-\n|[[Samarahan District|Samarahan]] || 55 || 59 || 61 || 62 || 62\n|-\n|[[Sarikei District|Sarikei]] || 6 || 6 || 6 || 6 || 6\n|-\n|[[Serian District|Serian]] || 9 || 9 || 10 || 10 || 10\n|-\n|[[Sibu District|Sibu]] || 5 || 5 || 5 || 5 || 5\n|-\n|[[Simunjan District|Simunjan]] || 3 || 3 || 5 || 5 || 5\n|-\n|[[Sri Aman District|Sri Aman]] || 1 || 1 || 1 || 2 || 2\n|}\n\n=== Charts ===\n<center>\n{{Graph:Chart\n | width = 900\n | height = 400\n | type = line\n | linewidth=1\n | showSymbols=1\n | xAxisAngle = -65\n | xGrid =\n | yGrid =\n | legend = Legend\n | colors = Orange, OrangeRed, SkyBlue, Black\n | y1Title = Total confirmed cases\n | y2Title = Active cases\n | y3Title = Recoveries\n | y4Title = Deaths\n | x = 24/1, 25/1, 26/1, 27/1, 28/1, 29/1, 30/1, 31/1, 1/2, 2/2, 3/2, 4/2, 5/2, 6/2, 7/2, 8/2, 9/2, 10/2, 11/2, 12/2, 13/2, 14/2, 15/2, 16/2, 17/2, 18/2, 19/2, 20/2, 21/2, 22/2, 23/2, 24/2, 25/2, 26/2, 27/2, 28/2, 29/2, 1/3, 2/3, 3/3, 4/3, 5/3, 6/3, 7/3, 8/3, 9/3, 10/3, 11/3, 12/3, 13/3, 14/3, 15/3, 16/3, 17/3, 18/3, 19/3, 20/3, 21/3, 22/3, 23/3, 24/3, 25/3, 26/3, 27/3, 28/3, 29/3, 30/3, 31/3, 1/4, 2/4, 3/4, 4/4, 5/4, 6/4, 7/4, 8/4, 9/4, 10/4, 11/4, 12/4, 13/4, 14/4, 15/4, 16/4, 17/4, 18/4, 19/4, 20/4, 21/4\n | y1 = 3, 4, 4, 6, 8, 8, 8, 8, 8, 8, 9, 12, 14, 14, 15, 16, 18, 18, 18, 19, 19, 21, 21, 22, 22, 22, 22, 22, 22, 22, 22, 22, 22, 22, 24, 25, 29, 29, 33, 36, 50, 55, 83, 93, 99, 117, 129, 149, 158, 197, 238, 428, 553, 673, 790, 900, 1030, 1183, 1306, 151