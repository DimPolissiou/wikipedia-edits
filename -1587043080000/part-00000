}} || 144 || 507 || 164 || .323 || 10 || 70\n|- align=\"center\"\n| 2B || {{sortname|Mickey|Morandini}} || 127 || 422 || 112 || .265 || 3 || 30\n|- align=\"center\"\n| 3B || {{sortname|Dave|Hollins}} || 156 || 586 || 158 || .270 || 27 || 93\n|- align=\"center\"\n| SS || {{sortname|Juan|Bell}} || 46 || 147 || 30 || .204 || 1 || 8\n|- align=\"center\"\n| LF || {{sortname|Mariano|Duncan}} || 142 || 574 || 153 || .267 || 8 || 50\n|- align=\"center\"\n| CF || {{sortname|Lenny|Dykstra}} || 85 || 345 || 104 || .301 || 6 || 39\n|- align=\"center\"\n| RF || {{sortname|Rub\u00e9n|Amaro|Rub\u00e9n Amaro, Jr.}} || 126 || 374 || 82 || .219 || 7 || 34\n|}\n\n====Other batters====\n''Note: G = Games played; AB = At bats; H = Hits; Avg. = Batting average; HR = Home runs; RBI = Runs batted in''\n{| border=\"1\" cellpadding=\"2\" cellspacing=\"0\" class=\"wikitable sortable\"\n|-\n! bgcolor=\"#DDDDFF\" width=\"16%\" | Player\n! bgcolor=\"#DDDDFF\" width=\"9%\" | G\n! bgcolor=\"#DDDDFF\" width=\"9%\" | AB\n! bgcolor=\"#DDDDFF\" width=\"9%\" | H\n! bgcolor=\"#DDDDFF\" width=\"9%\" | Avg.\n! bgcolor=\"#DDDDFF\" width=\"9%\" | HR\n! bgcolor=\"#DDDDFF\" width=\"9%\" | RBI\n|- align=\"center\"\n| {{sortname|Dale|Sveum}} || 54 || 135 || 24 || .178 || 2 || 16\n|- align=\"center\"\n| {{sortname|Steve|Lake}} || 20 || 53 || 13 || .245 || 1 || 2\n|- align=\"center\"\n| {{sortname|Jim|Lindeman}} || 29 || 39 || 10 || .256 || 1 || 6\n|- align=\"center\"\n|}\n\n===Pitching===\n\n====Starting pitchers====\n''Note: G = Games pitched; IP = Innings pitched; W = Wins; L = Losses; ERA = Earned run average; SO = Strikeouts''\n{| border=\"1\" cellpadding=\"2\" cellspacing=\"0\" class=\"wikitable sortable\"\n|-\n! bgcolor=\"#DDDDFF\" width=\"16%\" | Player\n! bgcolor=\"#DDDDFF\" width=\"9%\" | G\n! bgcolor=\"#DDDDFF\" width=\"9%\" | IP\n! bgcolor=\"#DDDDFF\" width=\"9%\" | W\n! bgcolor=\"#DDDDFF\" width=\"9%\" | L\n! bgcolor=\"#DDDDFF\" width=\"9%\" | ERA\n! bgcolor=\"#DDDDFF\" width=\"9%\" | SO\n|- align=\"