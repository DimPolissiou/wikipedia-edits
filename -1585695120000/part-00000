h\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| [[Chris Coy]]\n| align=\"left\"| Martin\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| Ricky Wayne\n| align=\"left\"| O'Donnell\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| [[Christine Woods]]\n| align=\"left\"| Dawn Lerner\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| [[Erik Jensen (actor)|Erik Jensen]]\n| align=\"left\"| Dr. Steven Edwards\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| [[Teri Wyble]]\n| align=\"left\"| Amanda Shepherd\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| Steve Coulter\n| align=\"left\"| Reg Monroe\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| [[Daniel Bonjour]]\n| align=\"left\"| Aiden Monroe\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| Corey Brill\n| align=\"left\"| Pete Anderson\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| April Billingsley\n| align=\"left\"| Theresa\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"5\" {{n/a|}}\n|-\n| align=\"left\"| Tiffany Morgan\n| align=\"left\"| Erin\n| colspan=\"4\" {{n/a|}}\n| colspan=\"1\" {{cRecurring}}\n| colspan=\"1\" {{cGuest}}\n| colspan=\"4\" {{n/a|}}\n|-\n| align=\"left\"| [[Major Dodson]]\n| align=\"left\"| Sam Anderson\n| colspan=\"4\" {{n/a|}}\n| colspan=\"2\" {{cRecurring}}\n| colspan=\"4\" {{n/a|}}\n|-\n| align=\"left\"| [[Austin Abrams]]\n| align=\"left\"| Ron Anderson\n| colspan=\"4\" {{n/a|}}\n| colspan=\"2\" {{cRecurring}}\n| colspan=\"4\" {{n/a|}}\n|-\n| align=\"left\"| 