|[[\u2554]]|201}}\n|{{chset-color-graph}}|{{chset-cell|2569|[[\u2569]]|202}}\n|{{chset-color-graph}}|{{chset-cell|2566|[[\u2566]]|203}}\n|{{chset-color-graph}}|{{chset-cell|2560|[[\u2560]]|204}}\n|{{chset-color-graph}}|{{chset-cell|2550|[[\u2550]]|205}}\n|{{chset-color-graph}}|{{chset-cell|256C|[[\u256c]]|206}}\n|{{chset-color-graph}}|{{chset-cell|2567|[[\u2567]]|207}}\n|-\n!{{chset-left2|D_<br/>208}}\n|{{chset-color-graph}}|{{chset-cell|2568|[[\u2568]]|208}}\n|{{chset-color-graph}}|{{chset-cell|2564|[[\u2564]]|209}}\n|{{chset-color-graph}}|{{chset-cell|2565|[[\u2565]]|210}}\n|{{chset-color-graph}}|{{chset-cell|2559|[[\u2559]]|211}}\n|{{chset-color-graph}}|{{chset-cell|2558|[[\u2558]]|212}}\n|{{chset-color-graph}}|{{chset-cell|2552|[[\u2552]]|213}}\n|{{chset-color-graph}}|{{chset-cell|2553|[[\u2553]]|214}}\n|{{chset-color-graph}}|{{chset-cell|256B|[[\u256b]]|215}}\n|{{chset-color-graph}}|{{chset-cell|256A|[[\u256a]]|216}}\n|{{chset-color-graph}}|{{chset-cell|2518|[[\u2518]]|217}}\n|{{chset-color-graph}}|{{chset-cell|250C|[[\u250c]]|218}}\n|{{chset-color-graph}}|{{chset-cell|2588|[[\u2588]]|219}}\n|{{chset-color-graph}}|{{chset-cell|2584|[[\u2584]]|220}}\n|{{chset-color-graph}}|{{chset-cell|258C|[[\u258c]]|221}}\n|{{chset-color-graph}}|{{chset-cell|2590|[[\u2590]]|222}}\n|{{chset-color-graph}}|{{chset-cell|2580|[[\u2580]]|223}}\n|-\n!{{chset-left2|E_<br/>224}}\n|{{chset-color-letter}}|{{chset-cell|03B1|[[\u03b1]]|224}}\n|{{chset-color-letter}}|{{chset-cell|00DF|[[\u00df]]|225}}\n|{{chset-color-letter}}|{{chset-cell|0393|[[\u0393]]|226}}\n|{{chset-color-letter}}|{{chset-cell|03C0|[[\u03c0]]|227}}\n|{{chset-color-letter}}|{{chset-cell|03A3|[[Sigma|\u03a3]]|228}}\n|{{chset-color-letter}}|{{chset-cell|03C3|[[Sigma|\u03c3]]|229}}\n|{{chset-color-letter}}|{{chset-cell|00B5|[[\u00b5]]|230}}\n|{{chset-color-letter}}|{{chset-cell|03C4|[[\u03c4]]|231}}\n|{{chset-color-letter}}|{{chset-cell|03A6|[[\u03a6]]|232}}\n|{{chset-color-letter}}|{{chset-cell|0398|[[\u0398]]|233}}\n|{{ch