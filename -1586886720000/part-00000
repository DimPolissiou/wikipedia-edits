6 NCAA Division I FBS football season|2006]]\n | name         = [[2006 Virginia Tech Hokies football team|Virginia Tech]]\n | overall      = 10\u20133\n | conference   = 6\u20132\n | confstanding = 2nd <small>(Coastal)</small>\n | bowlname     = [[2006 Chick-fil-A Bowl|Chick-fil-A]]\n | bowloutcome  = L\n | bcsbowl      =\n | ranking      = 18\n | ranking2     = 19\n}}\n{{CFB Yearly Record Entry\n | championship = conference\n | year         = [[2007 NCAA Division I FBS football season|2007]]\n | name         = [[2007 Virginia Tech Hokies football team|Virginia Tech]]\n | overall      = 11\u20133\n | conference   = 7\u20131\n | confstanding = 1st <small>(Coastal)</small>\n | bowlname     = [[2008 Orange Bowl|Orange]]\n | bowloutcome  = L\n | bcsbowl      = yes\n | ranking      = 9\n | ranking2     = 9\n}}\n{{CFB Yearly Record Entry\n | championship = conference\n | year         = [[2008 NCAA Division I FBS football season|2008]]\n | name         = [[2008 Virginia Tech Hokies football team|Virginia Tech]]\n | overall      = 10\u20134\n | conference   = 5\u20133\n | confstanding = T\u20131st <small>(Coastal)</small>\n | bowlname     = [[2009 Orange Bowl|Orange]]\n | bowloutcome  = W\n | bcsbowl      = yes\n | ranking      = 14\n | ranking2     = 15\n}}\n{{CFB Yearly Record Entry\n | championship =\n | year         = [[2009 NCAA Division I FBS football season|2009]]\n | name         = [[2009 Virginia Tech Hokies football team|Virginia Tech]]\n | overall      = 10\u20133\n | conference   = 6\u20132\n | confstanding = 2nd <small>(Coastal)</small>\n | bowlname     = [[2009 Chick-fil-A Bowl|Chick-fil-A]]\n | bowloutcome  = W\n | bcsbowl      =\n | ranking      = 10\n | ranking2     = 10\n}}\n{{CFB Yearly Record Entry\n | championship = conference\n | year         = [[2010 NCAA Division I FBS football season|2010]]\n | name         = [[2010 Virginia Tech Hokies football team|Virginia Tech]]\n | overall      = 11\u20133\n | conference   = 8\u20130\n | confstanding = 1st <sma