o]]}}\n                    | crew02      = {{Sailor|Klinkenborg}}\n                    | fleetrace01 = {{Fleetrace|place=5|points=10.0}}\n                    | fleetrace02 = {{Fleetrace|place=26|points=32.0}}\n                    | fleetrace03 = {{Fleetrace|place=9|points=15.0}}\n                    | fleetrace04 = {{Fleetrace|place=DNF|points=66.0|discard=yes}}\n                    | fleetrace05 = {{Fleetrace|place=19|points=25.0}}\n                    | fleetrace06 = {{Fleetrace|place=2|points=3.0}}\n                    | fleetrace07 = {{Fleetrace|place=10|points=16.0}}\n                    | total       = 167.0\n                    | netto       = 101.0\n                  }}\n                  {{Sailing result line\n                    |year        = 1981\n                    | rank        = 10\n                    | countrycode = SWE \n                    | sailno      = S 118\n                    | helmsman    = {{Sailor|[[Jan Andersson (sailor)|Jan Andersson]]}}\n                    | crew01      = {{Sailor|[[Bertil Larsson (sailor)|Bertil Larsson]]}}\n                    | crew02      = {{Sailor|Wallaten}}\n                    | fleetrace01 = {{Fleetrace|place=12|points=18.0}}\n                    | fleetrace02 = {{Fleetrace|place=8|points=14.0}}\n                    | fleetrace03 = {{Fleetrace|place=41|points=47.0|discard=yes}}\n                    | fleetrace04 = {{Fleetrace|place=22|points=28.0}}\n                    | fleetrace05 = {{Fleetrace|place=16|points=22.0}}\n                    | fleetrace06 = {{Fleetrace|place=1|points=0.0}}\n                    | fleetrace07 = {{Fleetrace|place=14|points=20.0}}\n                    | total       = 149.0\n                    | netto       = 102.0\n                  }}\n                  {{Sailing result line\n                    |year        = 1981\n                    | rank        = 11\n                    | countrycode = AUT\n                    | sailno      = OE 70\n                    | helmsman    = {{Sai