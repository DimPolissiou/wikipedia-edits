an|last=Render|pos=OL|rs=y}}\n{{American football roster/Player|num=59|class=FR|first=Ross|last=Reynolds|pos=OL|rs=y}}\n{{American football roster/Player|num=58|class=SR|first=Eric|last=Simmons|pos=OL}}\n{{American football roster/Player|num=71|class=FR|first=Brett|last=Waechter|pos=OL}}\n{{American football roster/Player|num=65|class=SR|first=Jordan|last=Walsh|pos=OL}}\n{{American football roster/Player|num=73|class=JR|first=Ryan|last=Ward|pos=OL}}\n{{American football roster/Player|num=79|class=SO|first=Sean|last=Welsh|pos=OL}}\n\n|defensive_players=\n{{American football roster/Player|num=99|class=SO|first=Nathan|last=Bazata|pos=DL}}\n{{American football roster/Player|num=90|class=FR|first=Sam|last=Brincks|pos=DL|rs=y}}\n{{American football roster/Player|num=97|class=SR|first=Darian|last=Cooper|pos=DL}}\n{{American football roster/Player|num=56|class=JR|first=Faith|last=Ekakitie|pos=DL|link=y}}\n{{American football roster/Player|num=77|class=SO|first=Daniel|last=Gaffney|pos=DL}}\n{{American football roster/Player|num=60|class=FR|first=Jacob|last=Giese|pos=DL}}\n{{American football roster/Player|num=72|class=SO|first=Brant|last=Gressel|pos=DL}}\n{{American football roster/Player|num=93|class=FR|first=Terrence|last=Harris|pos=DL|rs=y}}\n{{American football roster/Player|num=40|class=FR|first=Parker|last=Hesse|pos=DL|rs=y}}\n{{American football roster/Player|num=88|class=SO|first=Jake|last=Hulett|pos=DL}}\n{{American football roster/Player|num=53|class=FR|first=Garret|last=Jansen|pos=DL}}\n{{American football roster/Player|num=67|class=JR|first=Jaleel|last=Johnson|pos=DL|link=y}}\n{{American football roster/Player|num=34|class=SR|first=Nate|last=Meier|pos=DL}}\n{{American football roster/Player|num=98|class=FR|first=Anthony|last=Nelson|pos=DL|link=y|dab= Anthony Nelson (American football)}}\n{{American football roster/Player|num=96|class=FR|first=Matt|last=Nelson|pos=DL|rs=y}}\n{{American football roster/Player|num=95|class=SR|first=Drew|last=Ott|pos=DL|inj=y}}\n{{Am