U|name_URU={{fbu|20|URU}}\n|team3=GHA|name_GHA={{fbu|20|GHA}}\n|team4=IRQ|name_IRQ={{fbu|20|IRQ}}\n|team5=ESP|name_ESP={{fbu|20|ESP}}\n|team6=CHI|name_CHI={{fbu|20|CHI}}\n|team7=UZB|name_UZB={{fbu|20|UZB}}\n|team8=KOR|name_KOR={{fbu|20|KOR}}\n|team9=COL|name_COL={{fbu|20|COL}}\n|team10=POR|name_POR={{fbu|20|POR}}\n|team11=CRO|name_CRO={{fbu|20|CRO}}\n|team12=NGA|name_NGA={{fbu|20|NGA}}\n|team13=TUR|name_TUR={{fbu|20|TUR}}\n|team14=PAR|name_PAR={{fbu|20|PAR|1990}}\n|team15=GRE|name_GRE={{fbu|20|GRE}}\n|team16=MEX|name_MEX={{fbu|20|MEX}}\n|team17=EGY|name_EGY={{fbu|20|EGY}}\n|team18=SLV|name_SLV={{fbu|20|SLV}}\n|team19=ENG|name_ENG={{fbu|20|ENG}}\n|team20=MLI|name_MLI={{fbu|20|MLI}}\n|team21=AUS|name_AUS={{fbu|20|AUS}}\n|team22=USA|name_USA={{fbu|20|USA}}\n|team23=NZL|name_NZL={{fbu|20|NZL}}\n|team24=CUB|name_CUB={{fbu|20|CUB}}\n|win_FRA=4|draw_FRA=2|loss_FRA=1|gf_FRA=15|ga_FRA=6\n|win_URU=4|draw_URU=2|loss_URU=1|gf_URU=10|ga_URU=3\n|win_GHA=4|draw_GHA=0|loss_GHA=3|gf_GHA=16|ga_GHA=12\n|win_IRQ=3|draw_IRQ=3|loss_IRQ=1|gf_IRQ=11|ga_IRQ=11\n|win_ESP=4|draw_ESP=0|loss_ESP=1|gf_ESP=9|ga_ESP=4\n|win_CHI=2|draw_CHI=1|loss_CHI=2|gf_CHI=9|ga_CHI=8\n|win_UZB=2|draw_UZB=1|loss_UZB=2|gf_UZB=7|ga_UZB=10\n|win_KOR=1|draw_KOR=3|loss_KOR=1|gf_KOR=8|ga_KOR=8\n|win_COL=2|draw_COL=2|loss_COL=0|gf_COL=6|ga_COL=2\n|win_POR=2|draw_POR=1|loss_POR=1|gf_POR=12|ga_POR=7\n|win_CRO=2|draw_CRO=1|loss_CRO=1|gf_CRO=4|ga_CRO=4\n|win_NGA=2|draw_NGA=0|loss_NGA=2|gf_NGA=7|ga_NGA=5\n|win_TUR=2|draw_TUR=0|loss_TUR=2|gf_TUR=6|ga_TUR=6\n|win_PAR=1|draw_PAR=2|loss_PAR=1|gf_PAR=3|ga_PAR=3\n|win_GRE=1|draw_GRE=2|loss_GRE=1|gf_GRE=4|ga_GRE=5\n|win_MEX=1|draw_MEX=0|loss_MEX=3|gf_MEX=6|ga_MEX=6\n|win_EGY=1|draw_EGY=0|loss_EGY=2|gf_EGY=4|ga_EGY=4\n|win_SLV=1|draw_SLV=0|loss_SLV=2|gf_SLV=2|ga_SLV=7\n|win_ENG=0|draw_ENG=2|loss_ENG=1|gf_ENG=3|ga_ENG=5\n|win_MLI=0|draw_MLI=2|loss_MLI=1|gf_MLI=2|ga_MLI=5\n|win_AUS=0|draw_AUS=1|loss_AUS=2|gf_AUS=3|ga_AUS=5\n|win_USA=0|draw_USA=1|loss_USA=2|gf_USA=3|ga_USA=9\n|win_NZL=