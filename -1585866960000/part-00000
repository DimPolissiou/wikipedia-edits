g]]\n| name_V\u00c5L = [[V\u00e5lerenga Fotball|V\u00e5lerengen]]\n\n| match_BRY_HAM = 0-2\n| match_BRY_KON = 4-1\n| match_BRY_LIL = 0-2\n| match_BRY_MIF = 0-1\n| match_BRY_MOL = 1-3\n| match_BRY_ROS = 3-1\n| match_BRY_IKS = 1-0\n| match_BRY_S\u00d8M = 4-1\n| match_BRY_TRO = 2-1\n| match_BRY_VIK = 2-0\n| match_BRY_V\u00c5L = 0-1\n\n| match_HAM_BRY = 2-0\n| match_HAM_KON = 2-1\n| match_HAM_LIL = 2-3\n| match_HAM_MIF = 3-2\n| match_HAM_MOL = 1-1\n| match_HAM_ROS = 1-1\n| match_HAM_IKS = 2-2\n| match_HAM_S\u00d8M = 1-0\n| match_HAM_TRO = 2-0\n| match_HAM_VIK = 1-1\n| match_HAM_V\u00c5L = 3-5\n\n| match_KON_BRY = 1-0\n| match_KON_HAM = 2-1\n| match_KON_LIL = 1-0\n| match_KON_MIF = 0-0\n| match_KON_MOL = 0-0\n| match_KON_ROS = 2-1\n| match_KON_IKS = 0-0\n| match_KON_S\u00d8M = 3-0\n| match_KON_TRO = 1-1\n| match_KON_VIK = 1-0\n| match_KON_V\u00c5L = 2-1\n\n| match_LIL_BRY = 3-0\n| match_LIL_HAM = 0-2\n| match_LIL_KON = 4-1\n| match_LIL_MIF = 3-1\n| match_LIL_MOL = 3-0\n| match_LIL_ROS = 1-0\n| match_LIL_IKS = 3-1\n| match_LIL_S\u00d8M = 1-0\n| match_LIL_TRO = 2-0\n| match_LIL_VIK = 3-1\n| match_LIL_V\u00c5L = 3-0\n\n| match_MIF_BRY = 4-2\n| match_MIF_HAM = 0-0\n| match_MIF_KON = 4-0\n| match_MIF_LIL = 2-0\n| match_MIF_MOL = 2-0\n| match_MIF_ROS = 0-2\n| match_MIF_IKS = 3-4\n| match_MIF_S\u00d8M = 3-1\n| match_MIF_TRO = 2-0\n| match_MIF_VIK = 0-1\n| match_MIF_V\u00c5L = 4-1\n\n| match_MOL_BRY = 1-3\n| match_MOL_HAM = 2-2\n| match_MOL_KON = 0-2\n| match_MOL_LIL = 0-1\n| match_MOL_MIF = 2-2\n| match_MOL_ROS = [[Molde FK\u2013Rosenborg BK rivalry|1-0]]\n| match_MOL_IKS = 0-4\n| match_MOL_S\u00d8M = 1-4\n| match_MOL_TRO = 1-1\n| match_MOL_VIK = 2-0\n| match_MOL_V\u00c5L = 1-2\n\n| match_ROS_BRY = 3-2\n| match_ROS_HAM = 1-0\n| match_ROS_KON = 1-3\n| match_ROS_LIL = 1-1\n| match_ROS_MIF = 3-0\n| match_ROS_MOL = [[Molde FK\u2013Rosenborg BK rivalry|1-1]]\n| match_ROS_S\u00d8M = 3-0\n| match_ROS_IKS = 1-0\n| match_ROS_TRO = 3-2\n| match_ROS_VIK = 4-1\n| match_ROS_V\u00c5L = 1-2\n