l\"|Editor\n! width=\"45%\" scpoe=\"col\" class=\"unsortable\"|Work(s)\n! width=\"4%\" scpoe=\"col\" class=\"unsortable\"|Ref(s)\n|- style=\"background:#B0C4DE;\"\n!scope=\"row\" align=\"center\" rowspan=\"5\" style=\"font-weight:normal;\"| [[1938 in literature|1939]]\n!align=\"center\" rowspan=\"5\" style=\"font-weight:normal;\"| 2014\n| {{sortname|John W.|Campbell, Jr.|John W. Campbell}}* || ''[[Analog Science Fiction and Fact|Astounding Science-Fiction]]''\n|align=\"center\"| <ref name=\"Hugo39\"/><ref name=\"MagazineEditors40s\"/>\n|-\n| {{sortname|Walter H.|Gillings|nolink=1}} || ''[[Tales of Wonder (magazine)|Tales of Wonder]]''\n|align=\"center\"| <ref name=\"Hugo39\"/><ref name=\"MagazineEditors40s\"/>\n|-\n| {{sortname|Raymond A.|Palmer}} || ''[[Amazing Stories]]''\n|align=\"center\"| <ref name=\"Hugo39\"/><ref name=\"MagazineEditors40s\"/>\n|-\n| {{sortname|Mort|Weisinger}} || ''[[Wonder Stories|Thrilling Wonder Stories]]''\n|align=\"center\"| <ref name=\"Hugo39\"/><ref name=\"MagazineEditors40s\"/>\n|-\n| {{sortname|Farnsworth|Wright}} || ''[[Weird Tales]]''\n|align=\"center\"| <ref name=\"Hugo39\"/><ref name=\"MagazineEditors40s\"/>\n|- style=\"background:#B0C4DE;\"\n!scope=\"row\" align=\"center\" rowspan=\"5\" style=\"font-weight:normal;\"| [[1940 in literature|1941]]\n!align=\"center\" rowspan=\"5\" style=\"font-weight:normal;\"| 2016\n| {{sortname|John W.|Campbell, Jr.|John W. Campbell}}* || ''[[Analog Science Fiction and Fact|Astounding Science-Fiction]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo41\"/>\n|-\n| {{sortname|Dorothy|McIlwraith}} || ''[[Weird Tales]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo41\"/>\n|-\n| {{sortname|Raymond A.|Palmer}} || ''[[Amazing Stories]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo41\"/>\n|-\n| {{sortname|Frederik|Pohl}} || ''[[Astonishing Stories]]'', ''[[Super Science Stories]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo41\"/>\n|-\n| {{sortname|Mort|Weisinger}} || ''[[Wonder Stories|Thrilling Wonder Stories]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo41\"/>\n|- style=\"background:#B0C4DE;\"\n!scope=\"row\" align=\"center\" rowspan=\"6\" style=\"font-weight:normal;\"| [[1942 in literature|1943]]\n!align=\"center\" rowspan=\"6\" style=\"font-weight:normal;\"| 2018\n| {{sortname|John W.|Campbell, Jr.|John W. Campbell}}* || ''[[Analog Science Fiction and Fact|Astounding Science-Fiction]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo43\"/>\n|-\n| {{sortname|Oscar J.|Friend}} || ''[[Wonder Stories|Thrilling Wonder Stories]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo43\"/>\n|-\n| {{sortname|Dorothy|McIlwraith}} || ''[[Weird Tales]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo43\"/>\n|-\n| {{sortname|Raymond A.|Palmer}} || ''[[Amazing Stories]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo41\"/>\n|-\n| {{sortname|Malcolm|Reiss|nolink=1}} || ''[[Planet Stories]]''\n|align=\"center\"| <ref name=\"MagazineEditors40s\"/><ref name=\"Hugo43\"/>\n|-\n| {{sortname|Donald A.|Wollheim}} || ''The Viking Portable Novels of Science''\n|align=\"center\"| <ref name=\"Hugo43\"/><ref name=\"VikingPortable\"/>\n|- style=\"background:#B0C4DE;\"\n!scope=\"row\" align=\"center\" rowspan=\"6\" style=\"font-weight:normal;\"| [[1943 in literature|1944]]\n!align=\"center\" rowspan=\"6\" style=\"font-weight:normal;\"| 2019\n| {{sortname|John W.|Campbell, Jr.|John W. Campbell}}* || ''[[Analog Science Fiction and Fact|Astounding Science-Fiction]]''\n|align=\"center\"| <ref name=\"Hugo44\"/><ref name=\"MagazineEditors40s\"/>\n|-\n| {{sortname|Oscar J.|Friend}} || ''[[Wonder Stories|Thrilling Wonder Stories]]''\n|align=\"center\"| <ref name=\"Hugo44\"/><ref name=\"MagazineEditors40s\"/>\n|-\n| {{sortname|Mary|Gnaedinger}} || ''[[Famous Fantastic Mysteries]]'