er|70 \n|align=center|309 \n|align=center|445 \n|align=center|7,641\n|align=center|55,464 \n|-\n|align=center|29 March 2020<ref name=\"29March\"></ref>\n|align=center|1,815\n|align=center|9,217 \n|align=center|8,981\n|align=center|23\n|align=center|131 \n|align=center|35\n|align=center|105 \n|align=center|394 \n|align=center|568 \n|align=center|9,982\n|align=center|65,446 \n|-\n|align=center|30 March 2020<ref name=\"30March\"></ref>\n|align=center|1,610\n|align=center|10,827 \n|align=center|10,497\n|align=center|37\n|align=center|168 \n|align=center|57\n|align=center|162 \n|align=center|523 \n|align=center|725 \n|align=center|11,535\n|align=center|76,981 \n|-\n|align=center|31 March 2020<ref name=\"31March\"></ref>\n|align=center|2,704\n|align=center|13,531 \n|align=center|13,074\n|align=center|46\n|align=center|214 \n|align=center|81\n|align=center|243 \n|align=center|622 \n|align=center|847 \n|align=center|15,422\n|align=center|92,403 \n|-\n|align=center|1 April 2020<ref name=\"1 April\" />\n|align=center|2,148\n|align=center|15,679 \n|align=center|15,069\n|align=center|63\n|align=center|277 \n|align=center|90\n|align=center|333 \n|align=center|692 \n|align=center|979 \n|align=center|14,397\n|align=center|106,799 \n|-\n|align=center|2 April 2020<ref name=\"2 April\" />\n|align=center|2,456\n|align=center|18,315 \n|align=center|17,544\n|align=center|79\n|align=center|356 \n|align=center|82\n|align=center|415 \n|align=center|783 \n|align=center|1,101 \n|align=center|18,757\n|align=center|125,556 \n|-\n|align=center|3 April 2020<ref name=\"3 April\" />\n|align=center|2,786\n|align=center|20,921 \n|align=center|20,012\n|align=center|69\n|align=center|425 \n|align=center|69\n|align=center|484 \n|align=center|867 \n|align=center|1,251 \n|align=center|16,160\n|align=center|141,716\n|-\n|align=center|4 April 2020<ref name=\"4 April\" />\n|align=center|3,013\n|align=center|23,934\n|align=center|22,647\n|align=center|76\n|align=center|501\n|align=center|302\n|align=center|7