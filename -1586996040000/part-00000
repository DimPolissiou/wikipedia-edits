rs.<ref>{{cite news|url=http://result.nanjing2014.qq.com/IRS/ENG/ZZ/ZZS103A_YO2014SW@@@@@@@ENG.htm |title=Number of Entries by NOC: Swimming |work=nanjing2014.org |date=6 August 2014 |accessdate=9 August 2014 |url-status=dead |archiveurl=https://web.archive.org/web/20140808210533/http://result.nanjing2014.qq.com/IRS/ENG/ZZ/ZZS103A_YO2014SW%40%40%40%40%40%40%40ENG.htm |archivedate=8 August 2014 }}</ref>\n\n;Boys\n{|class=wikitable style=\"font-size:90%\"\n|-\n!rowspan=2|Athlete\n!rowspan=2|Event\n!colspan=\"2\"|Heat\n!colspan=\"2\"|Semifinal\n!colspan=\"2\"|Final\n|-style=\"font-size:95%\"\n!Time\n!Rank\n!Time\n!Rank\n!Time\n!Rank\n|-align=center\n|align=left rowspan=4|[[Lim Ching Hwang]]\n|align=left|[[Swimming at the 2014 Summer Youth Olympics \u2013 Boys' 50 metre freestyle|50 m freestyle]]\n|24.26\n|28\n|colspan=4|Did not advance\n|-align=center\n|align=left|[[Swimming at the 2014 Summer Youth Olympics \u2013 Boys' 100 metre freestyle|100 m freestyle]]\n|51.64\n|19\n|colspan=4|Did not advance\n|-align=center\n|align=left|[[Swimming at the 2014 Summer Youth Olympics \u2013 Boys' 200 metre freestyle|200 m freestyle]]\n|1:51.49\n|10\n|colspan=2 {{n/a}}\n|colspan=2|Did not advance\n|-align=center\n|align=left|[[Swimming at the 2014 Summer Youth Olympics \u2013 Boys' 50 metre breaststroke|50 m breaststroke]]\n|30.63\n|34\n|colspan=4|Did not advance\n|-align=center\n|align=left rowspan=3|[[Welson Sim]]\n|align=left|[[Swimming at the 2014 Summer Youth Olympics \u2013 Boys' 200 metre freestyle|200 m freestyle]]\n|1:51.99\n|13\n|colspan=2 {{n/a}}\n|colspan=2|Did not advance\n|-align=center\n|align=left|[[Swimming at the 2014 Summer Youth Olympics \u2013 Boys' 400 metre freestyle|400 m freestyle]]\n|3:55.72\n|11\n|colspan=2 {{n/a}}\n|colspan=2|Did not advance\n|-align=center\n|align=left|[[Swimming at the 2014 Summer Youth Olympics \u2013 Boys' 800 metre freestyle|800 m freestyle]]\n|colspan=4 {{n/a}}\n|8:15.84\n|15\n|}\n\n;Girls\n{|class=wikitable style=\"font-size:90%\"\