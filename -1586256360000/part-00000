 1 | pages = 1\u201323\n | url = https://sp.lyellcollection.org/content/specpubgsl/173/1/1.full.pdf | access-date = 30 November 2019\n | doi = 10.1144/GSL.SP.2000.173.01.01 | ref = harv| bibcode = 2000GSLSP.173....1S\n }}<!-- {{Harvnb|Stampfli|2000}} -->\n* {{Cite journal\n | last1 = Torsvik | first1 = T. H.\n | last2 = Cocks | first2 = L. R. M.\n | title = Earth geography from 400 to 250 Ma: a palaeomagnetic, faunal and facies review\n | year = 2004 | journal = Journal of the Geological Society | volume = 161 | issue = 4 | pages = 555-572\n | url = http://www.earthdynamics.org/torsvik/torsvik-papers/2004/2004_Torsvik_JGeolSoc.pdf | access-date = 25 January 2020\n | doi = 10.1144/0016-764903-098 | ref = harv}}<!-- {{Harvnb|Torsvik|Cocks|2004}} -->\n* {{Cite journal\n | last1 = Torsvik | first1 = T. H.\n | last2 = Smethurst | first2 = M. A.\n | last3 = Meert | first3 = J. G.\n | last4 = Van der Voo | first4 = R.\n | last5 = McKerrow | first5 = W. S.\n | last6 = Brasier | first6 = M. D.\n | last7 = Sturt | first7 = B. A.\n | last8 = Walderhaug | first8 = H. J.\n | title = Continental break-up and collision in the Neoproterozoic and Palaeozoic\u2014a tale of Baltica and Laurentia\n | year = 1996 | journal = Earth-Science Reviews | volume = 40 | issue = 3\u20134 | pages = 229\u2013258\n | url = http://www.earthdynamics.org/torsvik/torsvik-papers/1996/1996_Torsvik_ESR.pdf | access-date = 22 December 2019\n | doi = 10.1016/0012-8252(96)00008-6 | ref = harv| bibcode = 1996ESRv...40..229T\n }}<!-- {{Harvnb|Torsvik|Smethurst|Meert|Van der Voo|1996}} -->\n* {{Cite journal\n | last1 = Torsvik | first1 = T. H.\n | last2 = Van der Voo | first2 = R.\n | last3 = Preeden | first3 = U.\n | last4 = Mac Niocaill | first4 = C.\n | last5 = Steinberger | first5 = B.\n | last6 = Doubrovine | first6 = P. V.\n | last7 = van Hinsbergen | first7 = D. J. J.\n | last8 = Domeier | first8 = M.\n | last9 = Gaina | first9 = C.\n | last10 = Tohver | first10 = E.\n | last11 = Meert | first11 = J. G.\