y}}\n{{American football roster/Player|num=4|first=Jordan|last=Fuller|link=y|pos=S|class=SO}}\n{{American football roster/Player|num=5|first=Baron|last=Browning|pos=LB|class=FR}}\n{{American football roster/Player|num=6|first=Sam|last=Hubbard|link=y|pos=DE|class=JR|note=C}}\n{{American football roster/Player|num=7|first=Damon|last=Webb|pos=S|class=SR}}\n{{American football roster/Player|num=9|first=Jashon|last=Cornell|pos=DT|class=SO|rs=y}}\n{{American football roster/Player|num=8|first=Kendall|last=Sheffield|pos=CB|class=SO|link=y}}\n{{American football roster/Player|num=10|first=Amir|last=Riep|pos=CB|class=FR}}\n{{American football roster/Player|num=11|first=Jalyn|last=Holmes|pos=DE|class=SR|note=C|link=y}}\n{{American football roster/Player|num=12|first=Denzel|last=Ward|link=y|pos=CB|class=JR}}\n{{American football roster/Player|num=13|first=Rashod|last=Berry|pos=DE|class=SO|rs=y}}\n{{American football roster/Player|num=14|first=Isaiah|last=Pryor|pos=S|class=FR}}\n{{American football roster/Player|num=15|first=Wayne|last=Davis|pos=DB|class=FR|rs=y}}\n{{American football roster/Player|num=16|first=Keandre|last=Jones|pos=LB|class=SO}}\n{{American football roster/Player|num=17|first=Jerome|last=Baker|pos=LB|class=JR|link=y|dab=Jerome Baker (American football)}}\n{{American football roster/Player|num=17|first=C. J.|last=Saunders|pos=CB|class=JR}}\n{{American football roster/Player|num=18|first=Jonathan|last=Cooper|pos=DE|class=FR|rs=y}}\n{{American football roster/Player|num=19|first=Eric|last=Glover-Williams|pos=S|class=JR}}\n{{American football roster/Player|num=20|first=Pete|last=Werner|pos=LB|class=FR}}\n{{American football roster/Player|num=21|first=Marcus|last=Williamson|pos=CB|class=FR}}\n{{American football roster/Player|num=23|first=Jahsen|last=Wint|pos=S|class=FR|rs=y}}\n{{American football roster/Player|num=24|first=Shaun|last=Wade|link=y|dab=Shaun Wade (American football)|pos=CB|class=FR}}\n{{American football roster/Player|num=25|first=Trevon|last=Forte|