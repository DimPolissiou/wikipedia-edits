13|[[\u3013]]|kuten=2-14}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-15}}\n|-\n!{{chset-left|3}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-16}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-17}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-18}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-19}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-21}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-21}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-22}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-23}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-24}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-25}}\n|{{chset-color-graph}}|{{chset-cell|2208|[[\u2208]]|kuten=2-26}}\n|{{chset-color-graph}}|{{chset-cell|220B|[[\u220b]]|kuten=2-27}}\n|{{chset-color-graph}}|{{chset-cell|2286|[[\u2286]]|kuten=2-28}}\n|{{chset-color-graph}}|{{chset-cell|2287|[[\u2287]]|kuten=2-29}}\n|{{chset-color-graph}}|{{chset-cell|2282|[[\u2282]]|kuten=2-30}}\n|{{chset-color-graph}}|{{chset-cell|2283|[[\u2283]]|kuten=2-31}}\n|-\n!{{chset-left|4}}\n|{{chset-color-graph}}|{{chset-cell|222A|[[\u222a]]|kuten=2-32}}\n|{{chset-color-graph}}|{{chset-cell|2229|[[\u2229]]|kuten=2-33}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-34}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-35}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-36}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-37}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-38}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-39}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-40}}\n|{{chset-color-undef}}|{{chset-cell|||kuten=2-41}}\n|{{chset-color-graph}}|{{chset-cell|2227|[[\u2227]]|kuten=2-42}}\n|{{chset-color-graph}}|{{chset-cell|2228|[[\u2228]]|kuten=2-43}}\n|{{chset-color-graph}}|{{chset-cell|00AC|[[\u00ac]]|kuten=2-44}}\n|{{chset-color-graph}}|{{chset-cell|21D2|[[\u21d2]]|kuten=2-45}}\n|{{chset-color-graph}}|{{chset-cell|21D4|[[\u21d4]]|kuten=2-46}}\n|{{chset-color-graph}}|{{chset-cell|2200|[[\u2200]]|kuten=2-47}}\n|-\n!{{chset-l