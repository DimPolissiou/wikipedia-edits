trl|00A0|[[Non-breaking space|NBSP]]|160}}\n|{{chset-color-graph-box}}|{{chset-cell|0385|[[\u0385]]|161}}\n|{{chset-color-letter-box}}|{{chset-cell|0386|\u0386|162}}\n|{{chset-color-graph}}|{{chset-cell|00A3|[[Pound sign|\u00a3]]|163}}\n|{{chset-color-graph-box}}|{{chset-cell|00A4|[[Currency (typography)|\u00a4]]|164}}\n|{{chset-color-graph-box}}|{{chset-cell|00A5|[[yen|\u00a5]]|165}}\n|{{chset-color-graph}}|{{chset-cell|00A6|[[Broken bar|&#xA6;]]|166}}\n|{{chset-color-ext-punct}}|{{chset-cell|00A7|[[section sign|\u00a7]]|167}}\n|{{chset-color-graph}}|{{chset-cell|00A8|[[trema (diacritic)|\u00a8]]|168}}\n|{{chset-color-graph}}|{{chset-cell|00A9|[[Copyright symbol|\u00a9]]|169}}\n|{{chset-color-undef-box}}|{{chset-cell|||170}}\n|{{chset-color-ext-punct}}|{{chset-cell|00AB|[[angle quotes|\u00ab]]|171}}\n|{{chset-color-graph}}|{{chset-cell|00AC|[[logical not|\u00ac]]|172}}\n|{{chset-color-ctrl}}|{{chset-ctrl|00AD|[[soft hyphen|SHY]]|173}}\n|{{chset-color-graph-box}}|{{chset-cell|00AE|[[\u00ae]]|174}}\n|{{chset-color-ext-punct}}|{{chset-cell|2015|[[Dash|\u2015]]|175}}\n|-\n!{{chset-left2|B_<br/>176}}\n|{{chset-color-graph}}|{{chset-cell|00B0|[[degree (symbol)|\u00b0]]|176}}\n|{{chset-color-graph}}|{{chset-cell|00B1|[[plus-minus sign|\u00b1]]|177}}\n|{{chset-color-digit}}|{{chset-cell|00B2|[[square (algebra)|\u00b2]]|178}}\n|{{chset-color-digit}}|{{chset-cell|00B3|[[cube (arithmetic)|\u00b3]]|179}}\n|{{chset-color-graph}}|{{chset-cell|0384|[[\u0384]]|180}}\n|{{chset-color-letter-box}}|{{chset-cell|00B5|[[micro sign|\u00b5]]|181}}\n|{{chset-color-ext-punct-box}}|{{chset-cell|00B6|[[pilcrow|\u00b6]]|182}}\n|{{chset-color-ext-punct}}|{{chset-cell|00B7|[[Interpunct|\u00b7]]|183}}\n|{{chset-color-letter}}|{{chset-cell|0388|\u0388|184}}\n|{{chset-color-letter}}|{{chset-cell|0389|\u0389|185}}\n|{{chset-color-letter}}|{{chset-cell|038A|\u038a|186}}\n|{{chset-color-ext-punct}}|{{chset-cell|00BB|[[angle quotes|\u00bb]]|187}}\n|{{chset-color-letter}}|{{chset-cell|038C|\u038c|188}}\