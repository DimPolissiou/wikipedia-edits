arch\"></ref>\n|align=center|277\n|align=center|947 \n|align=center|13\n|align=center|21 \n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|2,952\n|align=center|18,604\n|-\n|align=center|22 March 2020<ref name=\"22March\"></ref>\n|align=center|289\n|align=center|1,236 \n|align=center|9\n|align=center|30 \n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|1,738\n|align=center|20,345 \n|-\n|align=center|23 March 2020<ref name=\"23March\"></ref>\n|align=center|293\n|align=center|1,529 \n|align=center|7\n|align=center|37 \n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|3,672\n|align=center|24,017\n|-\n|align=center|24 March 2020<ref name=\"24March\"></ref>\n|align=center|343\n|align=center|1,872 \n|align=center|7\n|align=center|44 \n|align=center|-\n|align=center|26\n|align=center|102\n|align=center|136\n|align=center|3,952\n|align=center|27,969\n|-\n|align=center|25 March 2020<ref name=\"25March\"></ref>\n|align=center|561\n|align=center|2,433 \n|align=center|15\n|align=center|59 \n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|5,035\n|align=center|33,004 \n|-\n|align=center|26 March 2020<ref name=\"26March\"></ref>\n|align=center|1,196\n|align=center|3,629 \n|align=center|16\n|align=center|75 \n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|-\n|align=center|7,286\n|align=center|40,290\n|-\n|align=center|27 March 2020<ref name=\"27March\"></ref>\n|align=center|2,069\n|align=center|5,698 \n|align=center|17\n|align=center|92 \n|align=center|16\n|align=center|42\n|align=center|241\n|align=center|344\n|align=center|7,533\n|align=center|47,823 \n|-\n|align=center|28 March 2020<ref name=\"28March\"></ref>\n|align=center|1,704\n|align=center|7,402 \n|align=center|16\n|align=center|108 \n|align=center|28\n|align=center|70 \n|align=center|309 \n|align=center|445 \n|align=center|7,641\n|align=center|55,464 \n|-\n|align=center|29 March 2020<ref name=\"29March\"></ref>\n|align=center|1,815\n|align=center|9,217 \n|align=center|23\n|align=center|131 \n|align=center|35\n|align=center|105 \n|align=center|394 \n|align=center|568 \n|align=center|9,982\n|align=center|65,446 \n|-\n|align=center|30 March 2020<ref name=\"30March\"></ref>\n|align=center|1,610\n|align=center|10,827 \n|align=center|37\n|align=center|168 \n|align=center|57\n|align=center|162 \n|align=center|523 \n|align=center|725 \n|align=center|11,535\n|align=center|76,981 \n|-\n|align=center|31 March 2020<ref name=\"31March\"></ref>\n|align=center|2,704\n|align=center|13,531 \n|align=center|46\n|align=center|214 \n|align=center|81\n|align=center|243 \n|align=center|622 \n|align=center|847 \n|align=center|15,422\n|align=center|92,403 \n|-\n|align=center|1 April 2020<ref name=\"1 April\" />\n|align=center|2,148\n|align=center|15,679 \n|align=center|63\n|align=center|277 \n|align=center|90\n|align=center|333 \n|align=center|692 \n|align=center|979 \n|align=center|14,397\n|align=center|106,799 \n|-\n|align=center|2 April 2020<ref name=\"2 April\" />\n|align=center|2,456\n|align=center|18,315 \n|align=center|79\n|align=center|356 \n|align=center|82\n|align=center|415 \n|align=center|783 \n|align=center|1,101 \n|align=center|18,757\n|align=center|125,556 \n|-\n|align=center|3 April 2020<ref name=\"3 April\" />\n|align=center|2,786\n|align=center|20,921 \n|align=center|69\n|align=center|425 \n|align=center|69\n|align=center|484 \n|align=center|867 \n|align=center|1,251 \n|align=center|16,160\n|align=center|141,716\n|-\n|align=center|4 April 2020<ref name=\"4 April\" />\n|align=center|3,013\n|align=center|23,934\n|align=center|76\n|align=center|501\n|align=center|302\n|align=center|786\n|align=center|909\n|align=center|1,311\n|align=center|19,664\n|align=center|161,380\n\n|-\n|align=center|5 April 2020<ref name=\"5 April\" />\n|align=center|3,135\n|align=center|27,069\n|align=center|73\n|align=center|574\n|align=center|256\n|align=center|1,042\n|align=center|935\n|align=center|1,381\n|align=center|20,065\n|align=center|181,445\n\n|-\n|align=center|6 April 2020<ref name=\" 6 April\" />\n|align=center|3,148\n|align=center|30,217\n|align=center|75\n|align=center|649\n|align=center|284\n|align=center|1,326\n|align=center|966\n|align=center|1,415\n|align=center|21,400\n|align=center|202,845\n\n|-\n|align=center|7 April 2020<ref name=\" 7 April\">{{cite news |title=Turkey: Death toll from coronavirus rises to 725 |url=https://www.aa.com.tr/en/latest-on-coronavirus-outbreak/turkey-death-toll-from-coronavirus-rises-to-725/1796179 |accessdate=7 April 2020 |agency=Anadolu Agency}}</ref>\n|align=center|3,892\n|align=center|34,109\n|align=center|76\n|align=center|725\n|align=center|256\n|align=center|1,582\n|align=center|987\n|align=center|1,464\n|align=center|20,023\n|align=center|222,868\n|}\n\n{{notelist}}\n\n===Graphs===\n{{Graph:Chart\n|type=line\n|linewidth=1\n|showSymbols=1\n|width=650\n|height=450\n|colors=#FF4500,#A50026,#87CEEB\n|xAxisTitle=Date\n|xAxisAngle=-45\n|xType=date\n|xAxisFormat=%b %d\n|x= 2020/03/11, 2020/03/12, 2020/03/13, 2020/03/14, 2020/03/15, 2020/03/16, 2020/03/17, 2020/03/18, 2020/03/19, 2020/03/20, 2020/03/21, 2020/03/22, 2020/03/23, 2020/03/24, 2020/03/25, 2020/03/26, 2020/03/27, 2020/03/28, 2020/03/29, 2020/03/30, 2020/03/31, 2020/04/01, 2020/04/02, 2020/04/03, 2020/04/04, 2020/04/05, 2020/04/06, 2020/04/07\n|yType=integer\n|yAxisMin=1\n<!--|yAxisTitle=No. of cases-->\n|legend=Legend\n|y1=1,1,5,6,18,47,98,191,359,670,947,1236,1529,1872,2433,3629,5698,7402,9217,10827,13531,15679,18135,20921,23924,27069,30217,34109\n|y1Title=Total confirmed cases\n|y2=, , , , , , 1, 2, 4, 9, 21, 30, 37, 44, 59, 75, 92, 108, 131, 168, 214, 277, 356, 425, 505, 574, 649,725\n|y2Title=Total deaths\n|y3=, , , , , , , , , , , , , 26, 26, 26, 42, 70, 105, 162, 243, 333, 415, 484, 786, 1042, 1326,1582\n|y3Title=Total recovered cases\n|yScaleType=log\n|yGrid=true\n|xGrid=true\n}}\n \n{{Graph:Chart\n|width=650\n|c