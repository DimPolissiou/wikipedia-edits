&nbsp;km/h)}} || bgcolor=#{{storm colour|TS}}|{{Sort|0993|993&nbsp;hPa (29.33&nbsp;inHg)}} || None || {{ntsh|0}} None || {{ntsh|0}} None ||\n|-\n| '''Kiko''' || {{Sort|0825|August 25&nbsp;\u2013 29}} || bgcolor=#{{storm colour|3}}|{{Sort|4|Category 3 hurricane}} || bgcolor=#{{storm colour|3}}|{{Sort|120|120&nbsp;mph (195&nbsp;km/h)}} || bgcolor=#{{storm colour|3}}|{{Sort|0955|955&nbsp;hPa (28.20&nbsp;inHg)}} || Baja California Sur, Western Mexico || {{ntsh|1}} Minor || {{ntsh|0}} None ||\n|-\n| '''Lorena''' || {{Sort|0827|August 27&nbsp;\u2013 September 6}} || bgcolor=#{{storm colour|1}}|{{Sort|2|Category 1 hurricane}} || bgcolor=#{{storm colour|1}}|{{Sort|075|75&nbsp;mph (120&nbsp;km/h)}} || bgcolor=#{{storm colour|1}}|{{Sort|0989|989&nbsp;hPa (29.21&nbsp;inHg)}} || None || {{ntsh|0}} None || {{ntsh|0}} None ||\n|-\n| '''Manuel''' || {{sort|0828|August 28&nbsp;\u2013 31}} || bgcolor=#{{storm colour|TS}}|{{Sort|1|Tropical Storm}} || bgcolor=#{{storm colour|TS}}|{{Sort|045|45&nbsp;mph (75&nbsp;km/h)}} || bgcolor=#{{storm colour|TS}}|{{Sort|1002|1002&nbsp;hPa (29.59&nbsp;inHg)}} || None || {{ntsh|0}} None || {{ntsh|0}} None ||\n|-\n| '''Narda''' || {{sort|0903|September 3&nbsp;\u2013 7}} || bgcolor=#{{storm colour|TS}}|{{Sort|1|Tropical Storm}} || bgcolor=#{{storm colour|TS}}|{{Sort|050|50&nbsp;mph (85&nbsp;km/h)}} || bgcolor=#{{storm colour|TS}}|{{Sort|1000|1000&nbsp;hPa (29.53&nbsp;inHg)}} || None || {{ntsh|0}} None || {{ntsh|0}} None ||\n|-\n| '''Octave''' || {{Sort|0908|September 8&nbsp;\u2013 16}} || bgcolor=#{{storm colour|4}}|{{Sort|5|Category 4 hurricane}} || bgcolor=#{{storm colour|4}}|{{sort|130|130&nbsp;mph (215&nbsp;km/h)}} || bgcolor=#{{storm colour|4}}|{{Sort|0948|948&nbsp;hPa (28.00&nbsp;inHg)}} || None || {{ntsh|0}} None || {{ntsh|0}} None ||\n|-\n| '''Priscilla''' || {{sort|0921|September 21&nbsp;\u2013 25}} || bgcolor=#{{storm colour|TS}}|{{Sort|1|Tropical Storm}} || bgcolor=#{{storm colour|TS}}|{{Sort|065|65&nbsp;mph (100&nbsp;km/h)}} || bgcolor=#{{