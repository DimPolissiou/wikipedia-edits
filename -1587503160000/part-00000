ane|num=59|pos=DL|class=FR}}\n{{American football roster/Player|first=Jack|last=Kern|num=67|pos=DL|class=FR}}\n{{American football roster/Player|first=Boye|last=Mafe|num=69|pos=DL|class=FR|rs=y}}\n{{American football roster/Player|first=Malcolm|last=Robinson|num=75|pos=DL|class=FR|rs=y}}\n{{American football roster/Player|first=Nate|last=Umlor|num=87|pos=DL|class=SO}}\n{{American football roster/Player|first=Sam|last=Renner|num=90|pos=DL|class=JR|rs=y}}\n{{American football roster/Player|first=Noah|last=Hickcox|num=91|pos=DL|class=FR|rs=y}}\n{{American football roster/Player|first=Mayan|last=Ahanotu|num=92|pos=DL|class=FR}}\n{{American football roster/Player|first=Connor|last=Novak-Goar|num=93|pos=DL|class=SO|rs=y}}\n{{American football roster/Player|first=Abi|last=N-Okonji|num=94|pos=DL|class=FR}}\n{{American football roster/Player|first=Victor|last=White|num=95|pos=DL|class=SO|rs=y|note=I}}\n{{American football roster/Player|first=Royal|last=Silver|num=97|pos=DL|class=JR|rs=y}}\n{{American football roster/Player|first=O.J.|last=Smith|num=99|pos=DL|class=SR|rs=y}}\n|special_teams_players=\n{{American football roster/Player|first=Casey|last=O'Brien|num=14|pos=H|class=FR|rs=y}}\n{{American football roster/Player|first=Payton|last=Jordahl|num=25|pos=LS|class=SR|rs=y}}\n{{American football roster/Player|first=Grant|last=Ryerse|num=33|pos=K|pos2=P|class=FR|rs=y}}\n{{American football roster/Player|first=Brock|last=Walker|num=34|pos=K|class=FR}}\n{{American football roster/Player|first=John|last=Mack|num=37|pos=K|class=SO|rs=y}}\n{{American football roster/Player|first=Emmit|last=Carpenter|num=38|pos=K|class=SR|rs=y}}\n{{American football roster/Player|first=Michael|last=Vojvodich|num=39|pos=LS|class=FR}}\n{{American football roster/Player|first=Alex|last=Melvin|num=46|pos=P|class=JR|rs=y}}\n{{American football roster/Player|first=Jacob|last=Herbers|num=47|pos=P|class=JR|rs=y}}\n{{American football roster/Player|first=Anders|last=Gelecinskyj|num=48|pos=K|class=SO|note=I}