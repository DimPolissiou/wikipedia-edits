ce|last=Witham|num=85|pos=TE|class=JR|rs=y}}\n{{American football roster/Player|first=Clayton|last=Witherspoon|num=86|pos=TE|class=FR}}\n{{American football roster/Player|first=Yale|last=Van Dyne|num=87|pos=WR|class=SO|rs=y}}\n{{American football roster/Player|first=Brevyn|last=Spann-Ford|num=88|pos=TE|class=FR}}\n{{American football roster/Player|first=Matt|last=Morse|num=89|pos=WR|class=JR|rs=y}}\n{{American football roster/Player|first=Nathan|last=Bursch|num=96|pos=TE|class=FR|rs=y}}\n{{American football roster/Player|first=Max|last=Jones|num=98|pos=TE|class=FR}}\n|defensive_players=\n{{American football roster/Player|first=Jacob|last=Huff|num=2|pos=DB|class=SR}}\n{{American football roster/Player|first=Jerry|last=Gibson|num=3|pos=DL|class=SR|rs=y}}\n{{American football roster/Player|first=Terell|last=Smith|num=4|pos=DB|class=FR}}\n{{American football roster/Player|first=Trenton|last=Guthrie|num=5|pos=LB|class=FR|rs=y}}\n{{American football roster/Player|first=Chris|last=Williamson|num=6|pos=DB|class=JR|rs=y}}\n{{American football roster/Player|first=Ray|last=Estes|num=7|pos=DB|class=FR|rs=y}}\n{{American football roster/Player|first=Thomas|last=Rush|num=8|pos=LB|class=FR}}\n{{American football roster/Player|first=Esezi|last=Otomewo|num=9|pos=DL|class=FR|rs=y}}\n{{American football roster/Player|first=Antoine|last=Winfield Jr.|link=y|num=11|pos=DB|class=SO|rs=y}}\n{{American football roster/Player|first=Tai'yon|last=Devers|num=12|pos=DL|class=JR}}\n{{American football roster/Player|first=Braelen|last=Oliver|num=14|pos=DB|class=FR}}\n{{American football roster/Player|first=C. J.|last=Smith|num=15|pos=LB|class=FR}}\n{{American football roster/Player|first=Coney|last=Durr|num=16|pos=DB|class=JR}}\n{{American football roster/Player|first=Gary|last=Moore|num=19|pos=DL|class=SR|rs=y}}\n{{American football roster/Player|first=Julian|last=Huff|num=20|pos=LB|class=SR}}\n{{American football roster/Player|first=Kamal|last=Martin|num=21|pos=LB|class=JR}}\n{{American football