y ([[Federazione Industria Musicale Italiana|FIMI]])<ref>{{cite web|url=https://www.fimi.it/top-of-the-music/classifiche.kl#/charts/3/2020/14|title=Top Singoli \u2013 Classifica settimanale WK 14|publisher=[[Federazione Industria Musicale Italiana]]|language=Italian|accessdate=April 4, 2020}}</ref>\n| 80\n|-\n! scope=\"row\"| Lithuania ([[AGATA (organization)|AGATA]])<ref>{{cite web|url=https://www.agata.lt/lt/naujienos/savaites-klausomiausi-2020w14/|title=2020 14-os savait\u0117s klausomiausi (TOP 100)|publisher=[[AGATA (organization)|AGATA]]|language=Lithuanian|date=April 3, 2020|accessdate=April 10, 2020}}</ref>\n| 21\n|-\n{{single chart|Dutch100|78|artist=Megan Thee Stallion|song=Savage|rowheader=true|accessdate=April 3, 2020}}\n|-\n! scope=\"row\"| New Zealand ([[Recorded Music NZ]])<ref>{{cite web|url=https://nztop40.co.nz/chart/singles?chart=4841|title=NZ Top 40 Singles Chart|publisher=[[Recorded Music NZ]]|date=April 6, 2020|accessdate=April 4, 2020}}</ref>\n| 21\n|-\n{{single chart|Portugal|60|artist=Megan Thee Stallion|song=Savage|rowheader=true|accessdate=April 9, 2020}}\n|-\n{{single chart|Scotland|50|date=20200403|rowheader=true|accessdate=April 4, 2020}}\n|-\n{{single chart|Slovakdigital|29|year=2020|week=14|rowheader=true|accessdate=April 7, 2020}}\n|-\n! scope=\"row\"| Sweden Heatseeker ([[Sverigetopplistan]])<ref>{{cite web|url=https://www.sverigetopplistan.se/chart/215?dspy=2020&dspp=15|title=Veckolista Heatseeker, vecka 15|publisher=[[Sverigetopplistan]]|accessdate=April 10, 2020}}</ref>\n| 2\n|-\n{{single chart|Switzerland|46|artist=Megan Thee Stallion|song=Savage|rowheader=true|accessdate=April 6, 2020}}\n|-\n{{single chart|UK|28|date=20200403|rowheader=true|accessdate=April 4, 2020}}\n|-\n{{single chart|UKrandb|14|date=20200403|rowheader=true|accessdate=April 4, 2020}}\n|-\n{{single chart|Billboardhot100|18|artist=Megan Thee Stallion|rowheader=true|accessdate=April 7, 2020|refname=Hot 100}}\n|-\n{{single chart|Billboardrandbhiphop|9|artist=Mega