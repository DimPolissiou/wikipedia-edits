/}} ||chair ||cadeira\n|-\n|\u0d15\u0d3e\u0d2a\u0d4d\u0d2a ||k\u0101ppa ||{{IPA|/ka:pa/}} ||Cape (cloth) ||capa\n|-\n|\u0d15\u0d3e\u0d2a\u0d4d\u0d2a\u0d3f\u0d30\u0d3f  ||k\u0101ppiri ||{{IPA|/ka:pi\u027ei/}} ||African ||cafre\n|-\n|\u0d15\u0d41\u0d28\u0d40\u0d32\u0d4d\u200d ||kun\u012bl ||{{IPA|/kuni:l}} ||funnel ||funil\n|-\n|\u0d15\u0d41\u0d2e\u0d4d\u0d2a\u0d38\u0d3e\u0d30\u0d02 ||kumpas\u0101ra\u1e41 ||{{IPA|/kumbasa:\u027eam/}} ||Confession ||confessar\n|-\n|\u0d15\u0d41\u0d2e\u0d4d\u0d2a\u0d3e\u0d24\u0d3f\u0d30\u0d3f ||kump\u0101tiri ||{{IPA|/kumba:d\u032airi/}} ||Godparent ||compadre\n|-\n|\u0d15\u0d41\u0d30\u0d3f\u0d36\u0d4d ||kuri\u015b\u016d ||{{IPA|/kuri\u0283\u0259/}} ||cross ||cruz\n|-\n|\u0d15\u0d41\u0d36\u0d3f\u0d28\u0d3f  ||ku\u015bini ||{{IPA|/ku\u0283ini/}} ||kitchen ||cozinha\n|-\n|\u0d15\u0d4a\u0d28\u0d4d\u0d24 ||konta ||{{IPA|/kond\u032aa/}} ||Rosary ||conta\n|-\n|\u0d15\u0d4a\u0d30\u0d1f\u0d3e\u0d35\u0d4d ||kora\u1e6d\u0101v\u016d ||{{IPA|/ko\u027ea\u0256a:v/}} ||whip ||corda\n|-\n|\u0d15\u0d4a\u0d32\u0d47\u0d30\u0d3f ||kol\u0113ri ||{{IPA|/kole:\u027ei/}} ||trowel ||colher\n|-\n|\u0d15\u0d4a\u0d35\u0d47\u0d28\u0d4d\u0d24 ||kov\u0113nta ||{{IPA|/ko\u028be:nd\u032aa/}} ||convent ||conventa\n|-\n|\u0d15\u0d4b\u0d1f\u0d24\u0d3f ||k\u014d\u1e6dati ||{{IPA|/ko:\u0256ad\u032ai/}} ||court ||corte\n|-\n|\u0d15\u0d4b\u0d2a\u0d4d\u0d2a ||k\u014dppa ||{{IPA|/ko:pa/}} ||cup ||copa\n|-\n|\u0d15\u0d4d\u0d30\u0d3f\u0d38\u0d4d\u0d24\u0d41 ||kristu ||{{IPA|/krist\u032au/}} ||Christ ||cristo\n|-\n|\u0d17\u0d41\u0d26\u0d3e\u0d02 ||gud\u0101\u1e41 ||{{IPA|/gud\u032aa:m/}} ||godown, factory ||god\u00e3o\n|-\n|\u0d17\u0d4b\u0d35\u0d30\u0d4d\u0d23\u0d24\u0d26\u0d4b\u0d30\u0d4d\u200d ||g\u014dvar\u1e47ad\u014dr ||{{IPA|/go:\u028bar\u0273ad\u032aor/}} ||Governor ||governador\n|-\n|\u0d1a\u0d15\u0d32\u0d3e\u0d38\u0d4d ||cakal\u0101s\u016d ||{{IPA|/t\u0361\u0283agala:s\u0259/}} ||a red blanket ||escarlate\n|-\n|\u0d1a\u0d19\u0d4d\u0d19\u0d3e\u0d1f\u0d02 ||ca\u1e45\u1e45\u