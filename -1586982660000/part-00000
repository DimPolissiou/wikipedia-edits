|align=\"center\"|\n|- bgcolor=\"#F0F8FF\"\n|2013\n|\"Para enamorarte de m\u00ed\" \n|rowspan=5| ''[[T\u00fa y Yo (David Bisbal album)|T\u00fa y Yo]]''\n|align=\"center\"|16\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|- bgcolor=\"#F0F8FF\"\n|rowspan=\"4\"|2014\n|\"[[Diez Mil Maneras]]\" \n|align=\"center\"|1\n|align=\"center\"|40\n|align=\"center\"|\u2014\n|align=\"center\"|\u2014\n|align=\"center\"|\u2014\n|align=\"center\"|\u2014\n|align=\"center\"|\u2014\n|align=\"center\"|14\n|align=\"center\"|\u2014\n|align=\"center\"|5\n|align=\"center\"|\u2014\n|- bgcolor=\"#F0F8FF\"\n|\"T\u00fa y Yo\" \n|align=\"center\"|8\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|- bgcolor=\"#F0F8FF\"\n|\"S\u00ed pero no\" \n|align=\"center\"|6\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|- bgcolor=\"#F0F8FF\"\n|\"No amanece\" \n|align=\"center\"|4\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|- bgcolor=\"#F0F8FF\"\n|2015\n|\"El ruido\"\n|\n|align=\"center\"|45\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|- bgcolor=\"#F0F8FF\"\n|rowspan=\"6\"|2016\n|\"Me enamor\u00e9 de ti\"\n|\n|align=\"center\"|13\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\n|align=\"center\"|\