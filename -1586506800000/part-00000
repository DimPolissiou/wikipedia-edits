er | daynight = yes | time = 20:00\n| team1 = {{cr-PAK|lions|L}}\n| score1 = 194/5 (20 overs)\n| score2 = 112 (18.4 overs)\n| team2 = {{cr-PAK|hawks|R}}\n| runs1 = [[Ahmed Shehzad]] 72 (50)\n| wickets1 = [[Nasir Awais]] 3/43 (4 overs)\n| runs2 = [[Sharjeel Khan]] 30 (23)\n| wickets2 = [[Raza Ali Dar]] 2/9 (3 overs)\n| result = Lahore Lions won by 74 runs\n| report = [http://www.espncricinfo.com/pakistan-domestic-2011/engine/match/533332.html Scorecard]\n| venue = [[National Stadium, Karachi]]\n| umpires = [[Hakeem Shah]] and [[Khalid Mahmood (umpire)|Khalid Mahmood]]\n| motm =\n| toss = Hyderabad Hawks won the toss and chose to field\n| notes = [[Tanzeel Altaf]] (Lahore Lions) made his T20 debut.\n}}\n----\n{{Limited overs matches\n| date = {{anchor|match4}} 26 September | daynight = | time = 12:00\n| team1 = {{cr-PAK|stall|L}}\n| score1 = 98/1 (16.4 overs)\n| score2 = 96/8 (20 overs)\n| team2 = {{cr-PAK|hawks|R}}\n| runs1 = [[Shoaib Malik]] 50 (52)\n| wickets1 = [[Nasir Awais]] 1/16 (2.4 overs)\n| runs2 = [[Shahzad Haider]] 42 (43)\n| wickets2 = [[Sarfraz Ahmed]] 4/13 (4 overs)\n| result = Sialkot Stallions won by 9 wickets\n| report = [http://www.espncricinfo.com/pakistan-domestic-2011/engine/match/533334.html Scorecard]\n| venue = [[National Stadium, Karachi]]\n| umpires = [[Anis-ur-Rehman]] and [[Rasheed Bhatti]]\n| motm = [[Sarfraz Ahmed]]\n| toss = Hyderabad Hawks won the toss and chose to bat\n| notes = [[Shahzad Haider]] (Hyderabad Hawks) made his T20 debut.\n}}\n----\n{{Limited overs matches\n| date = {{anchor|match9}} 27 September | daynight = yes | time = 20:00\n| team1 = {{cr-PAK|lions|L}}\n| score1 = 121 (19.4 overs)\n| score2 = 124/4 (19.1 overs)\n| team2 = {{cr-PAK|stall|R}}\n| runs1 = [[Abdul Razzaq (cricketer)|Abdul Razzaq]] 42 (33)\n| wickets1 = [[Naved-ul-Hasan]] 3/30 (4 overs)\n| runs2 = [[Shahid Yousuf]] 47 (52)\n| wickets2 = [[Raza Ali Dar]] 1/13 (4 overs)\n| result = Sialkot Stallions won by 6 wickets\n| report = [http://www.espncricinfo.com/p