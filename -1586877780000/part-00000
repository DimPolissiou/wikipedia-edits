ium=\n|attendance=\n|referee=\n|report=https://www.fifa.com/worldcup/preliminaries/southamerica/matches/match/400104635/\n}}\n{{Football box\n|id=CHI v ARG\n|date={{Start date|2021|10|7|df=y}}\n|time=\n|team1={{fb-rt|CHI}}\n|score=\n|team2={{fb|ARG}}\n|goals1=\n|goals2=\n|stadium=\n|attendance=\n|referee=\n|report=https://www.fifa.com/worldcup/preliminaries/southamerica/matches/match/400104621/\n}}\n{{Football box\n|id=ECU v BRA\n|date={{Start date|2021|10|7|df=y}}\n|time=\n|team1={{fb-rt|ECU}}\n|score=\n|team2={{fb|BRA}}\n|goals1=\n|goals2=\n|stadium=\n|attendance=\n|referee=\n|report=https://www.fifa.com/worldcup/preliminaries/southamerica/matches/match/400104633/\n}}\n\n===Matchday 16===\n{{Football box\n|id=URU v VEN\n|date={{Start date|2021|10|12|df=y}}\n|time=\n|team1={{fb-rt|URU}}\n|score=\n|team2={{fb|VEN}}\n|goals1=\n|goals2=\n|stadium=\n|attendance=\n|referee=\n|report=https://www.fifa.com/worldcup/preliminaries/southamerica/matches/match/400104629/\n}}\n{{Football box\n|id=PER v ECU\n|date={{Start date|2021|10|12|df=y}}\n|time=\n|team1={{fb-rt|PER}}\n|score=\n|team2={{fb|ECU}}\n|goals1=\n|goals2=\n|stadium=\n|attendance=\n|referee=\n|report=https://www.fifa.com/worldcup/preliminaries/southamerica/matches/match/400104631/\n}}\n{{Football box\n|id=BRA v PAR\n|date={{Start date|2021|10|12|df=y}}\n|time=\n|team1={{fb-rt|BRA}}\n|score=\n|team2={{fb|PAR}}\n|goals1=\n|goals2=\n|stadium= [[Arena da Amaz\u00f4nia]], [[Manaus]]\n|attendance=\n|referee=\n|report=https://www.fifa.com/worldcup/preliminaries/southamerica/matches/match/400104623/\n}}\n{{Football box\n|id=BOL v CHI\n|date={{Start date|2021|10|12|df=y}}\n|time=\n|team1={{fb-rt|BOL}}\n|score=\n|team2={{fb|CHI}}\n|goals1=\n|goals2=\n|stadium=\n|attendance=\n|referee=\n|report=https://www.fifa.com/worldcup/preliminaries/southamerica/matches/match/400104619/\n}}\n{{Football box\n|id=ARG v COL\n|date={{Start date|2021|10|12|df=y}}\n|time=\n|team1={{fb-rt|ARG}}\n|score=\n|team2={{fb|COL}}\n|goals1=\n|goals2=\n|