el-color12 = red\n| mark-coord13 = {{coord|49.098 |-117.709}} |label13=Trail           |label-pos13  = right | mark13 = Red pog.svg   | label-color13 = red\n| mark-coord14 = {{coord|50.275 |-119.238}} |label14=Vernon          |label-pos14  = right | mark14 = Red pog.svg   | label-color14 = red\n| mark-coord15 = {{coord|47.442 |-120.322}} |label15=Wenatchee       |label-pos15  = right | mark15 = Red pog.svg   | label-color15 = red\n| mark-coord16 = {{coord|49.858 |-119.582}} |label16=West Kelowna    |label-pos16  = right | mark16 = Red pog.svg   | label-color16 = red\n| caption = [[Image:Black pog.svg|10px]] Island Division [[Image:Green pog.svg|10px]] Mainland Division [[Image:Red pog.svg|10px]] Interior Division\n}}\n{| class=\"wikitable\" style=\"width:60em\"\n|-\n!Division\n!Team\n!City\n!Arena\n!Joined BCHL\n|-\n!rowspan=5|Island\n|'''[[Alberni Valley Bulldogs]]'''\n|[[Port Alberni|Port Alberni, British Columbia]]\n|[[Weyerhaeuser Arena]]\n|1998\n|-\n|'''[[Cowichan Valley Capitals]]'''\n|[[Duncan, British Columbia]]\n|[[Cowichan Centre|Island Savings Centre]]\n|1980\n|-\n|'''[[Nanaimo Clippers]]'''\n|[[Nanaimo|Nanaimo, British Columbia]]\n|[[Frank Crane Arena]]\n|1972\n|-\n|'''[[Victoria Grizzlies]]'''\n|[[Victoria, British Columbia]]\n|[[The Q Centre]]\n|1967\n|-\n|'''[[Powell River Kings]]'''\n|[[Powell River, British Columbia]]\n|Hap Parker Arena\n|1988\n|- style=\"border-top: 2px solid black;\"\n!rowspan=5|Mainland\n|'''[[Chilliwack Chiefs (2011\u2013)|Chilliwack Chiefs]]'''\n|[[Chilliwack|Chilliwack, British Columbia]]\n|[[Chilliwack Coliseum]]\n|1996\n|-\n|'''[[Coquitlam Express]]'''\n|[[Coquitlam|Coquitlam, British Columbia]]\n|[[Poirier Sport & Leisure Complex]]\n|2001\n|-\n|'''[[Langley Rivermen]]'''\n|[[Langley, British Columbia (district municipality)|Langley, British Columbia]]\n|George Preston Recreation Centre\n|1990\n|-\n|'''[[Prince George Spruce Kings]]'''\n|[[Prince George, British Columbia]]\n|[[Rolling Mix Concrete Arena]]\n|1972\n|-\n|'''[[S