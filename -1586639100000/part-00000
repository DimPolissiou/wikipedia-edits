n=right|36||align=right|'''46'''\n|Round of 32\n| rowspan=\"3\" |Second Division B<br />\n|-\n|[[1991\u201392 Segunda Divis\u00e3o B|1991\u201392]]\n|align=right |'''5th'''\n|align=right|34||align=right|11||align=right|15||align=right|8\n|align=right|39||align=right|31||align=right|'''37'''\n|Fourth Round\n|-\n|[[1992\u201393 Segunda Divis\u00e3o B|1992\u201393]]\n|align=right bgcolor=\"#FFCCCC\"|{{down-arrow|alt=relegated}} '''17th'''\n|align=right|34||align=right|6||align=right|6||align=right|22\n|align=right|26||align=right|66||align=right|'''18'''\n|Fourth Round\n|-\n|[[1993\u201394 Terceira Divis\u00e3o Serie F|1993\u201394]]\n|scope=col rowspan=\"1\" |4\n|align=right bgcolor=\"#DDFFDD\"|{{up-arrow|alt=promoted}}'''1st'''\n|align=right|34||align=right|6||align=right|6||align=right|22\n|align=right|26||align=right|66||align=right|'''18'''\n|Fourth Round\n|Third Division\n|-\n|[[1994\u201395 Segunda Divis\u00e3o B|1994\u201395]]\n|scope=col rowspan=\"1\" |3\n|align=right bgcolor=\"#FFCCCC\"|{{down-arrow|alt=relegated}} '''16th'''\n|align=right|34||align=right|7||align=right|12||align=right|15\n|align=right|42||align=right|61||align=right|'''33'''\n|Fourth Round\n|Relegated\n|-\n|[[1995\u201396 Terceira Divis\u00e3o S\u00e9rie F|1995\u201396]]\n|scope=col rowspan=\"5\" |4\n|align=right |'''7th'''\n|align=right|34||align=right|13||align=right|9||align=right|12\n|align=right|39||align=right|35||align=right|'''48'''\n|Second Round\n| rowspan=\"4\" |Third Division - Series F\n|-\n|[[1996\u201397 Terceira Divis\u00e3o S\u00e9rie F|1996\u201397]]\n|align=right |'''5th'''\n|align=right|34||align=right|14||align=right|9||align=right|11\n|align=right|46||align=right|49||align=right|'''51'''\n|Third Round\n|-\n|[[1997\u201398 Terceira Divis\u00e3o S\u00e9rie F|1997\u201398]]\n|align=right |'''9th'''\n|align=right|34||align=right|14||align=right|6||align=right|14\n|align=right|49||align=right|40||align=right|'''48'''\n|Second Round\n|-\n|[[1998\u201399 Terceira Divis\u00e3o S\u00e9rie F|1998\u201399]]\n|align=right |'''4th'''\n|align=right|34||align=right|16||align=right|16||align=right|2\n|align=right|61||align=right|33||align=right|'''64'''\n|2nd Round\n|-\n|[[1999\u20132000 Terceira Divis\u00e3o S\u00e9rie F|1999\u201300]]\n|align=right right bgcolor=\"#DDFFDD\"|{{up-arrow|alt=promoted}}'''2nd'''\n|align=right|34||align=right|22||align=right|6||align=right|6\n|align=right|63||align=right|24||align=right|'''72'''\n|3rd Round\n|Promoted\n|-\n|[[2000\u201301 Segunda Divis\u00e3o B|2000\u201301]]\n|scope=col rowspan=\"1\" |3\n|align=right bgcolor=\"#FFCCCC\"|{{down-arrow|alt=relegated}}'''20th'''\n|align=right|38||align=right|5||align=right|9||align=right|24\n|align=right|31||align=right|66||align=right|'''24'''\n|Fourth Round\n|Relegated\n|-\n|[[2001\u201302 Terceira Divis\u00e3o S\u00e9rie F|2001\u201302]]\n|scope=col rowspan=\"2\" |4\n|align=right |'''14th'''\n|align=right|34||align=right|11||align=right|7||align=right|16\n|align=right|48||align=right|48||align=right|'''40'''\n|First round\n| rowspan=\"2\" |Third Division - Series F\n|-\n|[[2002\u201303 Terceira Divis\u00e3o S\u00e9rie F|2002\u201303]]\n|align=right |'''15th'''\n|align=right|34||align=right|10||align=right|9||align=right|15\n|align=right|36||align=right|46||align=right|'''39'''\n|First round\n|-\n|2003-2004  \n|Reg\n|'''1st'''\n| 26\n| -\n| -\n| -\n| -\n| -\n|'''61'''\n|Did not enter\n|\u00c9vora FA Champions, \u00c9vora FA Cup Winners\n|-|-|-|-\n|-\n|2004-05\n| rowspan=\"6\" |4\n|'''9th'''\n|34\n|13\n|7\n|14\n|52\n|49\n|'''46'''\n|Third Round\n| rowspan=\"6\" |Third Division - Series F\n|-\n|2005-06\n|'''3rd'''\n|32\n|17\n|9\n|6\n|48\n|27\n|'''60'''\n|Second Round\n|-\n|2006-07\n|'''11th'''\n|28\n|10\n|7\n|11\n|34\n|35\n|'''37'''\n|First Round\n|-\n|2007-08\n|'''9th'''\n|26\n|10\n|7\n|9\n|36\n|35\n|'''37'''\n|Second Round\n|-\n|2008-09\n|'''10th'''\n|26\n|7\n|8\n|11\n|21\n|31\n|'''29'''\n|First Round\n|-\n|2009-10\n|12th\n|22\n|2\n|9\n|11\n|19\n|31\n|'''15'''\n|Seco