ter}}|{{chset-cell|0E25|\u0e25|197}}\n|{{chset-color-letter}}|{{chset-cell|0E26|\u0e26|198}}\n|{{chset-color-letter}}|{{chset-cell|0E27|\u0e27|199}}\n|{{chset-color-letter}}|{{chset-cell|0E28|\u0e28|200}}\n|{{chset-color-letter}}|{{chset-cell|0E29|\u0e29|201}}\n|{{chset-color-letter}}|{{chset-cell|0E2A|\u0e2a|202}}\n|{{chset-color-letter}}|{{chset-cell|0E2B|\u0e2b|203}}\n|{{chset-color-letter}}|{{chset-cell|0E2C|\u0e2c|204}}\n|{{chset-color-letter}}|{{chset-cell|0E2D|\u0e2d|205}}\n|{{chset-color-letter}}|{{chset-cell|0E2E|\u0e2e|206}}\n|{{chset-color-letter}}|{{chset-cell|0E2F|\u0e2f|207}}\n|-\n!{{chset-left2|D_<br/>208}}\n|{{chset-color-letter}}|{{chset-cell|0E30|\u0e30|208}}\n|{{chset-color-misc}}|{{chset-cell|0E31|\u25cc\u0e31|209}}\n|{{chset-color-letter}}|{{chset-cell|0E32|\u0e32|210}}\n|{{chset-color-letter}}|{{chset-cell|0E33|\u0e33|211}}\n|{{chset-color-misc}}|{{chset-cell|0E34|\u25cc\u0e34|212}}\n|{{chset-color-misc}}|{{chset-cell|0E35|\u25cc\u0e35|213}}\n|{{chset-color-misc}}|{{chset-cell|0E36|\u25cc\u0e36|214}}\n|{{chset-color-misc}}|{{chset-cell|0E37|\u25cc\u0e37|215}}\n|{{chset-color-misc}}|{{chset-cell|0E38|\u25cc\u0e38|216}}\n|{{chset-color-misc}}|{{chset-cell|0E39|\u25cc\u0e39|217}}\n|{{chset-color-misc}}|{{chset-cell|0E3A|\u25cc\u0e3a|218}}\n|{{chset-color-undef}}|\n|{{chset-color-undef}}|\n|{{chset-color-undef}}|\n|{{chset-color-undef}}|\n|{{chset-color-graph}}|{{chset-cell|0E3F|[[Thai baht|\u0e3f]]|223}}\n|-\n!{{chset-left2|E_<br/>224}}\n|{{chset-color-letter}}|{{chset-cell|0E40|\u0e40|224}}\n|{{chset-color-letter}}|{{chset-cell|0E41|\u0e41|225}}\n|{{chset-color-letter}}|{{chset-cell|0E42|\u0e42|226}}\n|{{chset-color-letter}}|{{chset-cell|0E43|\u0e43|227}}\n|{{chset-color-letter}}|{{chset-cell|0E44|\u0e44|228}}\n|{{chset-color-letter}}|{{chset-cell|0E45|\u0e45|229}}\n|{{chset-color-letter}}|{{chset-cell|0E46|\u0e46|230}}\n|{{chset-color-misc}}|{{chset-cell|0E47|\u25cc\u0e47|231}}\n|{{chset-color-misc}}|{{chset-cell|0E48|\u25cc\u0e48|232}}\n|{{chse