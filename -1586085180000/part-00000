0131\n| match_JUV_JUV = null\n\n| match_RIP_VEN = 1\u20136\n| match_RIP_GLA = 4\u20130\n| match_RIP_JUV = 5\u20130\n| match_RIP_SPO = 3\u20130\n| match_RIP_UCJ = 7\u20131\n| match_RIP_VUL = 3\u20131\n| match_RIP_CRI = 3\u20130\n| match_RIP_CFB = 3\u20131\n| match_RIP_DRA = 8\u20131\n| match_RIP_RIP = null\n\n| match_SPO_RIP = 3\u20134\n| match_SPO_VEN = 0\u20130\n| match_SPO_GLA = 3\u20130\n| match_SPO_JUV = 1\u20131\n| match_SPO_UCJ = 4\u20131\n| match_SPO_VUL = 3\u20131\n| match_SPO_CRI = 2\u20131\n| match_SPO_CFB = 4\u20132\n| match_SPO_DRA = 3\u20132\n| match_SPO_SPO = null\n\n| match_UCJ_RIP = 2\u20135\n| match_UCJ_VEN = 1\u20130\n| match_UCJ_GLA = 1\u20131\n| match_UCJ_JUV = 6\u20132\n| match_UCJ_SPO = 4\u20131\n| match_UCJ_VUL = 3\u20132\n| match_UCJ_CRI = 0\u20131\n| match_UCJ_CFB = 3\u20131\n| match_UCJ_DRA = 3\u20130\n| match_UCJ_UCJ = null\n\n| match_VEN_RIP = 0\u20131\n| match_VEN_GLA = 1\u20131\n| match_VEN_JUV = 3\u20131\n| match_VEN_SPO = 4\u20131\n| match_VEN_UCJ = 9\u20133\n| match_VEN_VUL = 5\u20131\n| match_VEN_CRI = 5\u20130\n| match_VEN_CFB = 7\u20130\n| match_VEN_DRA = 7\u20132\n| match_VEN_VEN = null\n\n| match_VUL_RIP = 3\u20132\n| match_VUL_VEN = 0\u20132\n| match_VUL_GLA = 1\u20131\n| match_VUL_JUV = 1\u20130\n| match_VUL_SPO = 1\u20133\n| match_VUL_UCJ = 0\u20131\n| match_VUL_CRI = 2\u20131\n| match_VUL_CFB = 0\u20131\n| match_VUL_DRA = 2\u20131\n| match_VUL_VUL = null\n\n}}\n\n==Final==\n{{TwoLeg start}}\n{{TwoLegResult|[[FC Rapid Bucure\u015fti|Rapid Bucure\u015fti]]||0\u20134|'''[[Ripensia Timi\u015foara]]'''||0\u20132|0\u20132}}\n{{end}}\n\n==Top goalscorers==\n{| class=\"wikitable sortable\" style=\"text-align:center\"\n|-\n!Rank<ref>{{cite web |url=http://www.romaniansoccer.ro/menu_items/topscorers.shtml |title=Top Scorers |work=www.romaniansoccer.ro |accessdate=22 April 2018 |language=Romanian}}</ref>\n!Player\n!Club\n!Goals\n|-\n|rowspan=1 align=center|1\n|[[Ludovic Thierjung]]\n|[[Chinezul Timi\u0219oara]]\n|rowspan=1 align=center