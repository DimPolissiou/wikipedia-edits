}\n| {{convert|{{sigfig|90699/108.73|2}}|/km2|abbr=values|disp=table}}\n| style=\"text-align:center;\" | 48\n| style=\"text-align:center;\" | {{coord|13.9503|N|120.7334|E|name=Balayan|region:PH-BTG_type:city|format=dms}}\n|-\n! scope=\"row\" colspan=2 style=\"text-align:left;background-color:initial;\" | [[Balete, Batangas|Balete]]\n| style=\"text-align:center;\" | III\n| {{percent and number|22,661|2,694,335|disp=table|1|pad=yes}}\n| 20,214\n| {{PAGR|2015.5835|22661|2010.3315|20214}}\n| {{convert|25.00|km2|abbr=values|disp=table}}\n| {{convert|{{sigfig|22661/25.00|2}}|/km2|abbr=values|disp=table}}\n| style=\"text-align:center;\" | 13\n| style=\"text-align:center;\" | {{coord|14.0168|N|121.0998|E|name=Balete|region:PH-BTG_type:city|format=dms}}\n|-style=\"background-color:#CCFFCC;\"\n! scope=\"row\" style=\"text-align:left;background-color:#CCFFCC;border-right:0;\" | [[Batangas City]]\n| style=\"text-align:right;border-left:0;\" | \u2020\n| style=\"text-align:center;\" | V\n| {{percent and number|329,874|2,694,335|disp=table|1|pad=yes}}\n| 305,607\n| {{PAGR|2015.5835|329874|2010.3315|305607}}\n| {{convert|282.96|km2|abbr=values|disp=table}}\n| {{convert|{{sigfig|329874/282.96|2}}|/km2|abbr=values|disp=table}}\n| style=\"text-align:center;\" | 105\n| style=\"text-align:center;\" | {{coord|13.7561|N|121.0577|E|name=Batangas City|region:PH-BTG_type:city|format=dms}}\n|-\n! scope=\"row\" colspan=2 style=\"text-align:left;background-color:initial;\" | [[Bauan, Batangas|Bauan]]\n| style=\"text-align:center;\" | II\n| {{percent and number|91,297|2,694,335|disp=table|1|pad=yes}}\n| 81,351\n| {{PAGR|2015.5835|91297|2010.3315|81351}}\n| {{convert|53.31|km2|abbr=values|disp=table}}\n| {{convert|{{sigfig|91297/53.31|2}}|/km2|abbr=values|disp=table}}\n| style=\"text-align:center;\" | 40\n| style=\"text-align:center;\" | {{coord|13.7925|N|121.0076|E|name=Bauan|region:PH-BTG_type:city|format=dms}}\n|-\n! scope=\"row\" colspan=2 style=\"text-align:left;background-color:initial;\" |