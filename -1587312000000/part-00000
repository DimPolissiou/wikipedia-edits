e]]\n|align=\"center\" style=\"background:#afeeee;\"|1R\n|align=\"center\"|A\n|align=\"center\" style=\"background:#afeeee;\"|1R\n|align=\"center\" style=\"background:#afeeee;\"|1R\n|align=\"center\" style=\"background:#afeeee;\"|1R\n|align=\"center\" style=\"background:#afeeee;\"|2R\n|align=\"center\" style=\"background:yellow;\"|SF\n|align=\"center\" style=\"background:#afeeee;\"|2R\n|align=\"center\" style=\"background:#ffebcd;\"|QF\n|align=\"center\"|A\n|align=\"center\" style=\"background:#afeeee;\"|1R\n|align=\"center\" style=\"background:#EFEFEF;\" |0 / 9\n|align=\"center\" style=\"background:#EFEFEF;\"|7\u20139\n|-\n|style=\"background:#EFEFEF;\" |[[Hamburg Masters|Hamburg]]\n|align=\"center\"|A\n|align=\"center\" style=\"background:#ffebcd;\"|QF\n|align=\"center\" style=\"background:#ffebcd;\"|QF\n|align=\"center\" style=\"background:#D8BFD8;\"|F\n|align=\"center\" style=\"background:#afeeee;\"|2R\n|align=\"center\" style=\"background:#ffebcd;\"|QF\n|align=\"center\" style=\"background:yellow;\"|SF\n|align=\"center\" style=\"background:yellow;\"|SF\n|align=\"center\"|A\n|align=\"center\"|A\n|align=\"center\" style=\"background:#afeeee;\"|1R\n|align=\"center\" style=\"background:#EFEFEF;\" |0 / 8\n|align=\"center\" style=\"background:#EFEFEF;\"|12\u20138\n|-\n|style=\"background:#EFEFEF;\" |[[Canada Masters|Canada]]\n|align=\"center\"|A\n|align=\"center\" style=\"background:#00ff00;\"|'''W'''\n|align=\"center\" style=\"background:#00ff00;\"|'''W'''\n|align=\"center\" style=\"background:yellow;\"|SF\n|align=\"center\" style=\"background:#afeeee;\"|2R\n|align=\"center\" style=\"background:yellow;\"|SF\n|align=\"center\" style=\"background:#00ff00;\"|'''W'''\n|align=\"center\" style=\"background:#afeeee;\"|2R\n|align=\"center\" style=\"background:#afeeee;\"|2R\n|align=\"center\"|A\n|align=\"center\"|A\n|align=\"center\" style=\"background:#EFEFEF;\" |3 / 8\n|align=\"center\" style=\"background:#EFEFEF;\"|18\u20135\n|-\n|style=\"background:#EFEFEF;\" |[[Cincinna