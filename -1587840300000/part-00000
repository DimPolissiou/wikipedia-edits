 21757\n|-\n| [[Marinette County|Marinette]]  || 7806 || 7716 || 7033\n|-\n| [[Marquette County, Wisconsin|Marquette]]|| 2402 || 2361 || 1885\n|-\n| [[Menominee County, Wisconsin|Menominee]]|| 112 || 74 || 57\n|-\n| [[Milwaukee County|Milwaukee]]  || 203884 || 202919 || 168882\n|-\n| [[Monroe County, Wisconsin|Monroe]]|| 5639 || 5514 || 4410\n|-\n| [[Oconto County|Oconto]]  || 6457 || 6207 || 5438\n|-\n| [[Oneida County, Wisconsin|Oneida]]|| 8291 || 8237 || 7225\n|-\n| [[Outagamie County|Outagamie]]  || 40465 || 40276 || 35291\n|-\n| [[Ozaukee County|Ozaukee]]  || 29351 || 28968 || 26113\n|-\n| [[Pepin County|Pepin]]  || 936 || 865 || 359\n|-\n| [[Pierce County, Wisconsin|Pierce]]|| 6269 || 6220 || 5505\n|-\n| [[Polk County, Wisconsin|Polk]]|| 5679 || 5435 || 4884\n|-\n| [[Portage County, Wisconsin|Portage]]|| 14794 || 14731 || 13129\n|-\n| [[Price County|Price]]  || 2624 || 2586 || 1512\n|-\n| [[Racine County|Racine]]  || 44236 || 43968 || 38180\n|-\n| [[Richland County, Wisconsin|Richland]]|| 2802 || 2748 || 1793\n|-\n| [[Rock County, Wisconsin|Rock]]|| 29870 || 29532 || 26263\n|-\n| [[Rusk County, Wisconsin|Rusk]]|| 1854 || 1814 || 1143\n|-\n| [[Sauk County|Sauk]]  || 14566 || 14786 || 12218\n|-\n| [[Sawyer County|Sawyer]]  || 15020 || 14860 || 13378\n|-\n| [[Shawano County|Shawano]]  || 3091 || 3038 || 2730\n|-\n| [[Sheboygan County|Sheboygan]]  || 6318 || 6214 || 5745\n|-\n| [[St Croix County|St Croix]]  || 25783 || 25544 || 23601\n|-\n| [[Taylor County, Wisconsin|Taylor]]|| 2722 || 2702 || 2310\n|-\n| [[Trempealeau County|Trempealeau]]  || 3710 || 3425 || 2947\n|-\n| [[Vernon County, Wisconsin|Vernon]]|| 4349 || 4190 || 2385\n|-\n| [[Vilas County|Vilas]]  || 5419 || 5355 || 4972\n|-\n| [[Walworth County, Wisconsin|Walworth]]|| 22220 || 22058 || 19894\n|-\n| [[Washburn County|Washburn]]  || 2813 || 2773 || 2246\n|-\n| [[Washington County, Wisconsin|Washington]]|| 39966 || 39727 || 37737\n|-\n| [[Waukesha County|Waukesha]]  || 129969 || 128846 || 118620\n|-\n| [