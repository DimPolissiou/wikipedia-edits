y -->\n|win_CHB=11|draw_CHB=3  |loss_CHB=5  |gf_CHB=30 |ga_CHB=19  <!-- Chernomorets Burgas -->\n|win_DIM=3 |draw_DIM=3  |loss_DIM=13 |gf_DIM=13 |ga_DIM=49  <!-- Dimitrovgrad -->\n|win_GIG=10|draw_GIG=4  |loss_GIG=5  |gf_GIG=27 |ga_GIG=19  <!-- Gigant Saedinenie -->\n|win_HAS=6 |draw_HAS=4  |loss_HAS=9  |gf_HAS=23 |ga_HAS=29  <!-- Haskovo -->\n|win_KAR=3 |draw_KAR=5  |loss_KAR=11 |gf_KAR=17 |ga_KAR=33  <!-- Karnobat -->\n|win_LEK=5 |draw_LEK=6  |loss_LEK=5  |gf_LEK=18 |ga_LEK=22  <!-- Levski Karlovo -->\n|win_MAR=13|draw_MAR=2  |loss_MAR=4  |gf_MAR=43 |ga_MAR=15  <!-- Maritsa Plovdiv -->\n|win_MIR=7 |draw_MIR=9  |loss_MIR=3  |gf_MIR=26 |ga_MIR=13  <!-- Minyor Radnevo -->\n|win_NES=8 |draw_NES=2  |loss_NES=9  |gf_NES=22 |ga_NES=25  <!-- Nesebar -->\n|win_ROD=6 |draw_ROD=3  |loss_ROD=10 |gf_ROD=23 |ga_ROD=33  <!-- Rodopa Smolyan -->\n|win_RDK=9 |draw_RDK=3  |loss_RDK=7  |gf_RDK=38 |ga_RDK=30  <!-- Rozova Dolina -->\n|win_SOK=5 |draw_SOK=3  |loss_SOK=11 |gf_SOK=18 |ga_SOK=47  <!-- Sokol Markovo -->\n|win_SOZ=14|draw_SOZ=3  |loss_SOZ=2  |gf_SOZ=36 |ga_SOZ=8   <!-- Sozopol -->\n|win_SVL=9 |draw_SVL=3  |loss_SVL=7  |gf_SVL=27 |ga_SVL=19  <!-- Svilengrad -->\n|win_YAM=4 |draw_YAM=4  |loss_YAM=11 |gf_YAM=17 |ga_YAM=38  <!-- Yambol -->\n|win_ZAG=14|draw_ZAG=1  |loss_ZAG=4  |gf_ZAG=52 |ga_ZAG=15  <!-- Zagorets -->\n\n<!-- Head-to-head -->\n|hth_MIR_RKD=Minyor Radnevo ahead of Rozova Dolina on head-to-head record; Minyor R\u2013Rozova Dolina 1\u20131, \n|hth_MIR_SVL=Minyor Radnevo ahead of Svilengrad on head-to-head record; Minyor R-Svilengrad 2-0,\n|hth_MIR=MIR_RKD,MIR_SVL\n|hth_RKD=Rozova Dolina ahead of Svilengrad on head-to-head record; Svilengrad-Rozova Dolina 0-0\n\n|note_HAS=On January 21, 2020, FC Haskovo 1957 (Haskovo) declined further participation. His results are counted so far, and his opponents on the program until the end are awarded official victories with 3: 0.<ref>{{cite web | title = \u042e\u0433\u043e\u0438\u0437\u0442\u043e\u0447\u043d\u0430\u0442\u0430 \u