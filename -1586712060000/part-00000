op|pos=DB}}\n{{American football roster/Player|num=13|class=JR|first=Jonathan|last=Ledbetter|pos=DL|link=y}}\n{{American football roster/Player|num=14|class=SR|first=Malkom|last=Parrish|pos=DB}}\n{{American football roster/Player|num=15|class=JR|first=D'Andre|last=Walker|pos=LB|link=y}}\n{{American football roster/Player|num=17|class=SR|first=Davin|last=Bellamy|pos=LB|link=y}}\n{{American football roster/Player|num=18|class=JR|first=Deandre|last=Baker|pos=DB|link=y}}\n{{American football roster/Player|num=19|class=JR|first=Jarvis|last=Wilson|pos=DB}}\n{{American football roster/Player|num=20|class=SO|first=J. R.|last=Reed|dab=J. R. Reed (American football, born 1995)|link=y||pos=DB|rs=y}}\n{{American football roster/Player|num=22|class=FR|first=Nate|last=McBride|pos=LB}}\n{{American football roster/Player|num=23|class=FR|first=Mark|last=Webb|pos=DB}}\n{{American football roster/Player|num=24|class=SR|first=Dominick|last=Sanders|pos=DB}}\n{{American football roster/Player|num=25|class=SO|first=Jaleel|last=Laguins|pos=LB}}\n{{American football roster/Player|num=26|class=SO|first=Tyrique|last=McGhee|pos=DB}}\n{{American football roster/Player|num=28|class=SO|first=KJ|last=Smith|pos=DB|rs=y}}\n{{American football roster/Player|num=29|class=SR|first=Tim|last=Hill|pos=DB}}\n{{American football roster/Player|num=30|class=SO|first=Tae|last=Crowder|pos=LB|rs=y}}\n{{American football roster/Player|num=31|class=FR|first=William|last=Poole|pos=DB}}\n{{American football roster/Player|num=32|class=FR|first=Monty|last=Rice|pos=LB}}\n{{American football roster/Player|num=33|class=FR|first=Robert|last=Beal Jr.|pos=LB}}\n{{American football roster/Player|num=35|class=SR|first=Aaron|last=Davis|pos=DB}}\n{{American football roster/Player|num=36|class=FR|first=Latavious|last=Brini|pos=DB}}\n{{American football roster/Player|num=37|class=FR|first=Jordon|last=McKinney|pos=DB|rs=y}}\n{{American football roster/Player|num=38|class=FR|first=Michael|last=Keene|pos=LB|rs=y}}\n{{American footba