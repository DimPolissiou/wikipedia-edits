020\n |xAxisAngle=-45\n |x= 23 Jan, 24 Jan, 25 Jan, 26 Jan, 27 Jan, 28 Jan, 29 Jan, 30 Jan, 31 Jan,\n |yAxisTitle=No. of new cases\n |yAxisMax = 20\n |y=2,3,0,3,0,0,2,2,1\n |yGrid= |xGrid=\n}}\n\n{{Graph:Chart\n |width=400\n |type=rect\n |colors=#F46D43\n |showValues=offset:1\n\n |xAxisTitle=February 2020\n |xAxisAngle=-45\n |x= 1 Feb, 2 Feb, 3 Feb, 4 Feb, 5 Feb, 6 Feb, 7 Feb, 8 Feb, 9 Feb, 10 Feb,\n     11 Feb, 12 Feb, 13 Feb, 14 Feb, 15 Feb, 16 Feb, 17 Feb, 18 Feb, 19 Feb, 20 Feb,\n     21 Feb, 22 Feb, 23 Feb, 24 Feb, 25 Feb, 26 Feb, 27 Feb, 28 Feb, 29 Feb,\n |yAxisTitle=No. of new cases\n |yAxisMax = 20\n |y=1,1,0,3,3,3,2,0,10,6,7,1,3,3,0,1,3,2,3,4,0,1,5,6,4,6,2,1,1,\n |yGrid= |xGrid=\n}}\n\n{{Graph:Chart\n |width=400\n |type=rect\n |colors=#F46D43\n |showValues=offset:1\n\n |xAxisTitle=March 2020\n |xAxisAngle=-45\n |x= 1 Mar, 2 Mar, 3 Mar, 4 Mar, 5 Mar, 6 Mar, 7 Mar, 8 Mar, 9 Mar, 10 Mar,\n     11 Mar, 12 Mar, 13 Mar, 14 Mar, 15 Mar, 16 Mar, 17 Mar, 18 Mar, 19 Mar, 20 Mar,\n     21 Mar, 22 Mar, 23 Mar, 24 Mar, 25 Mar, 26 Mar, 27 Mar, 28 Mar, 29 Mar, 30 Mar, 31 Mar\n |yAxisTitle=No. of new cases\n |yAxisMax = 100\n |y=5,1,0,4,0,3,2,5,1,5,9,2,6,4,7,9,10,25,16,48,17,44,39,30,24,43,65,64,59,41,32\n |yGrid= |xGrid=\n}}\n\n{{Graph:Chart\n |width=400\n |type=rect\n |colors=#F46D43\n |showValues=offset:1\n\n |xAxisTitle=April 2020\n |xAxisAngle=-45\n |x= 1 Apr, 2 Apr, 3 Apr, 4 Apr, 5 Apr, 6 Apr, 7 Apr, 8 Apr, 9 Apr, 10 Apr,\n     11 Apr, 12 Apr, 13 Apr, 14 Apr, 15 Apr, 16 Apr, 17 Apr, 18 Apr, 19 Apr, 20 Apr,\n     21 Apr, 22 Apr, 23 Apr, 24 Apr, 25 Apr, 26 Apr, 27 Apr, 28 Apr, 29 Apr, 30 Apr\n |yAxisTitle=No. of new cases\n |yAxisMax = 100\n |y=51,39,43,17,,,,,,,,,,,,,,,,,,,,,,,,,,\n |yGrid= |xGrid=\n}}\n\n{{Graph:Chart\n |width=400\n |type=rect\n |colors=#f44343\n |showValues=offset:1\n\n |xAxisTitle=Date\n |xAxisAngle=-45\n |x= 1 Mar, 2 Mar, 3 Mar, 4 Mar, 5 Mar, 6 Mar, 7 Mar, 8 Mar, 9 Mar, 10 Mar,\n     11 Mar, 12 Mar, 13 Mar, 14 Mar, 15 Mar, 16 Mar, 17 Mar, 18 Mar, 