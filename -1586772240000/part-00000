e}}<sup>5</sup> \n|{{PIE|i}}\n|rowspan=3|{{PIE|e}}\n|{{PIE|\u0113}}\n|{{PIE|ei}}\n|{{PIE|\u012b}}\n|{{PIE|\u012ba}}; {{PIE|\u0113}}<sup>6</sup> \n|- align=center\n!{{PIE|*oi}}\n!{{PIE|*oi}}, {{PIE|*h\u2083ei}}\n|rowspan=2|{{PIE|\u011b}}\n|rowspan=2|{{PIE|ai}}; {{PIE|ie}}<sup>5</sup> \n|rowspan=2|{{PIE|e}}\n|rowspan=2|{{PIE|e, ai}}\n|rowspan=2|\u0113, ai\n|{{PIE|oi}}\n|{{PIE|\u016b}}\n|{{PIE|oe}}\n|rowspan=2|{{PIE|\u00e1i}}\n|rowspan=2|{{PIE|\u0101}}\n|rowspan=2|{{PIE|\u01e3}}\n|- align=center\n!{{PIE|*ai}}\n!({{PIE|*ai}}<sup>3</sup>), {{PIE|*h\u2082ei}}\n|{{PIE|ai}}\n|{{PIE|ae}}\n|{{PIE|ae}}\n|- align=center\n!{{PIE|*\u0113i}}\n!{{PIE|*\u0113i}}\n|rowspan=3|{{PIE|ai}}; {{PIE|\u0101}}<sup>8</sup>\n|rowspan=3|{{PIE|\u0101i}}; {{PIE|\u0101(i)}}<sup>8</sup>\n|{{PIE|i}}\n| \n|\n|{{PIE|i}}\n|\n|\u0113\n|{{PIE|\u0113i}}\n|{{PIE|\u012b}}?\n| \n|{{PIE|ei}} {{IPA|[i\u02d0]}}\n|colspan=2|{{PIE|\u012b}}\n|- align=center\n!{{PIE|*\u014di}}\n!{{PIE|*\u014di (*oei)}}\n|{{PIE|y}}; {{PIE|u}}<sup>8</sup>\n|{{PIE|ai}}; {{PIE|ui}}<sup>8</sup>\n|\n|rowspan=2|{{PIE|e, ai}}\n|\n|{{PIE|ai}}\n|{{PIE|\u014di}}\n|{{PIE|\u014d}}\n|{{PIE|u}}<sup>8</sup>\n|rowspan=2|{{PIE|\u00e1i}}\n|rowspan=2|{{PIE|\u0101}}\n|rowspan=2|{{PIE|\u01e3}}\n|- align=center\n!{{PIE|*\u0101i}}\n!{{PIE|*eh\u2082ei}}\n|{{PIE|\u011b}}\n| \n|\n|\n|ai\n|{{PIE|\u0101i > \u0113i}}<sup>15</sup>\n|{{PIE|ae}}\n|- align=center\n!{{PIE|*u}}\n!{{PIE|*u}}\n| colspan=\"2\"|{{PIE|u}}\n|{{PIE|\u044a}}\n|{{PIE|u}}\n|rowspan=2|{{PIE|u}}\n|{{PIE|u}}; {{PIE|y}}<sup>11</sup>\n|{{PIE|\u00e4}}\n|rowspan=7|{{PIE|u}}\n|{{PIE|u}}\n|{{PIE|u}}\n|{{PIE|u}}; {{PIE|o}}<sup>1</sup>\n|{{PIE|u}}; {{PIE|a\u00fa}} {{IPA|[\u0254]}}<sup>2</sup>\n|{{PIE|u}}; {{PIE|o}}<sup>23</sup>\n|{{PIE|y}}\n|- align=center\n!rowspan=3|{{PIE|*\u016b}}\n!{{PIE|*uh\u2081}}\n|rowspan=3 colspan=\"2\"|{{PIE|\u016b}}\n|rowspan=3|{{PIE|y}}\n|rowspan=3|{{PIE|\u016b}}\n|rowspan=3|{{PIE|y}}; {{PIE|i}}<sup>8</sup>\n|{{PIE|u}}\n|{{PIE|\u016b}}\n|rowspan=3 colspan=\"4\"|{{PIE|\u016b}}\