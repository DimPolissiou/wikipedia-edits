hes|num=83|pos=WR|class=SO|rs=y}}\n{{American football roster/Player|first=Jacob|last=Bernard|num=86|pos=WR|class=FR}}\n{{American football roster/Player|first=Pearse|last=Migl|num=86|pos=TE|class=FR|rs=y}}\n{{American football roster/Player|first=Neal|last=Johnson|num=87|pos=TE|class=FR}}\n{{American football roster/Player|first=Johnny|last=Lumpkin|num=88|pos=TE|class=JR|rs=y}}\n{{American football roster/Player|first=Hunter|last=Bergeron|num=89|pos=TE|class=SO|rs=y}}\n|defensive_players=\n{{American football roster/Player|first=Lorenzo|last=McCaskill|num=2|pos=LB|class=SO|rs=y}}\n{{American football roster/Player|first=Joe|last=Dillon|num=3|pos=LB|class=JR|rs=y}}\n{{American football roster/Player|first=Zi'Yon|last=Hill|num=4|pos=DT|class=SO|rs=y}}\n{{American football roster/Player|first=Kris|last=Moncrief|num=6|pos=LB|class=SO}}\n{{American football roster/Player|first=Ferrod|last=Gardner|num=7|pos=LB|class=JR|rs=y}}\n{{American football roster/Player|first=Kendall|last=Wilkerson|num=8|pos=DL|class=FR}}\n{{American football roster/Player|first=Kamar|last=Greenhouse|num=9|pos=DB|class=SR|rs=y}}\n{{American football roster/Player|first=Bobby|last=Holmes|num=10|pos=CB|class=FR}}\n{{American football roster/Player|first=Michael|last=Jacquet III|num=11|pos=DB|class=SR|rs=y}}\n{{American football roster/Player|first=Patrick|last=Mensah|num=13|pos=DB|class=FR|rs=y}}\n{{American football roster/Player|first=Bennie|last=Higgins|num=15|pos=DE|class=SR}}\n{{American football roster/Player|first=Percy|last=Butler|num=16|pos=DB|class=SO}}\n{{American football roster/Player|first=Chauncey|last=Manac|num=17|pos=LB|class=JR|rs=y}}\n{{American football roster/Player|first=Asjlin|last=Washington|num=18|pos=DB|class=SO}}\n{{American football roster/Player|first=Eric|last=Garror|num=19|pos=DB|class=SO}}\n{{American football roster/Player|first=Mekhi|last=Garner|num=20|pos=S|class=SO}}\n{{American football roster/Player|first=Amir|last=McDaniel|num=21|pos=CB|class=FR}}\n{{American f