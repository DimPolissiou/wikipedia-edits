center\n| width=\"20\" style=\"background:#f0d0d0;\" | \uff3c || width=\"20\" | &nbsp; || width=\"20\" | &nbsp; || width=\"20\" | &nbsp; || width=\"20\" | &nbsp; || width=\"20\" | &nbsp; || width=\"20\" style=\"background:#f0d0d0;\" | \uff0f\n|- align=center\n| &nbsp; || style=\"background:#f0d0d0;\" | \uff3c || &nbsp; || &nbsp; || &nbsp; || style=\"background:#f0d0d0;\" | \uff0f || &nbsp;\n|- align=center\n| &nbsp; || &nbsp; || style=\"background:#f0d0d0; color:orange;\" | '''\u203b''' || style=\"color:orange\" | '''\u203b''' || style=\"background:#f0d0d0; color:orange;\" | '''\u203b''' || &nbsp; || &nbsp;\n|- align=center\n| style=\"background:#f0d0d0;\" | \u2500 || style=\"background:#f0d0d0;\" | \u2500 || style=\"background:#f0d0d0; color:orange;\" | '''\u203b''' || style=\"background:#e0e0e0;\" | \u706b || style=\"background:#f0d0d0; color:orange;\" | '''\u203b''' || style=\"background:#f0d0d0;\" | \u2500 || style=\"background:#f0d0d0;\" | \u2500\n|- align=center\n| &nbsp; || &nbsp; || style=\"background:#f0d0d0; color:orange;\" | '''\u203b''' || style=\"color:orange\" | '''\u203b''' || style=\"background:#f0d0d0; color:orange;\" | '''\u203b''' || &nbsp; || &nbsp;\n|- align=center\n| &nbsp; || style=\"background:#f0d0d0;\" | \uff0f || &nbsp; || &nbsp; || &nbsp; || style=\"background:#f0d0d0;\" | \uff3c || &nbsp;\n|- align=center\n| style=\"background:#f0d0d0;\" | \uff0f || &nbsp; || &nbsp; || &nbsp; || &nbsp; || &nbsp; || style=\"background:#f0d0d0;\" | \uff3c\n|}\n{| border=\"1\" class=\"wikitable\"\n|- align=center\n| style=\"background:#d0d0f0;\" width=\"20\" | \u25cf || style=\"background:#d0d0f0;\" width=\"20\" | \u25cf || style=\"background:#d0d0f0;\" width=\"20\" | \u25cf || style=\"background:#d0d0f0;\" width=\"20\" | \u25cf || style=\"background:#d0d0f0;\" width=\"20\" | \u25cf || style=\"background:#d0d0f0;\" width=\"20\" | \u25cf || style=\"background:#d0d0f0;\" width=\"20\" | \u25cf\n|- align=center\n| style=\"background:#d0d0f0;\" | \u25cf || style