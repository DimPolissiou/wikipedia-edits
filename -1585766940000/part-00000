]]\n|match_MID_ARS=0\u20131\n|match_MID_AST=3\u20130\n|match_MID_BIR=2\u20131\n|match_MID_BLB=1\u20130\n|match_MID_BOL=1\u20131\n|match_MID_CHA=2\u20132\n|match_MID_CHE=0\u20131\n|match_MID_CRY=2\u20131\n|match_MID_EVE=1\u20131\n|match_MID_FUL=1\u20131\n|match_MID_LIV=2\u20130\n|match_MID_MCI=3\u20132\n|match_MID_MUN=0\u20132\n|match_MID_NEW=[[Tyne\u2013Tees derby|2\u20132]]\n|match_MID_NWC=2\u20130\n|match_MID_POR=1\u20131\n|match_MID_SOU=1\u20133\n|match_MID_TOT=1\u20130\n|match_MID_WBA=4\u20130\n\n|name_NEW=[[Newcastle United F.C.|Newcastle United]]\n|match_NEW_ARS=0\u20131\n|match_NEW_AST=0\u20133\n|match_NEW_BIR=2\u20131\n|match_NEW_BLB=3\u20130\n|match_NEW_BOL=2\u20131\n|match_NEW_CHA=1\u20131\n|match_NEW_CHE=1\u20131\n|match_NEW_CRY=0\u20130\n|match_NEW_EVE=1\u20131\n|match_NEW_FUL=1\u20134\n|match_NEW_LIV=1\u20130\n|match_NEW_MCI=4\u20133\n|match_NEW_MUN=1\u20133\n|match_NEW_MID=[[Tyne\u2013Tees derby|0\u20130]]\n|match_NEW_NWC=2\u20132\n|match_NEW_POR=1\u20131\n|match_NEW_SOU=2\u20131\n|match_NEW_TOT=0\u20131\n|match_NEW_WBA=3\u20131\n\n|name_NWC=[[Norwich City F.C.|Norwich City]]\n|match_NWC_ARS=1\u20134\n|match_NWC_AST=0\u20130\n|match_NWC_BIR=1\u20130\n|match_NWC_BLB=1\u20131\n|match_NWC_BOL=3\u20132\n|match_NWC_CHA=1\u20130\n|match_NWC_CHE=1\u20133\n|match_NWC_CRY=1\u20131\n|match_NWC_EVE=2\u20133\n|match_NWC_FUL=0\u20131\n|match_NWC_LIV=1\u20132\n|match_NWC_MCI=2\u20133\n|match_NWC_MUN=2\u20130\n|match_NWC_MID=4\u20134\n|match_NWC_NEW=2\u20131\n|match_NWC_POR=2\u20132\n|match_NWC_SOU=2\u20131\n|match_NWC_TOT=0\u20132\n|match_NWC_WBA=3\u20132\n\n|name_POR=[[Portsmouth F.C.|Portsmouth]]\n|match_POR_ARS=0\u20131\n|match_POR_AST=1\u20132\n|match_POR_BIR=1\u20131\n|match_POR_BLB=0\u20131\n|match_POR_BOL=1\u20131\n|match_POR_CHA=4\u20132\n|match_POR_CHE=0\u20132\n|match_POR_CRY=3\u20131\n|match_POR_EVE=0\u20131\n|match_POR_FUL=4\u20133\n|match_POR_LIV=1\u20132\n|match_POR_MCI=1\u20133\n|match_POR_MUN=2\u20130\n|match_POR_MID=2\u20131\n|match_POR_NEW=1\u20131