b|ps=1|P1603|qid=Q90701409|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q90701409|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q90701409|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}}}\n|{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q90701409|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q90701409|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|-\n!scope=\"row\" |[[Panchmahal district|Panchmahal]]\n|{{stringsplit | txt={{wdib|ps=1|P1603|qid=Q89234357|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{#expr: {{stringsplit | txt={{wdib|ps=1|P1603|qid=Q89234357|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q89234357|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q89234357|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}}}\n|{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q89234357|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q89234357|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|-\n!scope=\"row\" |[[Patan district|Patan]]\n|{{stringsplit | txt={{wdib|ps=1|P1603|qid=Q89412600|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{#expr: {{stringsplit | txt={{wdib|ps=1|P1603|qid=Q89412600|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q89412600|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q89412600|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}}}\n|{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q89412600|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q89412600|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|-\n! scope=\"row\" |[[Porbandar district|Porbandar]]\n|{{stringsplit | txt={{wdib|ps=1|P1603|qid=Q89065501|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{#expr: {{stringsplit | txt={{wdib|ps=1|P1603|qid=Q89065501|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q89065501|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q89065501|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}}}\n|{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q89065501|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q89065501|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|-\n!scope=\"row\" |[[Rajkot district|Rajkot]]\n|{{stringsplit | txt={{wdib|ps=1|P1603|qid=Q88097388|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}<!---DO NOT CHANGE THIS AUTOUPDATED SENTENCE. IT IS POWERED BY WIKIDATA. IT WILL BE UPDATED SOON.--->\n|{{#expr: {{stringsplit | txt={{wdib|ps=1|P1603|qid=Q88097388|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q88097388|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q88097388|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}}}\n|{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q88097388|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q88097388|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|-\n! scope=\"row\" |[[Sabarkantha district|Sabarkantha]]\n|{{stringsplit | txt={{wdib|ps=1|P1603|qid=Q89655353|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{#expr: {{stringsplit | txt={{wdib|ps=1|P1603|qid=Q89655353|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q89655353|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}-{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q89655353|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}}}\n|{{stringsplit | txt={{wdib|ps=1|P1120|qid=Q89655353|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|{{stringsplit | txt={{wdib|ps=1|P8010|qid=Q89655353|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}\n|-\n!scope=\"row\" |[[Surat district|Surat]]\n|{{stringsplit | txt={{wdib|ps=1|P1603|qid=Q88097394|sep=\"--\"|df=dmy}} |sep=\"--\" |idx=-1}}<!---DO NOT CHANGE THIS AUTOUPDATED SENTENCE. IT IS POWERED BY WIKIDATA. IT WILL BE UPDATED SOON.--->\n|{{#expr: {{stringsplit |