current Red Wings player''\n{{col-start}}\n{{col-break}}\n{|class=\"wikitable sortable plainrowheaders\" border=\"1\" cellpadding=\"4\" cellspacing=\"0\" style=\"margin: 1em 1em 1em 0; background: #f9f9f9; border: 1px #aaa solid; border-collapse: collapse; text-align: center\"\n|+ style=\"background:#FFFFFF; border-top:#CE1126 5px solid; border-bottom:#CE1126 5px solid;\"|Regular season\n|-\n!scope=\"col\" |Player\n!scope=\"col\" |GP\n!scope=\"col\" |W\n!scope=\"col\" |L\n!scope=\"col\" |T\n!scope=\"col\" |OT\n!scope=\"col\" |SO\n!scope=\"col\" |GAA\n|-\n!scope=\"row\" |{{sortname|Terry|Sawchuk}}\n| 734 || 351 || 243 || 132 || {{sortdash}} || 85 || 2.44\n|-\n!scope=\"row\" |{{sortname|Chris|Osgood}}\n| 565 || 317 || 149 || 46 || {{sort|036|36}} || 39 || 2.49\n|- style=\"background:#cfc\"\n!scope=\"row\" |{{sortname|Jimmy|Howard}}*\n| 461 || 221 || 151 || {{sortdash}} || {{sort|063|63}} || 24 || 2.49\n|-\n!scope=\"row\" |{{sortname|Harry|Lumley|dab=ice hockey}}\n| 324 || 163 || 105 || 56 || {{sortdash}} || 26 || 2.75\n|-\n!scope=\"row\" |{{sortname|Roger|Crozier}}\n| 313 || 131 || 121 || 41 || {{sortdash}} || 20 || 2.93\n|-\n!scope=\"row\" |{{sortname|Tim|Cheveldae}}\n| 264 || 128 || 93 || 30 || {{sort|007|7}} || 9 || 3.40\n|-\n!scope=\"row\" |{{sortname|Greg|Stefan}}\n| 299 || 115 || 127 || 30 || {{sort|001|1}} || 5 || 3.92\n|-\n!scope=\"row\" |{{sortname|Dominik|Hasek|Dominik Ha\u0161ek}}\n| 176 || 114 || 39 || 10 || {{sort|012|12}} || 20 || 2.13\n|-\n!scope=\"row\" |{{sortname|Manny|Legace}}\n| 180 || 112 || 34 || 16 || {{sort|006|6}} || 13 || 2.18\n|-\n!scope=\"row\" |{{sortname|Jim|Rutherford}}\n| 314 || 97 || 165 || 43 || {{sortdash}} || 10 || 3.69\n|}\n\n{{col-break}}\n{|class=\"wikitable sortable plainrowheaders\" border=\"1\" cellpadding=\"4\" cellspacing=\"0\" style=\"margin: 1em 1em 1em 0; background: #f9f9f9; border: 1px #aaa solid; border-collapse: collapse; text-align: center\"\n|+ style=\"background:#FFFFFF; border-top:#CE1126 5px solid; border-bottom: