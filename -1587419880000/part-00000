''' || 0.73\n|-\n!scope=\"row\" |{{sortname|Henrik|Zetterberg}}\n| C || 1,082 || 337 || 623 || '''960''' || 0.89\n|-\n!scope=\"row\" |{{Sortname|Sergei|Fedorov}}\n| C || 908 || 400 || 554 || '''954''' || 1.05\n|-\n!scope=\"row\" |{{sortname|Pavel|Datsyuk}}\n| C || 953 || 314 || 604 || '''918''' || 0.96\n|-\n!scope=\"row\" |{{sortname|Norm|Ullman}}\n| C || 875 || 324 || 434 || '''758''' || 0.87\n|-\n!scope=\"row\" |{{sortname|Ted|Lindsay}}\n| LW || 862 || 335 || 393 || '''728''' || 0.84\n|-\n!scope=\"row\" |{{sortname|Brendan|Shanahan}}\n| LW || 716 || 309 || 324 || '''633''' || 0.88\n|}\n\n{{col-break}}\n{|class=\"wikitable sortable plainrowheaders\" border=\"1\" cellpadding=\"4\" cellspacing=\"0\" style=\"margin: 1em 1em 1em 0; background: #f9f9f9; border: 1px #aaa solid; border-collapse: collapse; text-align: center\"\n|+ style=\"background:#FFFFFF; border-top:#CE1126 5px solid; border-bottom:#CE1126 5px solid;\"|Postseason\n|-align=\"center\"\n!scope=\"col\" |Player\n!scope=\"col\" class=\"unsortable\" |Pos\n!scope=\"col\" |GP\n!scope=\"col\" |G\n!scope=\"col\" |A\n!scope=\"col\" |Pts\n!scope=\"col\" |P/G\n|-\n!scope=\"row\" |{{sortname|Steve|Yzerman}}\n| C || 196 || 70 || 115 || '''185''' || 0.94\n|-\n!scope=\"row\" |{{sortname|Nicklas|Lidstrom|Nicklas Lidstr\u00f6m}}\n| D || 263 || 54 || 129 || '''183''' || 0.70\n|-\n!scope=\"row\" |{{Sortname|Sergei|Fedorov}}\n| C || 162 || 50 || 113 || '''163''' || 1.01\n|-\n!scope=\"row\" |{{sortname|Gordie|Howe}}\n| RW || 154 || 67 || 91 || '''158''' || 1.03\n|- \n!scope=\"row\" | {{sortname|Henrik|Zetterberg}}\n| C || 137 || 57 || 63 || '''120''' || 0.88\n|-\n!scope=\"row\" |{{sortname|Pavel|Datsyuk}}\n| C || 157 || 42 || 71 || '''113''' || 0.72\n|-\n!scope=\"row\" |{{sortname|Alex|Delvecchio}}\n| C || 121 || 35 || 69 || '''104''' || 0.86\n|-\n!scope=\"row\" |{{sortname|Tomas|Holmstrom|Tomas Holmstr\u00f6m}}\n| LW || 180 || 46 || 51 || '''97''' || 0.54\n|-\n!scope=\"row\" |{{sortname|Ted|Lindsay}}\n| LW || 123 || 44 || 44 