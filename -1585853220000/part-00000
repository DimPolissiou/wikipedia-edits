ound==\n\n===Matches===\n{{Football box collapsible\n|date=2 December 1876\n|team1=Lancefield\n|score=2\u20130\n|team2='''Hamilton'''\n|goals1=\n|goals2=\n|stadium=\n|location=Glasgow\n|attendance=\n|referee=\n}}\n{{Football box collapsible\n|date=2 December 1876\n|team1=Lennox\n|score=4\u20130\n|team2='''Swifts'''\n|goals1=\n|goals2=\n|stadium=\n|location=Lennoxtown\n|attendance=\n|referee=\n}}\n{{Football box collapsible\n|date=2 December 1876\n|team1=Queen's Park\n|score=4\u20130\n|team2='''Northern'''\n|goals1=\n|goals2=\n|stadium=[[Hampden Park (1873\u201383)|Hampden Park]]\n|location=Glasgow\n|attendance=\n|referee=\n|report=<ref>{{cite web |url=http://www.fitbastats.com/queenspark/game.php?gameid=75010 |title=Queen's Park 4 - 0 Northern,  Scottish Cup (02/12/1876) |publisher=Fitbastats.com |accessdate=16 April 2019}}</ref>\n}}\n{{Football box collapsible\n|date=2 December 1876\n|team1=Vale of Leven\n|score=4\u20130\n|team2='''Busby'''\n|goals1=\n|goals2=\n|stadium=North Street Park\n|location=Alexandria\n|attendance=\n|referee=\n|report=<ref>{{cite web |url=http://www.fitbastats.com/valeofleven/game.php?gameid=88237 |title=Vale of Leven 4 - 0 Busby,  Scottish Cup (02/12/1876) |publisher=Fitbastats.com |accessdate=16 April 2019}}</ref>\n}}\n{{Football box collapsible\n|date=9 December 1876\n|team1='''Ayr Thistle'''\n|score=1\u20131\n|team2='''Partick'''\n|goals1=\n|goals2=\n|stadium=\n|location=Ayr\n|attendance=\n|referee=\n}}\n{{Football box collapsible\n|date=9 December 1876\n|team1=Rangers\n|score=3\u20130\n|team2='''Mauchline'''\n|goals1=*[[James Watson (Rangers footballer)|Watson]] {{goal}}\n*Marshall {{goal}}\n*[[Peter Campbell (Rangers footballer)|Campbell]] {{goal}}\n|goals2=\n|stadium=[[Kinning Park]]\n|location=Glasgow\n|attendance=\n|referee=\n|report=<ref>{{cite web |url=http://www.fitbastats.com/rangers/game.php?gameid=80441 |title=Rangers 3 - 0 Mauchline,  Scottish Cup (09/12/1876) |publisher=Fitbastats.com |accessdate=16 April 2019}}</ref>\n}}\n