land|28|chartid=1987|year=2016|rowheader=true|accessdate=May 30, 2016}}\n|-\n{{singlechart|Scotland|66|date=20151120|rowheader=true|accessdate=December 19, 2015}}\n|-\n{{singlechart|Slovakia|32|year=2016|week=03|rowheader=true|accessdate=January 11, 2016}}\n|-\n{{singlechart|Slovakdigital|10|year=2015|week=44|rowheader=true|accessdate=December 19, 2015|refname=\"Slovakia\"}}\n|-\n{{singlechart|Spain|49|artist=Selena Gomez|song=Same Old Love|rowheader=true|accessdate=December 19, 2015}}\n|-\n{{singlechart|Sweden|37|artist=Selena Gomez|song=Same Old Love|rowheader=true|accessdate=February 6, 2016}}\n|-\n{{singlechart|Switzerland|47|artist=Selena Gomez|song=Same Old Love|rowheader=true|accessdate=February 1, 2016}}\n|-\n{{singlechart|UK|81|date=20151120|rowheader=true|accessdate=December 19, 2015}}\n|-\n{{singlechart|Billboardhot100|5|artist=Selena Gomez|rowheader=true|accessdate=January 19, 2016|refname=\"hot100\"}}\n|-\n{{singlechart|Billboardadultcontemporary|17|artist=Selena Gomez|rowheader=true|accessdate=April 30, 2016}}\n|-\n{{singlechart|Billboardadultpopsongs|7|artist=Selena Gomez|song=Same Old Love|rowheader=true|accessdate=February 23, 2016}}\n|-\n{{singlechart|Billboarddanceairplay|4|artist=Selena Gomez|song=Same Old Love|rowheader=true|accessdate=December 19, 2015|refname=\"US Dance\"}}\n|-\n{{singlechart|Billboardlatinairplay|48|artist=Selena Gomez|rowheader=true|accessdate=January 12, 2016}}\n|-\n{{singlechart|Billboardlatinpopsongs|31|artist=Selena Gomez|rowheader=true|accessdate=December 23, 2015}}\n|-\n{{singlechart|Billboardpopsongs|1|artist=Selena Gomez|song=Same Old Love||rowheader=true|accessdate=January 5, 2016|refname=\"US Pop\"}}\n|-\n{{singlechart|Billboardrhythmic|6|artist=Selena Gomez|song=Same Old Love|rowheader=true|accessdate=January 20, 2016}}\n|-\n!scope=\"row\"|Venezuela ([[Record Report]])<ref>{{cite web|url=http://www.recordreport.com.ve/publico/top100/|title=Top 100|language=Spanish|publisher=[[Record Report]]|access-date=December 1