ad Numbers]\n|date=21 November 2018\n\n\n|list1a={{Rls player|no=1|name=[[Lachlan Coote]]|pos=FB|pos2=SO}}   \n|list1b={{Rls player|no=2|name=[[Thomas Makinson|Tom Makinson]]|pos=WG|pos2=FB}}\n|list1c={{Rls player|no=3|name=[[Kevin Naiqama]]|pos=CE|pos2=WG|pos3=FB}}\n|list1d={{Rls player|no=4|name=[[Mark Percival]]|pos=CE}}\n|list1e={{Rls player|no=5|name=[[Regan Grace]]|pos=WG}}\n|list1f={{Rls player|no=6|name=[[Jonny Lomax]]|pos=FB|pos2=SH|pos3=SO|pos4=WG}}\n|list1g={{Rls player|no=7|name=[[Theo Fages]]|pos=SH|pos2=HK}}\n|list1h={{Rls player|no=8|name=[[Alex Walmsley]]|pos=PR}}\n|list1i={{Rls player|no=9|name=[[James Roby]]|pos=HK}} (c)\n|list1j={{Rls player|no=10|name=[[Luke Thompson (rugby league)|Luke Thompson]]|pos=PR|pos2=SR}}\n|list1k={{Rls player|no=11|name=[[Zeb Taia]]|pos=SR|pos2=LF|pos3=CE|pos4=PR}}\n|list1l={{Rls player|no=12|name=[[Dominique Peyroux]]|pos=SR|pos2=CE}}\n|list1m={{Rls player|no=13|name=[[Louie McCarthy-Scarsbrook]]|pos=LF|pos2=SR|pos3=PR}}\n\n|list2a={{Rls player|no=14|name=[[Morgan Knowles]]|pos=LF||pos2=SR|pos3=HK}}\n|list2b={{Rls player|no=15|name=[[Matty Lees]]|pos=PR|pos2=SR}}\n|list2c={{Rls player|no=16|name=[[Kyle Amor]]|pos=PR|pos2=LF}}\n|list2d={{Rls player|no=17|name=[[Jack Ashworth]]|pos=SR|pos2=CE}}\n|list2e={{Rls player|no=18|name=[[Joseph Paulo]]|pos=SR|pos2=LF|pos3=SO}}\n|list2f={{Rls player|no=19|name=[[Aaron Smith (rugby league, born 1996)|Aaron Smith]]|pos=HK}}\n|list2g={{Rls player|no=20|name=[[James Bentley (rugby league)|James Bentley]]|pos=SR|pos2=LF|pos3=HK}}\n|list2h={{Rls player|no=21|name=[[Matty Costello]]|pos=CE|pos2=FB}}\n|list2i={{Rls player|no=22|name=[[Jack Welsby]]|pos=SO|pos2=SH}}\n|list2j={{Rls player|no=23|name=[[Joe Batchelor]]|pos=SR}}\n|list2k={{Rls player|no=24|name=[[Josh Eaves]]|pos=HK}}\n|list2l={{Rls player|no=25|name=[[Callum Hazzard]]|pos=LF}}\n|list2m={{Rls player|no=26|name=[[Josh Simm]]|pos=CE}}\n\n|list3a={{Rls player|no=27|name=Lewis Dodd|pos=HB}}\n|list3b={{Rls player|no=28|name=Nico Ri