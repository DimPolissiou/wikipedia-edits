scsaba]])===\n\n{{#invoke:sports table|main|style=WDL\n|res_col_header=Q\n|winpoints=2\n|show_positions=n\n|sortable_table=y\n\n|team1=DEN|name_DEN={{hbw|DEN}}\n|team2=GER|name_GER={{hbw|GER}}\n|team3=ROM|name_ROM={{hbw|ROM}}\n|team4=SWE|name_SWE={{hbw|SWE}}\n|win_DEN=2|draw_DEN=0|loss_DEN=1|gf_DEN=75|ga_DEN=70\n|win_GER=2|draw_GER=0|loss_GER=1|gf_GER=75|ga_GER=73\n|win_ROM=2|draw_ROM=0|loss_ROM=1|gf_ROM=81|ga_ROM=75\n|win_SWE=0|draw_SWE=0|loss_SWE=3|gf_SWE=68|ga_SWE=81\n\n|hth_DEN=Tiebreaker: +2\n|hth_GER=Tiebreaker: -1\n|hth_ROM=Tiebreaker: -1\n\n|col_A=#ccffcc|text_A=\n|result1=A|result2=A|result3=A\n|class_rules=The tiebreaker was the goal difference in the matches between Denmark, Germany and Romania\n|update=complete|source=\n}}\n\n{| style=\"text-align:center\" width=400\n|-\n! width=15%| !! width=10%| !! width=15%| !! width=10%|\n|- align=\"left\"\n|December 9, 2004\n|-\n| align=\"right\"|Denmark || 27 \u2013 24 || align=\"left\"|Germany || \n|-\n| align=\"right\"|Romania || 32 \u2013 25 || align=\"left\"|Sweden || \n|- align=\"left\"\n| December 10, 2004\n|-\n| align=\"right\"|Sweden || 21 \u2013 24 || align=\"left\"|Denmark || \n|-\n| align=\"right\"|Germany || 26 \u2013 24 || align=\"left\"|Romania ||\n|- align=\"left\"\n| December 12, 2004\n|-\n| align=\"right\"|Denmark || 24 \u2013 25 || align=\"left\"|Romania || \n|-\n| align=\"right\"|Germany || 25 \u2013 22 || align=\"left\"|Sweden || \n|}\n\n===Group D ([[Gy\u0151r]])===\n\n{{#invoke:sports table|main|style=WDL\n|res_col_header=Q\n|winpoints=2\n|show_positions=n\n|sortable_table=y\n\n|team1=HUN|name_HUN={{hbw|HUN}}\n|team2=AUT|name_AUT={{hbw|AUT}}\n|team3=FRA|name_FRA={{hbw|FRA}}\n|team4=BLR|name_BLR={{hbw|BLR}}\n|win_HUN=3|draw_HUN=0|loss_HUN=0|gf_HUN=98|ga_HUN=74\n|win_AUT=2|draw_AUT=0|loss_AUT=1|gf_AUT=91|ga_AUT=84\n|win_FRA=1|draw_FRA=0|loss_FRA=2|gf_FRA=81|ga_FRA=95\n|win_BLR=0|draw_BLR=0|loss_BLR=3|gf_BLR=70|ga_BLR=87\n\n|col_A=#ccffcc|text_A=\n|result1=A|result2=A|result3=A\n|update=complete|