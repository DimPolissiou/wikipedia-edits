rst = Naomi\n | year = 2013\n | publisher = [[International Press Institute]]\n | url = https://ethicaljournalismnetwork.org/assets/docs/197/150/4d96ac5-55a3396.pdf\n | isbn = 978-3-9503007-9-6\n | ref = harv\n}}\n*{{Cite book| title = Globalization and Postcolonialism: Hegemony and Resistance in the Twenty-first Century\n | last = Krishna | first = Sankaran\n | year = 2009\n | publisher = [[Rowman & Littlefield]]\n | url = https://books.google.com/books?id=-kjCgiB9Q48C&pg=PA135\n | isbn = 978-0-742-55468-9\n | ref = harv\n}}\n*{{Cite book| title = Digital militarism: Israel's occupation in the social media age\n | last1 = Kuntsman | first1 = Adi\n | last2 = Stein | first2 = Rebecca\n | year = 2015\n | publisher = [[Stanford University Press]]\n | url = https://books.google.com/books?id=tXXeBgAAQBAJ&pg=PA67\n | isbn = 978-0-804-79497-8\n | ref = harv\n}}\n*{{Cite book| title = Laughter in Occupied Palestine: Comedy and Identity in Art and Film\n | last = Lionis | first = Chrisoula\n | year = 2016\n | publisher = [[I.B.Tauris]]\n | url = https://books.google.com/books?id=yCr7CwAAQBAJ&pg=PA89\n | isbn = 978-1-784-53288-8\n | ref = harv\n}}\n*{{Cite journal | title = The Logic of Israel's Targeted Killing\n | last = Luft | first = Gal\n | journal = [[Middle East Quarterly]]\n | date = Winter 2003 | volume = 10 | issue = 1\n | url = https://www.meforum.org/articles/other/the-logic-of-israel-s-targeted-killing\n | ref = harv\n}}\n*{{Cite news| title = Diary\n | last = Mendel | first = Yonatan\n | magazine = [[London Review of Books]]\n | volume = 30 | issue = 5 | pages = 30\u201331\n | url = https://www.lrb.co.uk/v30/n05/yonatan-mendel/diary\n | date = March 6, 2008\n | ref = harv\n}}\n*{{Cite book| title = The World According To Israeli Newspapers: Representations of International Involvement in the Israeli-Palestinian Conflict\n | last = M\u00fcller | first = Margret\n | year = 2017\n | publisher = Frank & Timme\n | url = https://books.google.com/books?id=4Yz-DQAAQBAJ&