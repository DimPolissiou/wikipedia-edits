r|dab=footballer}}\n|<ref>{{SFA Profile|id=65509|name=Paul Telfer}}</ref>\n|colspan=2|2000\n|1\n|0\n|-\n|align=left|{{sortname|Willie|Telfer|dab=footballer, born 1925}}\n|<ref>{{SFA Profile|id=113062|name=Willie Telfer}}</ref>\n|colspan=2|1953\n|1\n|0\n|-\n|align=left|{{sortname|Willie|Telfer|dab=footballer, born 1909}}\n|<ref>{{SFA Profile|id=113265|name=Willie Telfer}}</ref>\n|1932\n|1933\n|2\n|0\n|-\n|align=left|{{sortname|Alec|Thomson|dab=footballer}}\n|<ref>{{SFA Profile|id=113066|name=Alec Thomson}}</ref>\n|1926\n|1932\n|3\n|0\n|-\n|align=left|{{sortname|Alexander|Thomson|dab=footballer}}\n|<ref>{{SFA Profile|id=113278|name=Alexander Thomson}}</ref>\n|colspan=2|1909\n|1\n|1\n|-\n|align=left|{{sortname|Andrew|Thomson|dab=Arthurlie footballer}}\n|<ref>{{SFA Profile|id=113064|name=Andrew Thomson}}</ref>\n|colspan=2|1886\n|1\n|0\n|-\n|align=left|{{sortname|Andrew|Thomson|dab=Third Lanark footballer}}\n|<ref>{{SFA Profile|id=113065|name=Andrew Thomson}}</ref>\n|colspan=2|1889\n|1\n|0\n|-\n|align=left|{{sortname|Bertie|Thomson}}\n|<ref>{{SFA Profile|id=113279|name=Bertie Thomson}}</ref>\n|colspan=2|1931\n|1\n|1\n|-\n|align=left|{{sortname|Charlie M.|Thomson}}\n|<ref>{{SFA Profile|id=113068|name=Charles Thomson}}</ref>\n|colspan=2|1937\n|1\n|0\n|-\n|align=left|{{sortname|David|Thomson|dab=footballer, born 1892}}\n|<ref>{{SFA Profile|id=113069|name=David Thomson}}</ref>\n|colspan=2|1920\n|1\n|0\n|-\n|align=left|{{sortname|James J.|Thomson}}\n|<ref>{{SFA Profile|id=112998|name=James Thomson}}</ref>\n|1872\n|1874\n|3\n|0\n|-\n|align=left|{{sortname|Jock|Thomson}}\n|<ref>{{SFA Profile|id=113070|name=Jock Thomson}}</ref>\n|colspan=2|1932\n|1\n|0\n|-\n|align=left|{{sortname|John|Thomson|dab=footballer, born 1909}}\n|<ref>{{SFA Profile|id=113003|name=John Thomson}}</ref>\n|1930\n|1931\n|4\n|0\n|-\n|align=left|{{sortname|Kevin|Thomson}}\n|<ref>{{SFA Profile|id=29063|name=Kevin Thomson}}</ref>\n|2008\n|2010\n|3\n|0\n|-\n|align=left|{{sortname|Robert|Thomson|dab=footballer, bo