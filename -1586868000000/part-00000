ng = 6th \n | postseason   = \n}}\n{{CBB Yearly Record Subtotal\n | name       = Tennessee\n | overall    = 36\u201347 ({{Winning percentage|36|47}})\n | confrecord = 14\u201324 ({{Winning percentage|14|24}})\n}}\n{{CBB Yearly Record Subhead\n | name       = [[Northwestern Wildcats men's basketball|Northwestern]]\n | conference = [[Big Ten Conference]]\n | startyear  = 1997\n | endyear    = 2000\n}}\n{{CBB Yearly Record Entry\n | championship = \n | season       = [[1997\u201398 NCAA Division I men's basketball season|1997\u201398]]\n | name         = Northwestern\n | overall      = 10\u201317\n | conference   = 3\u201313\n | confstanding = T\u20139th\n | postseason   =\n}}\n{{CBB Yearly Record Entry\n | championship = \n | season       = [[1998\u201399 NCAA Division I men's basketball season|1998\u201399]]\n | name         = Northwestern\n | overall      = 15\u201314\n | conference   = 6\u201310\n | confstanding = 8th\n | postseason   = [[1999 National Invitation Tournament|NIT First Round]]\n}}\n{{CBB Yearly Record Entry\n | championship = \n | season       = [[1999\u20132000 NCAA Division I men's basketball season|1999\u201300]]\n | name         = Northwestern\n | overall      = 5\u201325\n | conference   = 0\u201316\n | confstanding = 11th\n | postseason   = \n}}\n{{CBB Yearly Record Subtotal\n | name       = Northwestern\n | overall    = 30\u201356 ({{Winning percentage|30|56}})\n | confrecord = 9\u201339 ({{Winning percentage|9|39}})\n}}\n{{CBB Yearly Record Subhead\n | name       = [[Arizona Wildcats men's basketball|Arizona Wildcats]]\n | conference = [[Pac-12 Conference|Pacific-10 Conference]]\n | startyear  = 2007\n | endyear    = 2008\n}}\n{{CBB Yearly Record Entry\n | championship = \n | season       = [[2007\u201308 NCAA Division I men's basketball season|2007\u201308]]\n | name         = [[2007\u201308 Arizona Wildcats men's basketball team|Arizona]]\n | overall      = 19\u201315* \n | conference   = 8\u201310\n | confstanding = 7th\n | postseason   = 