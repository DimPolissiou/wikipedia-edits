or]]\n|team10=OFS|name_OFS=[[Ofspor|Ofspor A.\u015e.]]\n|team11=PAZ|name_PAZ=[[Pazarspor]]\n|team12=ALT|name_ALT=[[Altay S.K.|Altay]]\n|team13=BYR|name_BYR=[[Bayrampa\u015faspor]]\n|team14=G\u00d6L|name_G\u00d6L=[[G\u00f6lba\u015f\u0131spor|G\u00f6lba\u015f\u0131spor A.\u015e.]]\n|team15=\u0130D\u00c7|name_\u0130D\u00c7=[[\u0130skenderun Demir \u00c7elikspor|K\u00f6rfez \u0130skenderun Spor]]\n|team16=EYP|name_EYP=[[Ey\u00fcpspor]]\n|team17=GUN|name_GUN=[[\u0130stanbul G\u00fcng\u00f6renspor|G\u00fcng\u00f6renspor]]\n|team18=BOZ|name_BOZ=[[Boz\u00fcy\u00fckspor]]\n|win_ATN=25|draw_ATN=6|loss_ATN=3|gf_ATN=77|ga_ATN=23|status_ATN=C,P\n|win_ALA=22|draw_ALA=6|loss_ALA=6|gf_ALA=76|ga_ALA=35|status_ALA=P\n|win_MKE=22|draw_MKE=5|loss_MKE=7|gf_MKE=77|ga_MKE=30\n|win_BAN=18|draw_BAN=7|loss_BAN=9|gf_BAN=68|ga_BAN=48\n|win_ASK=17|draw_ASK=9|loss_ASK=8|gf_ASK=40|ga_ASK=31\n|win_TPC=15|draw_TPC=7|loss_TPC=12|gf_TPC=41|ga_TPC=43\n|win_NBS=13|draw_NBS=11|loss_NBS=10|gf_NBS=41|ga_NBS=39\n|win_BUG=13|draw_BUG=8|loss_BUG=13|gf_BUG=50|ga_BUG=43\n|win_KRF=13|draw_KRF=5|loss_KRF=16|gf_KRF=55|ga_KRF=56\n|win_OFS=11|draw_OFS=10|loss_OFS=13|gf_OFS=45|ga_OFS=54\n|win_PAZ=13|draw_PAZ=3|loss_PAZ=18|gf_PAZ=40|ga_PAZ=55\n|win_ALT=11|draw_ALT=9|loss_ALT=14|gf_ALT=50|ga_ALT=52\n|win_BYR=11|draw_BYR=8|loss_BYR=15|gf_BYR=37|ga_BYR=56\n|win_G\u00d6L=11|draw_G\u00d6L=4|loss_G\u00d6L=19|gf_G\u00d6L=43|ga_G\u00d6L=53\n|win_\u0130D\u00c7=9|draw_\u0130D\u00c7=10|loss_\u0130D\u00c7=15|gf_\u0130D\u00c7=34|ga_\u0130D\u00c7=46\n|win_EYP=9|draw_EYP=9|loss_EYP=16|gf_EYP=36|ga_EYP=61|status_EYP=R\n|win_GUN=7|draw_GUN=6|loss_GUN=21|gf_GUN=27|ga_GUN=54|status_GUN=R\n|win_BOZ=2|draw_BOZ=5|loss_BOZ=27|gf_BOZ=21|ga_BOZ=79|status_BOZ=R\n|col_P=green1|text_P=Promotion to [[2014\u201315 TFF First League|TFF First League]]\n|result1=P\n|col_QP=green2|text_QP=Qualification for [[#Promotion playoffs|Promotion Playoffs]]\n|result2=QP|result3=QP|result4=QP|result5=QP\n|col_R=red1|text_R=Relegation to [[2014\u201315 TFF Thir