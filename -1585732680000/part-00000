alspor]]\n|team6=PDK|name_PDK=[[Pendikspor]]\n|team7=ING|name_ING=[[\u0130neg\u00f6lspor]]\n|team8=AYD|name_AYD=[[Ayd\u0131nspor|Ayd\u0131nspor 1923]]\n|team9=TRG|name_TRG=[[Turgutluspor]]\n|team10=GMH|name_GMH=[[G\u00fcm\u00fc\u015fhanespor]]\n|team11=KRL|name_KRL=[[K\u0131rklarelispor]]\n|team12=TOK|name_TOK=[[Tokatspor]]\n|team13=ANS|name_ANS=[[Konya Anadolu Sel\u00e7ukspor]]\n|team14=SAR|name_SAR=[[Sar\u0131yer G.K.|Sar\u0131yer]]\n|team15=T\u0130Y|name_T\u0130Y=[[Tarsus Idman Yurdu SK|Tarsus \u0130.Y.]]\n|team16=DAR|name_DAR=[[Dardanel Spor A.\u015e.|\u00c7anakkale Dardanelspor]]\n|team17=GOP|name_GOP=[[Gaziosmanpa\u015fa S.K.|Gaziosmanpa\u015fa]]\n|team18=\u00c7BE|name_\u00c7BE=[[\u00c7ank\u0131r\u0131 Belediyespor]]\n|win_GRS=20|draw_GRS=7|loss_GRS=7|gf_GRS=55|ga_GRS=33|status_GRS=C,P\n|win_G\u00d6Z=18|draw_G\u00d6Z=12|loss_G\u00d6Z=4|gf_G\u00d6Z=51|ga_G\u00d6Z=27\n|win_HTY=18|draw_HTY=9|loss_HTY=7|gf_HTY=50|ga_HTY=30\n|win_YML=16|draw_YML=10|loss_YML=8|gf_YML=50|ga_YML=34\n|win_KRT=16|draw_KRT=10|loss_KRT=8|gf_KRT=49|ga_KRT=33\n|win_PDK=15|draw_PDK=11|loss_PDK=8|gf_PDK=43|ga_PDK=31\n|win_ING=15|draw_ING=9|loss_ING=10|gf_ING=46|ga_ING=33\n|win_AYD=11|draw_AYD=13|loss_AYD=10|gf_AYD=43|ga_AYD=38\n|win_TRG=10|draw_TRG=13|loss_TRG=11|gf_TRG=43|ga_TRG=43\n|win_GMH=9|draw_GMH=13|loss_GMH=12|gf_GMH=31|ga_GMH=32\n|win_KRL=9|draw_KRL=11|loss_KRL=14|gf_KRL=34|ga_KRL=51\n|win_TOK=10|draw_TOK=8|loss_TOK=16|gf_TOK=39|ga_TOK=57\n|win_ANS=8|draw_ANS=12|loss_ANS=14|gf_ANS=31|ga_ANS=39\n|win_SAR=6|draw_SAR=16|loss_SAR=12|gf_SAR=38|ga_SAR=48\n|win_T\u0130Y=7|draw_T\u0130Y=12|loss_T\u0130Y=15|gf_T\u0130Y=32|ga_T\u0130Y=50\n|win_DAR=6|draw_DAR=14|loss_DAR=14|gf_DAR=29|ga_DAR=40|status_DAR=R\n|win_GOP=5|draw_GOP=13|loss_GOP=16|gf_GOP=33|ga_GOP=52|status_GOP=R\n|win_\u00c7BE=4|draw_\u00c7BE=13|loss_\u00c7BE=17|gf_\u00c7BE=17|ga_\u00c7BE=43|status_\u00c7BE=R\n|col_P=green1|text_P=Promotion to [[2014\u201315 TFF First League|TFF First League]]\n|result1=P\n|col_QP=green2|text_QP=Qua