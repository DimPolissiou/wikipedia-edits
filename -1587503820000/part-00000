erican football roster/Player|first=Joey|last=Gorgen|num=22|pos=DB|class=FR|rs=y}}\n{{American football roster/Player|first=Troy|last=Dipre|num=24|pos=DB|class=FR|rs=y}}\n{{American football roster/Player|first=Calvin|last=Swenson|num=26|pos=DB|class=FR}}\n{{American football roster/Player|first=Eric|last=Amoako|num=30|pos=DB|class=SR|rs=y}}\n{{American football roster/Player|first=Kiondre|last=Thomas|num=31|pos=DB|class=FR}}\n{{American football roster/Player|first=Tamarion|last=Johnson|num=33|pos=DL|class=FR}}\n{{American football roster/Player|first=Antonio|last=Shenault|num=34|pos=DB|class=SO}}\n{{American football roster/Player|first=Jaylen|last=Waters|num=35|pos=LB|class=FR|rs=y}}\n{{American football roster/Player|first=Blake|last=Cashman|num=36|pos=DB|class=SO|link=y}}\n{{American football roster/Player|first=Alexander|last=Starks|num=40|pos=DB|class=SR|rs=y}}\n{{American football roster/Player|first=Thomas|last=Barber|num=41|pos=LB|class=FR}}\n{{American football roster/Player|first=Bailey|last=Schoenfelder|num=43|pos=LB|class=FR}}\n{{American football roster/Player|first=Kamal|last=Martin|num=44|pos=LB|class=FR}}\n{{American football roster/Player|first=Carter|last=Coughlin|num=45|pos=LB|class=FR|link=y}}\n{{American football roster/Player|first=Blake|last=Weber|num=48|pos=LB|class=JR}}\n{{American football roster/Player|first=Jack|last=Lynn|num=50|pos=LB|class=SR|rs=y|note=C}}\n{{American football roster/Player|first=Yoshoub|last=Timms|num=52|pos=DL|class=SR|rs=y}}\n{{American football roster/Player|first=Nick|last=Rallis|num=56|pos=LB|class=SR|rs=y}}\n{{American football roster/Player|first=Ray|last=Dixon|num=59|pos=LB|class=JR|rs=y}}\n{{American football roster/Player|first=Gaelin|last=Elmore|num=87|pos=DL|class=JR}}\n{{American football roster/Player|first=Winston|last=DeLattiboudere|num=88|pos=DL|class=FR|rs=y}}\n{{American football roster/Player|first=Sam|last=Renner|num=90|pos=DL|class=FR|rs=y}}\n{{American football roster/Player|first=Julien|last=Kafo|num=91|pos=DE|class=SO|rs=y}}\n{{American football roster/Player|first=Tai'yon|last=Devers|num=92|pos=DL|class=FR}}\n{{American football roster/Player|first=Merrick|last=Jackson|num=93|pos=DL|class=JR}}\n{{American football roster/Player|first=Hendrick|last=Ekpe|num=95|pos=DL|class=SR}}\n{{American football roster/Player|first=Steven|last=Richardson|num=96|pos=DL|class=JR}}\n{{American football roster/Player|first=Scott|last=Ekpe|num=97|pos=DL|class=SR|rs=y}}\n{{American football roster/Player|first=Andrew|last=Stelter|num=99|pos=DE|class=JR}}\n|special_teams_players=\n{{American football roster/Player|first=Ryan|last=Santoso|num=18|pos=K|pos2=P|class=JR|rs=y|link=y}}\n{{American football roster/Player|first=Payton|last=Jordahl|num=25|pos=LS|class=SO|rs=y}}\n{{American football roster/Player|first=Andrew|last=Harte|num=33|pos=K|class=JR|rs=y}}\n{{American football roster/Player|first=Logan|last=McElfresh|num=34|pos=P|class=SO|rs=y}}\n{{American football roster/Player|first=John|last=Mack|num=37|pos=K|class=FR}}\n{{American football roster/Player|first=Emmit|last=Carpenter|num=38|pos=K|class=SO|rs=y}}\n{{American football roster/Player|first=Trey|last=Hansen|num=39|pos=LS|class=FR|rs=y}}\n{{American football roster/Player|first=Justin|last=Juenemann|num=42|pos=K|class=JR|rs=y}}\n{{American football roster/Player|first=Jacob|last=Herbers|num=47|pos=P|class=FR|rs=y}}\n}}\n{{American football roster/Footer|roster_url=http://www.gophersports.com/sports/m-footbl/mtt/minn-m-footbl-mtt.html |accessdate=August 17, 2016\n|head_coach=\n*[[Tracy Claeys]] (Head Coach)\n|asst_coach=\n*Brian Anderson (Wide Receivers)\n*Jay Johnson (Offensive Coordinator / Quarterbacks)\n*Bart Miller (Offesnive Line)\n*Dan O'Brien (Asst. Defensive Backs / Co-Special Teams Coordinator / Asst. to the Head Coach)\n*Jeff Phelps (Defensive Line)\n*Pat Poore (Running Backs / Co-Special Teams Coordinator)\n*Rob Reeves (H-Backs / Tight Ends)\n*Jay Sawvel (Defensive Coordinator)\n*Mike Sherels (Lineback