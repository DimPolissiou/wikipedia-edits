er 2018}}\n|-\n! scope=\"row\" | Germany ([[German Airplay Chart|Airplay Chart]])<ref>{{cite web|url=http://www.radiocharts.com/html/charts_de_main.htm |title=Radio-Charts Deutschland \u2013 Chartwoche 02/2008 |work=MusicTrace |publisher=RadioCharts.com |language=German |accessdate=11 June 2017 |archiveurl=https://web.archive.org/web/20080121190030/http://www.radiocharts.com/html/charts_de_main.htm |archivedate=21 January 2008 |url-status = dead}}</ref>\n| align=\"center\" | 1\n|-\n!scope=\"row\"{{singlechart|Hungary|1|artist=Sugababes|song=About You Now|year=2008|week=4|accessdate=25 April 2013}}\n|-\n!scope=\"row\"{{singlechart|Ireland2|2|artist=Sugababes|song=About You Now|accessdate=15 January 2020}}\n|-\n!scope=\"row\"{{singlechart|Dutch40|18|artist=Sugababes|song=About You Now|year=2007|week=45|accessdate=25 April 2013}}\n|-\n!scope=\"row\"{{singlechart|Dutch100|16|artist=Sugababes|song=About You Now|accessdate=25 April 2013}}\n|-\n!scope=\"row\"{{singlechart|New Zealand|18|artist=Sugababes|song=About You Now|accessdate=25 April 2013}}\n|-\n!scope=\"row\"{{singlechart|Norway|7|artist=Sugababes|song=About You Now|accessdate=25 April 2013}}\n|-\n!scope=\"row\"| Poland ([[Polish Music Charts|Airplay Chart]])<ref>{{cite web|archiveurl=https://web.archive.org/web/20071206201150/http://www.nielsenmusiccontrol.com/index_pol|title=Nielsen Music Control|archivedate=6 December 2007|url=http://www.nielsenmusiccontrol.com/index_pol}}</ref>\n| style=\"text-align:center;\"|1\n|-\n!scope=\"row\"|Romania ([[Romanian Top 100]])<ref>{{cite web|url=http://www.rt100.ro/top-100-edition.html|title=Romanian Top 100 Edition 618|publisher=[[Romanian Top 100]]|accessdate=3 January 2013|archiveurl=https://web.archive.org/web/20080228221149/http://www.rt100.ro/top-100-edition.html|archivedate=28 February 2008}}</ref>\n|style=\"text-align:center;\"|35\n|-\n!scope=\"row\"{{singlechart|Scotland|1|artist=Sugababes|song=About You Now|date=20071013|accessdate=9 July 2018}}\n|-\n!scope=\"row\"{