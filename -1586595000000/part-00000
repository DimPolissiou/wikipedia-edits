 = Grub (Oberbay)\n| native_name         = \n| native_name_lang    = de\n| symbol              = rail\n| symbol_location     = de\n| symbol2             = s\n| symbol_location2    = munich\n| type                = [[Railway station types in Germany|Hp]]\n| image               = \n| alt                 = \n| caption             = \n| address             = Parsdorfer Stra\u00dfe<br>85586 [[Grub (Poing)|Grub]] \n| borough             = [[Bavaria]]\n| country             = [[Germany]]\n| coordinates         = {{coord|48.1644|N|11.7824|E|display=inline,title}}\n| elevation           = <!-- {{convert|0|m|abbr=on}} -->\n| owned               = [[Deutsche Bahn]]\n| operator            = {{Plainlist|1=\n*[[DB Netz]]\n*[[DB Station&Service]]\n}}\n| line                = {{Plainlist|1=\n*[[Munich\u2013M\u00fchldorf railway]] <small>({{Kursbuchlink|Region=900-999|Nummer=940}})</small>\n}}\n| distance            = \n| platforms           = 2 [[side platforms]]\n| tracks              = 2\n| train_operators     = {{Plainlist|1=\n*[[S-Bahn M\u00fcnchen]]\n}}\n| connections         = {{Plainlist|1=\n*{{\u00d6PNV M\u00fcnchen|S2}}\n*{{rint|bus}} 446 452 463\n}}\n| structure           = \n| parking             = yes\n| bicycle             = yes\n| disabled            = partly\n| code                = {{Deutsche Bahn station codes|code=2376|ds100=MGRB<ref name=Eisenbahnatlas>{{cite book|title=Eisenbahnatlas Deutschland (German railway atlas)|edition=2017|publisher=Schweers + Wall|year=2017|isbn=978-3-89494-146-8}}</ref>|ibnr=8002435|category=5<ref name=categories>{{DBCatsURL}}</ref>}}\n| zone                = {{\u00d6PNV M\u00fcnchen|MVV}}: 1 and 2<ref>{{cite web |title=S-Bahn, U-Bahn, Regionalzug, Tram und ExpressBus im MVV |url=https://www.mvv-muenchen.de/fileadmin/mediapool/03-Plaene_Bahnhoefe/Netzplaene/MVV_Netzplan_S_U_R_T_X.pdf |publisher=[[M\u00fcnchner Verkehrs- und Tarifverbund]] |accessdate=28 February 2020 |date=December 2019}}</ref>\n| website             = [https://www.bah