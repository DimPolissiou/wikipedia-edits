n|num=75|pos=OL|class=FR|rs=y}}\n{{American football roster/Player|first=Kendall|last=Baker|num=76|pos=OL|class=FR}}\n{{American football roster/Player|first=Adrian|last=Goodacre|num=77|pos=OL|class=SO}}\n{{American football roster/Player|first=Allen|last=Cummins|num=78|pos=OL|class=JR}}\n{{American football roster/Player|first=Eddie|last=Gordon|num=79|pos=OL|class=SR}}\n{{American football roster/Player|first=Nick|last=Byrne|num=81|pos=TE|class=SR}}\n{{American football roster/Player|first=Nate|last=Arceneaux|num=82|pos=WR|class=SO}}\n{{American football roster/Player|first=Trey|last=Green|num=83|pos=WR|class=FR}}\n{{American football roster/Player|first=David|last=Doeker|num=84|pos=WR|class=FR}}\n{{American football roster/Player|first=Calif|last=Gossett|num=85|pos=WR|class=FR}}\n{{American football roster/Player|first=Chris|last=Turner|num=87|pos=TE|class=FR|rs=y}}\n{{American football roster/Player|first=Cabron|last=Baham|num=|pos=WR|class=FR}}\n{{American football roster/Player|first=Ryheem|last=Malone|num=|pos=WR|class=JR}}\n\n|defensive_players=\n{{American football roster/Player|first=Joe|last=Dillon|num=3|pos=DL|class=FR|rs=y}}\n{{American football roster/Player|first=Tre\u2019Maine|last=Lightfoot|num=4|pos=LB|class=SR}}\n{{American football roster/Player|first=Savion|last=Brown|num=6|pos=DB|class=FR|rs=y}}\n{{American football roster/Player|first=Otha|last=Peters|link=y|num=7|pos=LB|class=SR}}\n{{American football roster/Player|first=Simeon|last=Thomas|link=y|num=8|pos=DB|class=JR}}\n{{American football roster/Player|first=Kamar|last=Greenhouse|num=9|pos=DB|class=FR|rs=y}}\n{{American football roster/Player|first=Christian|last=Goodlett|num=10|pos=DB|class=SR}}\n{{American football roster/Player|first=Lorenzo|last=Cryer|num=11|pos=DB|class=SO}}\n{{American football roster/Player|first=Vince|last=Thomas|num=13|pos=DB|class=SO}}\n{{American football roster/Player|first=Travis|last=Crawford|num=16|pos=DB|class=JR}}\n{{American football roster/Player|first=Dam