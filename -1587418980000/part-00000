ne|Ottey}}\n|{{nowrap|{{Flag|JAM}}}}\n|{{right|{{dts|13 Mar 1982}}}}\n|\n|[[Cedar Falls, Iowa|Cedar Falls]]\n|{{nowrap|{{Flag|USA}}}}\n|\n|-\n|rowspan=2|[[300 metres|300 m]]\n|rowspan=2|{{right|35.45}}\n|\n|[[Irina Privalova]]\n|{{nowrap|{{Flag|RUS}}}}\n|{{right|{{dts|17 Jan 1993}}}}\n|\n|[[Moscow]]\n|{{nowrap|{{Flag|RUS}}}}\n|\n|-\n|\n|[[Shaunae Miller]]\n|{{nowrap|{{Flag|BAH}}}}\n|{{right|{{dts|3 Feb 2018}}}}\n|[[Millrose Games]]\n|[[Fort Washington Avenue Armory|New York City]]\n|{{nowrap|{{Flag|USA}}}}\n|<ref name=\"Bob Ramsak\"/>\n|-\n|{{sort|1Run000457|[[500 yards|500 y]]}}\n|data-sort-value=\"1Secs00060.61\"|{{right|1:00.61}} \n|{{AthAbbr|A}}\n|{{sortname|Jearl|Miles-Clark}}\n|{{nowrap|{{Flag|USA}}}}\n|{{right|{{dts|19 Feb 2000}}}}\n|\n|[[Pocatello]]\n|{{nowrap|{{Flag|USA}}}}\n|\n|-\n|{{sort|1Run000500|[[500 metres|500 m]]}}\n|data-sort-value=\"1Secs00066.31\"|{{right|1:06.31}}\n|\n|{{sortname|Olesya|Krasnomovets}}\n|{{nowrap|{{Flag|RUS}}}}\n|{{right|{{dts|7 Jan 2006}}}}\n|\n|[[Yekaterinburg]]\n|{{nowrap|{{Flag|RUS}}}}\n|\n|-\n|{{sort|1Run000549|[[600 yards|600 y]]}}\n|data-sort-value=\"1Secs00077.38\"|{{right|1:17.38}}\n|\n|{{sortname|Delisa|Walton}}\n|{{nowrap|{{Flag|USA}}}}\n|{{right|{{dts|13 Mar 1982}}}}\n|\n|[[Cedar Falls, Iowa|Cedar Falls]]\n|{{nowrap|{{Flag|USA}}}}\n|\n|-\n|{{sort|1Run000600|[[600 metres|600 m]]}}\n|data-sort-value=\"1Secs00083.44\"|{{right|1:23.44}}\n|\n|{{sortname|Olga|Kotlyarova}}\n|{{nowrap|{{Flag|RUS}}}}\n|{{right|{{dts|1 Feb 2004}}}}\n|[[Russian Winter Meeting]]\n|[[Moscow]]\n|{{nowrap|{{Flag|RUS}}}}\n|\n|-\n|{{sort|1Run000914|[[1000 yards|1000 y]]}}\n|data-sort-value=\"1Secs000143.5\"|{{right|2:23.5}}\n|\n|{{sortname|Joetta|Clark}}\n|{{nowrap|{{Flag|USA}}}}\n|{{right|{{dts|9 Mar 1986}}}}\n|\n|[[Gainesville, Florida|Gainesville]]\n|{{nowrap|{{Flag|USA}}}}\n|\n|-\n|{{sort|1Run002000|[[2000 metres|2000 m]]}}\n|data-sort-value=\"1Secs00323.75\"|{{right|5:23.75}}\n|\n|{{sortname|Genzebe|Dibaba}}\n|{{nowrap|{{Flag|ETH}}}}\n|{{right|{{