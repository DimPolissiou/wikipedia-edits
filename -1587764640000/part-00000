ky\n| RD1-score14-1=2\n| RD1-score14-2=6\n| RD1-score14-3=&nbsp;\n\n| RD1-seed15=&nbsp;\n| RD1-team15={{flagicon|TCH}} [[Vladim\u00edr Zedn\u00edk|V Zedn\u00edk]]\n| RD1-score15-1=4\n| RD1-score15-2=4\n| RD1-score15-3=&nbsp;\n| RD1-seed16=&nbsp;\n| RD1-team16='''{{flagicon|SWE}} [[Rolf Norberg|R Norberg]]'''\n| RD1-score16-1='''6'''\n| RD1-score16-2='''6'''\n| RD1-score16-3=&nbsp;\n\n| RD2-seed01=1\n| RD2-team01='''{{flagicon|USA}} [[Jimmy Connors|J Connors]]'''\n| RD2-score01-1='''6'''\n| RD2-score01-2='''6'''\n| RD2-score01-3=&nbsp;\n| RD2-seed02=&nbsp;\n| RD2-team02={{flagicon|SWE}} [[Birger Andersson|B Andersson]]\n| RD2-score02-1=3\n| RD2-score02-2=1\n| RD2-score02-3=&nbsp;\n\n| RD2-seed03=&nbsp;\n| RD2-team03='''{{flagicon|HUN}} [[Bal\u00e1zs Tar\u00f3czy|B Tar\u00f3czy]]'''\n| RD2-score03-1='''6'''\n| RD2-score03-2='''7'''\n| RD2-score03-3=&nbsp;\n| RD2-seed04=&nbsp;\n| RD2-team04={{flagicon|USA}} [[Dick R. Bohrnstedt|D Bohrnstedt]]\n| RD2-score04-1=3\n| RD2-score04-2=5\n| RD2-score04-3=&nbsp;\n\n| RD2-seed05=&nbsp;\n| RD2-team05='''{{flagicon|MEX}} [[Ra\u00fal Ram\u00edrez|R Ram\u00edrez]]'''\n| RD2-score05-1='''6'''\n| RD2-score05-2=6\n| RD2-score05-3='''6'''\n| RD2-seed06=&nbsp;\n| RD2-team06={{flagicon|POL}} [[Jacek Nied\u017awiedzki|J Nied\u017awiedzki]]\n| RD2-score06-1=2\n| RD2-score06-2='''7'''\n| RD2-score06-3=2\n\n| RD2-seed07=&nbsp;\n| RD2-team07='''{{flagicon|NED}} [[Tom Okker|T Okker]]'''\n| RD2-score07-1='''6'''\n| RD2-score07-2=4\n| RD2-score07-3='''6'''\n| RD2-seed08=&nbsp;\n| RD2-team08={{flagicon|SWE}} [[Rolf Norberg|R Norberg]]\n| RD2-score08-1=4\n| RD2-score08-2='''6'''\n| RD2-score08-3=2\n\n| RD3-seed01=1\n| RD3-team01='''{{flagicon|USA}} [[Jimmy Connors|J Connors]]'''\n| RD3-score01-1='''6'''\n| RD3-score01-2='''6'''\n| RD3-score01-3=&nbsp;\n| RD3-seed02=&nbsp;\n| RD3-team02={{flagicon|HUN}} [[Bal\u00e1zs Tar\u00f3czy|B Tar\u00f3czy]]\n| RD3-score02-1=4\n| RD3-score02-2=2\n| RD3-score02-3=&nbsp;\n\n| RD3-seed03=&nbsp;\n| RD3-team03='''{{