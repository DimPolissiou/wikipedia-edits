rot|Radni\u010dki Pirot]]\n|team7=\u017dIT|name_\u017dIT=[[FK \u017ditora\u0111a|\u017ditora\u0111a]]\n|team8=BSK|name_BSK=[[FK BSK Bor\u010da|BSK Bor\u010da]]\n|team9=BEO|name_BEO=[[FK Beograd|Beograd]]\n|team10=MRA|name_MRA=[[FK Mladi Radnik|Mladi Radnik]]\n|team11=DUB|name_DUB=[[FK Dubo\u010dica|Dubo\u010dica]]\n|team12=GSV|name_GSV=[[FK Gra\u0111anski Svilajnac|Gra\u0111anski Svilajnac]]\n|team13=NKU|name_NKU=[[FK Napredak Ku\u0161iljevo|Napredak Ku\u0161iljevo]]\n|team14=JPA|name_JPA=[[FK Jedinstvo Para\u0107in|Jedinstvo Para\u0107in]]\n|team15=TKR|name_TKR=[[FK Trayal Kru\u0161evac|Trayal Kru\u0161evac]]\n|team16=RSV|name_RSV=[[FK Radni\u010dki Svilajnac|Radni\u010dki Svilajnac]]\n|team17=\u017dNI|name_\u017dNI=[[FK \u017delezni\u010dar Ni\u0161|\u017delezni\u010dar Ni\u0161]]\n|team18=VU\u010c|name_VU\u010c=[[FK Vu\u010dje|Vu\u010dje]]\n|win_RNI=26|draw_RNI=5|loss_RNI=3|gf_RNI=79|ga_RNI=27|status_RNI=C,P\n|win_NKR=26|draw_NKR=4|loss_NKR=4|gf_NKR=96|ga_NKR=24\n|win_MLA=17|draw_MLA=12|loss_MLA=5|gf_MLA=73|ga_MLA=38\n|win_OFK=16|draw_OFK=8|loss_OFK=10|gf_OFK=54|ga_OFK=38\n|win_HBE=15|draw_HBE=10|loss_HBE=9|gf_HBE=64|ga_HBE=44\n|win_RPI=16|draw_RPI=7|loss_RPI=11|gf_RPI=63|ga_RPI=45\n|win_\u017dIT=16|draw_\u017dIT=6|loss_\u017dIT=12|gf_\u017dIT=51|ga_\u017dIT=47\n|win_BSK=15|draw_BSK=7|loss_BSK=12|gf_BSK=66|ga_BSK=52\n|win_BEO=14|draw_BEO=10|loss_BEO=10|gf_BEO=74|ga_BEO=51\n|win_MRA=15|draw_MRA=6|loss_MRA=13|gf_MRA=64|ga_MRA=38|status_MRA=R\n|win_DUB=14|draw_DUB=6|loss_DUB=14|gf_DUB=55|ga_DUB=60\n|win_GSV=13|draw_GSV=8|loss_GSV=13|gf_GSV=45|ga_GSV=48\n|win_NKU=13|draw_NKU=8|loss_NKU=13|gf_NKU=49|ga_NKU=65|status_NKU=R\n|win_JPA=10|draw_JPA=6|loss_JPA=18|gf_JPA=55|ga_JPA=85|status_JPA=R\n|win_TKR=7|draw_TKR=4|loss_TKR=23|gf_TKR=37|ga_TKR=100|status_TKR=R\n|win_RSV=5|draw_RSV=6|loss_RSV=23|gf_RSV=33|ga_RSV=83|status_RSV=R\n|win_\u017dNI=4|draw_\u017dNI=5|loss_\u017dNI=25|gf_\u017dNI=36|ga_\u017dNI=88|status_\u017dNI=R\n|win_VU\u010c=4|draw_VU\u010c=2|loss_VU\u010c=2