 Oct}}]]\n|match_ESP_MNE=[[#ESP v MNE|2\u20130]]\n|match_ESP_MKD=[[#ESP v MKD|3\u20130]]\n|match_ESP_KAZ=[[#ESP v KAZ|{{small|TBD}}]]\n|match_ESP_FRO=[[#ESP v FRO|{{small|8 Oct}}]]\n\n|match_ISR_ESP=[[#ISR v ESP|1\u20131]]\n|match_ISR_MNE=[[#ISR v MNE|0\u20130]]\n|match_ISR_MKD=[[#ISR v MKD|{{small|TBD}}]]\n|match_ISR_KAZ=[[#ISR v KAZ|{{small|8 Sep}}]]\n|match_ISR_FRO=[[#ISR v FRO|3\u20131]]\n\n|match_MNE_ESP=[[#MNE v ESP|0\u20132]]\n|match_MNE_ISR=[[#MNE v ISR|{{small|8 Oct}}]]\n|match_MNE_MKD=[[#MNE v MKD|1\u20132]]\n|match_MNE_KAZ=[[#MNE v KAZ|1\u20132]]\n|match_MNE_FRO=[[#MNE v FRO|3\u20130]]\n\n|match_MKD_ESP=[[#MKD v ESP|{{small|8 Sep}}]]\n|match_MKD_ISR=[[#MKD v ISR|{{small|TBD}}]]\n|match_MKD_MNE=[[#MKD v MNE|{{small|13 Oct}}]]\n|match_MKD_KAZ=[[#MKD v KAZ|1\u20131]]\n|match_MKD_FRO=[[#MKD v FRO|7\u20131]]\n\n|match_KAZ_ESP=[[#KAZ v ESP|0\u20131]]\n|match_KAZ_ISR=[[#KAZ v ISR|1\u20132]]\n|match_KAZ_MNE=[[#KAZ v MNE|{{small|3 Sep}}]]\n|match_KAZ_MKD=[[#KAZ v MKD|{{small|8 Oct}}]]\n|match_KAZ_FRO=[[#KAZ v FRO|{{small|13 Oct}}]]\n\n|match_FRO_ESP=[[#FRO v ESP|{{small|TBD}}]]\n|match_FRO_ISR=[[#FRO v ISR|{{small|2 Sep}}]]\n|match_FRO_MNE=[[#FRO v MNE|{{small|TBD}}]]\n|match_FRO_MKD=[[#FRO v MKD|{{small|TBD}}]]\n|match_FRO_KAZ=[[#FRO v KAZ|1\u20133]]\n}}</onlyinclude>\n\n==Matches==\nTimes are [[Central European Time|CET]]/[[Central European Summer Time|CEST]],{{refn|group=\"note\"|name=\"Time zone\"|[[Central European Summer Time|CEST]] ([[UTC+02:00|UTC+2]]) for dates between 31 March and 26 October 2019 and between 29 March and 24 October 2020, and [[Central European Time|CET]] ([[UTC+01:00|UTC+1]]) for all other dates.}} as listed by UEFA (local times, if different, are in parentheses).\n\n{{Football box\n|id         = FRO v KAZ\n|date       = {{Start date|2019|6|6|df=y}}\n|time       = 18:00 {{small|(17:00 [[Western European Summer Time|WEST]])}}\n|team1      = {{fbu-rt|21|FRO}}\n|score      = 1\u20133\n|team2      = {{fbu|21|KAZ}}\n|goals1     =\n*[[Stefan R