rs|season=2017\u201318\n| bg1 = #006BB6 | color1 = #FFFFFF\n| bg2 = #FDB927 | color2 = #26282A\n}}\n<!-- begin player list -->\n{{player2 | num = 2 | first = Jordan | last = Bell | pos = FC | ft = 6 | in = 9 | lbs = 224 | college = Oregon | DOB = 1995\u201301\u201307 }}\n{{player2 | num = 25 | first = Chris | last = Boucher | dab = basketball | pos = F | ft = 6 | in = 10 | lbs = 200 | college = Oregon | DOB = 1993\u201301\u201311 | note = TW }} \n{{player2 | num = 4 | first = Quinn | last = Cook | pos = G | ft = 6 | in = 2 | lbs = 179 | college = Duke | DOB = 1993\u201303\u201323 }}\n{{player2 | num = 30 | first = Stephen | last = Curry | pos = G | ft = 6 | in = 3 | lbs = 190 | college = Davidson | DOB = 1988\u201303\u201314 | note = C }}\n{{player2 | num = 35 | first = Kevin | last = Durant | pos = F | ft = 6 | in = 9 | lbs = 240 | college = Texas | DOB = 1988\u201309\u201329 }}\n{{player2 | num = 23 | first = Draymond | last = Green | pos = F | ft = 6 | in = 7 | lbs = 230 | college = Michigan State | DOB = 1990\u201303\u201304 }}\n{{player2 | num = 9 | first = Andre | last = Iguodala | pos = GF | ft = 6 | in = 6 | lbs = 215 | college = Arizona | DOB = 1984\u201301\u201328 }}\n{{player2 | num = 15 | first = Damian | last = Jones | dab = basketball | pos = C | ft = 7 | in = 0 | lbs = 245 | college = Vanderbilt | DOB = 1995\u201306\u201330 }}\n{{player2 | num = 34 | first = Shaun | last = Livingston | pos = G | ft = 6 | in = 7 | lbs = 192 | school = [[Peoria High School (Peoria, Illinois)|Peoria Central HS (IL)]] | DOB = 1985\u201309\u201311 }}\n{{player2 | num = 5 | first = Kevon | last = Looney | pos = FC | ft = 6 | in = 9 | lbs = 220 | college = UCLA | DOB = 1996\u201302\u201306}}\n{{player2 | num = 0 | first = Patrick | last = McCaw | pos = G | ft = 6 | in = 7 | lbs = 185 | college = UNLV | DOB = 1995\u201310\u201325 }}\n{{player2 | num = 1 | first = JaVale | last = McGee | pos = C | ft = 7 | in = 0 | lbs = 270 | college = Nevada | DOB = 1988\u201301\u201319 }}\n