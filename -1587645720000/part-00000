n| align=\"center\" bgcolor=\"e9e9e9\" |24\n|-\n!style=\"background-color:#\" align=\"center\" |\n|-\n!style=\"background-color:#\" align=\"center\" |2009\n| align=\"center\" bgcolor=\"e9e9e9\" |5\n| align=\"center\" bgcolor=\"e9e9e9\" |4\n| align=\"center\" bgcolor=\"e9e9e9\" |5\n| align=\"center\" bgcolor=\"e9e9e9\" |4\n| align=\"center\" bgcolor=\"e9e9e9\" |4\n| align=\"center\" bgcolor=\"e9e9e9\" |1\n| align=\"center\" bgcolor=\"e9e9e9\" |3\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |26\n|-\n!style=\"background-color:#\" align=\"center\" |\n|-\n!style=\"background-color:#\" align=\"center\" |2010\n| align=\"center\" bgcolor=\"e9e9e9\" |6\n| align=\"center\" bgcolor=\"e9e9e9\" |5\n| align=\"center\" bgcolor=\"e9e9e9\" |4\n| align=\"center\" bgcolor=\"e9e9e9\" |4\n| align=\"center\" bgcolor=\"e9e9e9\" |4\n| align=\"center\" bgcolor=\"e9e9e9\" |2\n| align=\"center\" bgcolor=\"e9e9e9\" |3\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |28\n|-\n!style=\"background-color:#\" align=\"center\" |\n|-\n!style=\"background-color:#\" align=\"center\" |2011\n| align=\"center\" bgcolor=\"e9e9e9\" |10\n| align=\"center\" bgcolor=\"e9e9e9\" |5\n| align=\"center\" bgcolor=\"e9e9e9\" |4\n| align=\"center\" bgcolor=\"e9e9e9\" |4\n| align=\"center\" bgcolor=\"e9e9e9\" |6\n| align=\"center\" bgcolor=\"e9e9e9\" |2\n| align=\"center\" bgcolor=\"e9e9e9\" |3\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |34\n|-\n!style=\"background-color:#\" align=\"center\" |\n|-\n!style=\"background-color:#\" align=\"center\" |2012\n| align=\"center\" bgcolor=\"e9e9e9\" |10\n| align=\"center\" bgcolor=\"e9e9e9\" |5\n| align=\"center\" bgcolor=\"e9e9e9\" |4\n| align=\"center\" bgcolor=\"e9e9e9\" |1\n| align=\"center\" bgcolor=\"e9e9e9\" |6\n| align=\"center\" bgcolor=\"e9e9e9\" |2\n| align=\"center\" bgcolor=\"e9e9e9\" |3\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |31\n|-\n!style=\"background-color:#\" align=\"center\" |\n|-\n!style=\"background-color:#\" align=\"center\" |2013\n| align=\"center\" bgcolor=\"e9e9e9\" |10\n| align=\"center\" bgcolor=\"e9e9e9\" |5\n| align=\"center\" bgcolor=\"e9e9e9\" |2\n| align=\"center\" bgcolor=\"e9e9e9\" |1\n| align=\"center\" bgcolor=\"e9e9e9\" |6\n| align=\"center\" bgcolor=\"e9e9e9\" |2\n| align=\"center\" bgcolor=\"e9e9e9\" |3\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |29\n|-\n!style=\"background-color:#\" align=\"center\" |\n|-\n!style=\"background-color:#\" align=\"center\" |2014\n| align=\"center\" bgcolor=\"e9e9e9\" |11\n| align=\"center\" bgcolor=\"e9e9e9\" |3\n| align=\"center\" bgcolor=\"e9e9e9\" |2\n| align=\"center\" bgcolor=\"e9e9e9\" |1\n| align=\"center\" bgcolor=\"e9e9e9\" |6\n| align=\"center\" bgcolor=\"e9e9e9\" |2\n| align=\"center\" bgcolor=\"e9e9e9\" |7\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |32\n|-\n!style=\"background-color:#\" align=\"center\" |\n|-\n!style=\"background-color:#\" align=\"center\" |2015\n| align=\"center\" bgcolor=\"e9e9e9\" |11\n| align=\"center\" bgcolor=\"e9e9e9\" |3\n| align=\"center\" bgcolor=\"e9e9e9\" |2\n| align=\"center\" bgcolor=\"e9e9e9\" |1\n| align=\"center\" bgcolor=\"e9e9e9\" |8\n| align=\"center\" bgcolor=\"e9e9e9\" |2\n| align=\"center\" bgcolor=\"e9e9e9\" |7\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |\n| align=\"center\" bgcolor=\"e9e9e9\" |34\n|-\n!style=\"background-color:#\" align=\"center\" |\n|-\n!style=\"background-color:#\" align=\"center\" |2016\n| align=\"center\" bgcolor=\"e9e9e9\" |10\n| align=\"center\" bgcolor=\"e9e9e9\" |3\n| align=\"center\" bgcolor=\"e9e9e9\"