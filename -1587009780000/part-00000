s1 = [[Michele Andreolo|Andreolo]] {{Goal|11}}<br />[[Amedeo Biavati|Biavati]] {{Goal|37}}<br />[[Ettore Puricelli|Puricelli]] {{Goal|74}}\n|goals2=\n|result=W\n}}\n{{footballbox collapsible\n|date = 12 March 1939\n|round = 22\n|team1 = [[A.C. Milan|Milan]]\n|team2 = Bologna\n|score = 0-1\n|report=\n|location = [[Milano]]\n|stadium  = [[Stadio Giuseppe Meazza|San Siro]]\n|attendance = 35 000\n|referee = Generoso Dattilo\n|goals2 = {{Goal|69}} [[Ettore Puricelli|Puricelli]]\n|result=W\n}}\n{{footballbox collapsible\n|date = 19 March 1939\n|round = 23\n|team1 = Bologna\n|team2 = Liguria\n|score = 1-1\n|report=\n|location = [[Bologna]]\n|stadium = [[Stadio Renato Dall'Ara|Stadio Littoriale]]\n|attendance = 10 000\n|referee = Giacomo Bertolio\n|goals1 = [[Amedeo Biavati|Biavati]] {{Goal|53}}\n|goals2 = {{Goal|34}} [[Elisio Gabardo|Gabardo]]\n|result=D\n}}\n{{footballbox collapsible\n|date = 2 April 1939\n|round = 24\n|team1 = [[Juventus]]\n|team2 = Bologna\n|score = 1-0\n|report=\n|location= [[Torino]]\n|stadium  = [[Stadio Olimpico Grande Torino|Stadio Benito Mussolini]]\n|attendance = 10 000\n|referee = Rinaldo Barlassina\n|goals1 = [[Giovanni Varglien|Varglien II]] {{Goal|64}}\n|result=L\n}}\n{{footballbox collapsible\n|date = 9 April 1939\n|round= 25\n|team1 = Bologna\n|team2 = Triestina\n|score = 2-0\n|report=\n|location = [[Bologna]]\n|stadium = [[Stadio Renato Dall'Ara|Stadio Littoriale]]\n|attendance= 7 500\n|referee = Emiliano Saracini\n|goals1 = [[Francisco Fedullo|Fedullo]] {{Goal|11}}<br />[[Amedeo Biavati|Biavati]] {{Goal|89}}\n|result=W\n}}\n{{footballbox collapsible\n|date = 16 April 1939\n|round = 26\n|team1 = [[Lucchese]]\n|team2 = Bologna\n|score = 1-1\n|report=\n|location = [[Lucca]]\n|stadium  = [[Stadio Porta Elisa]]\n|referee = Generoso Dattilo\n|goals1 = [[Luigi Rosellini|Rosellini]] {{Goal|50}}\n|goals2 = {{Goal|37}} [[Ettore Puricelli|Puricelli]]\n|result=D\n}}\n{{footballbox collapsible\n|date = 21 April 1939\n|round = 27\n|team1 = Bologna\n|te