).\n\n__TOC__\n{{-}}\n==Mainline highways==\n{{routelist top|length_ref=}}\n{{routelist row\n|state=OR\n|type=US\n|route=20 \n|terminus_a=[[US 101 (OR)|US 101]] in [[Newport, Oregon|Newport]]\n|terminus_b=[[US 20 (ID)|US 20]]/[[US 26 (ID)|US 26]] towards [[Boise, ID]]\n|established=1940\n|notes=none}}\n{{routelist row\n|state=OR\n|type=US\n|route=26\n|terminus_a=[[US 101 (OR)|US 101]] south of [[Seaside, Oregon|Seaside]]\n|terminus_b=[[US 20 (ID)|US 20]]/[[US 26 (ID)|US 26]] towards [[Boise, ID]]\n|established=1952\n|notes=none}}\n{{routelist row\n|state=OR\n|type=US 1948\n|route=28\n|terminus_a=[[OR 99]] in [[Eugene, OR|Eugene]]\n|terminus_b=Former [[US 30 (OR)|US 30]] in [[Ontario, Oregon|Ontario]]\n|established=1926\n|decommissioned=1952\n|notes=none}}\n{{routelist row\n|state=OR\n|type=US\n|route=30\n|terminus_a=[[US 101 (OR)|US 101]] in [[Astoria, Oregon|Astoria]]\n|terminus_b=[[US 30 (ID)|US 30]] in [[Fruitland, ID]]\n|established=1926\n|notes=none}}\n{{routelist row\n|state=OR\n|type=US\n|route=30N\n|terminus_a=Former [[US 30 (OR)|US 30]] south of [[Huntington, Oregon|Huntington]]\n|terminus_b=[[Weiser, ID]]\n|established=1927\n|decommissioned=1980\n|notes=none}}\n{{routelist row\n|state=OR\n|type=US\n|route=30S\n|terminus_a=[[US 30 (OR)|US 30]] in [[Ontario, OR|Ontario]]\n|terminus_b=Towards [[Parma, ID]]\n|established=1938\n|decommissioned=1940\n|notes=none}}\n{{routelist row\n|state=OR\n|type=US\n|route=95\n|terminus_a=[[US 95 (NV)|US 95]] towards [[Winnemucca, Nevada|Winnemucca, NV]]\n|terminus_b=[[US 95 (ID)|US 95]] towards [[Homedale, Idaho|Homedale, ID]]\n|established=1939\n|notes=none}}\n{{routelist row\n|state=OR\n|type=US\n|route=97\n|terminus_a=[[US 97 (CA)|US 97]] towards [[Weed, California|Weed, CA]]\n|terminus_b=[[US 97 (WA)|US 97]] towards [[Yakima, Washington|Yakima, WA]]\n|established=1926\n|notes=none}}\n{{routelist row\n|state=OR\n|type=US\n|route=99\n|terminus_a=Towards [[Weed, California|Weed, CA]]\n|terminus_b=[[Vancouver, WA]]\n|establi