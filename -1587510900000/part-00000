 |last=Griswold |rs=y |class=FR |pos=OT |link=n}}\n{{American football roster/Player|num=52 |first=Trey |last=Elliott |rs=y |class=FR |pos=OT |link=n}}\n{{American football roster/Player|num=68 |first=Joseph |last=Noteboom |rs=y |class=SO |pos=OT |link=y}}\n{{American football roster/Player|num=64 |first=Matt |last=Pryor |rs=y |class=SO |pos=OT |link=y|dab=Matt Pryor (American football)}}\n{{American football roster/Player|num=74 |first=Halapoulivaati |last=Vaitai |class=SR |pos=OT |link=y}}\n\n|defensive_players=\n\n{{American football roster/Player|num=94 |first=Josh |last=Carraway |rs=y |class=JR |pos=DE|link=y}}\n{{American football roster/Player|num=91 |first=L. J. |last=Collier |rs=y |class=FR |pos=DE|link=y}}\n{{American football roster/Player|num=47 |first=Tipa |last=Galeai |class=FR |pos=DE}}\n{{American football roster/Player|num=92 |first=Bryson |last=Henderson |rs=y |class=SO |pos=DE}}\n{{American football roster/Player|num=90 |first=Terrell |last=Lathan |class=SR |pos=DE}}\n{{American football roster/Player|num=40 |first=James |last=McFarland |class=SR |pos=DE |inj=y}}\n{{American football roster/Player|num=96 |first=Breylin |last=Mitchell |class=FR |pos=DE}}\n{{American football roster/Player|num=93 |first=Michael |last=Morsharrafa |rs=y |class=JR |pos=DE}}\n{{American football roster/Player|num=93 |first=Mike |last=Tuaua |class=SR |pos=DE}}\n\n{{American football roster/Player|num=66 |first=Pakamiaiaea |last=Davis |rs=y |class=FR |pos=DL}}\n\n{{American football roster/Player|num=97 |first=Chris |last=Bradley |class=SO |pos=DT}}\n{{American football roster/Player|num=54 |first=Joseph |last=Broadnax |class=FR |pos=DT}}\n{{American football roster/Player|num=95 |first=Aaron |last=Curry |class=JR |pos=DT |inj-y}}\n{{American football roster/Player|num=99 |first=Tevin |last=Lawson |class=JR |pos=DT |inj=y}}\n{{American football roster/Player|num=98 |first=Casey |last=McDermott Vai |rs=y |class=SR |pos=DT}}\n{{American football roster/Player|num=57 |first=