raguay}} || 18263 ||17445 || 27581 || 15481\n|- \n|{{Flag|Qatar}} || 161066 ||26844 || 174166 || 149149\n|- \n|{{Flag|Romania}} || 46300 ||8418 || 54442 || 63676\n|- \n|{{Flag|Russian Federation}} || 48847 ||33537 || 58904 || 46334\n|- \n|{{Flag|Rwanda}} || 3398 ||1357 || 8758 || 10077\n|- \n|{{Flag|Saudi Arabia}} || 130050 ||55431 || 258381 || 95528\n|- \n|{{Flag|Sudan}} || 17780 ||20984 || 2277 || 22263\n|- \n|{{Flag|Senegal}} || 7883 ||2533 || 9197 || 17313\n|- \n|{{Flag|Singapore}} || 143706 ||- || 219341 || 128396\n|- \n|{{Flag|Sierra Leone}} || 3524 ||3117 || 2573 || 4880\n|- \n|{{Flag|El Salvador}} || 18844 ||11009 || 24908 || 19278\n|- \n|{{Flag|Serbia}} || 29902 ||12639 || 38323 || 32247\n|- \n|{{Flag|Sao Tome and Principe}} || 9964 ||5459 || 10990 || 11378\n|- \n|{{Flag|Suriname}} || 41917 ||129562 || 51888 || 34950\n|- \n|{{Flag|Slovak Republic}} || 63448 ||70912 || 62186 || 63758\n|- \n|{{Flag|Slovenia}} || 64868 ||16395 || 69108 || 70032\n|- \n|{{Flag|Sweden}} || 91977 ||67773 || 121153 || 85845\n|- \n|{{Flag|Swaziland}} || 32982 ||15222 || 74356 || 27900\n|- \n|{{Flag|Chad}} || 5787 ||3964 || 39131 || 9032\n|- \n|{{Flag|Togo}} || 3063 ||1976 || 6304 || 4421\n|- \n|{{Flag|Thailand}} || 26449 ||6519 || 42980 || 34327\n|- \n|{{Flag|Tajikistan}} || 6707 ||2906 || 14558 || 10782\n|- \n|{{Flag|Timor-Leste}} || 9775 ||3448 || 102094 || 12871\n|- \n|{{Flag|Tonga}} || 13664 ||8166 || 9501 || 21548\n|- \n|{{Flag|Trinidad and Tobago}} || 65021 ||7741 || 83658 || 59477\n|- \n|{{Flag|Tunisia}} || 34898 ||32259 || 32177 || 36804\n|- \n|{{Flag|Turkey}} || 58400 ||22220 || 66593 || 68340\n|- \n|{{Flag|Tanzania}} || 5741 ||2671 || 24058 || 9318\n|- \n|{{Flag|Uganda}} || 3912 ||1404 || 11845 || 10140\n|- \n|{{Flag|Ukraine}} || 15708 ||14579 || 16280 || 15734\n|- \n|{{Flag|Uruguay}} || 41998 ||33408 || 59207 || 38077\n|- \n|{{Flag|United States}} || 111561 ||81811 || 127498 || 108669\n|- \n|{{Flag|Uzbekistan}} || 14140 ||8550 || 20497 || 14508\n|- \n|{{Flag|St. Vincent a