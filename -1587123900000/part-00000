icon|Brazil|1968}} || bgcolor=gold| {{Flagicon|Brazil|1968}} ||  || ||  || bgcolor=gold| {{Flagicon|Brazil}} || bgcolor=gold| {{Flagicon|Argentina}} || bgcolor=gold| {{Flagicon|Argentina}} ||  || style=\"border:3px solid red\" bgcolor=gold| {{Flagicon|Argentina}} || bgcolor=gold| {{Flagicon|Brazil}} || bgcolor=gold| {{Flagicon|Argentina}} || bgcolor=gold| {{Flagicon|Argentina}} ||  || bgcolor=gold| {{Flagicon|Brazil}} ||  ||  ||  ||  || '''11'''\n|- \n|bgcolor=silver| 2nd ||  ||  ||  || bgcolor=silver| {{Flagicon|Argentina}} ||  ||  ||  || bgcolor=silver| {{Flagicon|Brazil|1968}} ||  || bgcolor=silver| {{Flagicon|Brazil}} || bgcolor=silver| {{Flagicon|Uruguay}} ||  ||  ||  ||  ||  || bgcolor=silver| {{Flagicon|Brazil}} ||  || bgcolor=silver| {{Flagicon|Uruguay}} || bgcolor=silver| {{Flagicon|Brazil}} || bgcolor=silver| {{Flagicon|Venezuela}} ||  || '''8'''\n|-\n|bgcolor=#cc9966| 3rd || bgcolor=#cc9966| {{Flagicon|Brazil|1968}} || bgcolor=#cc9966| {{Flagicon|Uruguay}} ||  ||  ||  ||  || bgcolor=#cc9966| {{Flagicon|Brazil|1968}} ||  ||  ||  ||  ||  ||  || bgcolor=#cc9966| {{Flagicon|Colombia}} || bgcolor=#cc9966| {{Flagicon|Brazil}} || bgcolor=#cc9966| {{Flagicon|Chile}} ||  ||  ||  ||  ||  || bgcolor=#cc9966| {{Flagicon|Ecuador}} || '''7'''\n|-\n|bgcolor=#9acdff| 4th || bgcolor=#9acdff| {{Flagicon|Uruguay}} ||  ||  ||  ||  || style=\"border:3px solid red\" bgcolor=#9acdff| {{Flagicon|Chile}} ||  ||  ||  ||  ||  || bgcolor=#9acdff| {{Flagicon|Uruguay}} || bgcolor=#9acdff| {{Flagicon|Paraguay|1990}} || bgcolor=#9acdff| {{Flagicon|Argentina}} ||  ||  ||  ||  ||  ||  || bgcolor=#9acdff| {{Flagicon|Uruguay}} ||  || '''6'''\n|}\n\n===OFC===\n{| class=\"wikitable\" style=\"text-align: center\"\n|- valign=top\n!\n![[1977 FIFA World Youth Championship|1977]]<br>{{Flagicon|Tunisia|1959}}<br>(16)\n![[1979 FIFA World Youth Championship|1979]]<br>{{Flagicon|Japan|1947}}<br>(16)\n!style=\"border:3px solid red\"|[[1981 FIFA World Youth Championship|1981]]<br>{{Flagicon|Australia}}<