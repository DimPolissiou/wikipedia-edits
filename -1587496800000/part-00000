al|num=0|pos=F|ft=6|in=8|lbs=235|class=SR|rs=|home=[[Natchitoches, Louisiana|Natchitoches, LA]]|high_school=[[Natchitoches Central High School|Natchitoches Central HS]]<br>[[Pearl River Community College|Pearl River CC]]}}\n{{CBB roster/Player|first=Keyshawn|last=Embery-Simpson|num=2|pos=G|ft=6|in=3|lbs=200|class=SO|rs=y|home=[[Midwest City, Oklahoma|Midwest City, OK]]|high_school=[[Midwest City High School|Midwest City HS]]<br>[[Arkansas Razorbacks men's basketball|Arkansas]]}}\n{{CBB roster/Player|first=Elijah|last=Joiner|num=3|pos=G|ft=6|in=3|lbs=200|class=SR|rs=|home=[[Chicago|Chicago, IL]]|high_school=[[Curie Metropolitan High School|Curie Metropolitan HS]]}}\n{{CBB roster/Player|first=Darien|last=Jackson|num=11|pos=G|ft=6|in=3|lbs=189|class=SR|rs=|home=[[Overland Park, Kansas|Overland Park, KS]]|high_school=[[Blue Valley Northwest High School|Blue Valley Northwest HS]]}}\n{{CBB roster/Player|first=Emmanuel|last=Ugboh|num=12|pos=C|ft=7|in=0|lbs=245|class=SR|rs=|home=[[Lagos|Lagos, Nigeria]]|high_school=Quality Education Academy<br>[[Iowa Western Community College|Iowa Western CC]]}}\n{{CBB roster/Player|first=George|last=Christopoulos|num=13|pos=G|ft=6|in=0|lbs=167|class=SO|rs=y|home=[[Nea Smyrni|Nea Smyrni, Greece]]|high_school=[[Warner Christian Academy]]|note=W}}\n{{CBB roster/Player|first=Josh|last=Earley|num=15|pos=F|ft=6|in=7|lbs=210|class=SO|rs=|home=[[Shawnee Mission, Kansas|Shawnee Mission, KS]]|high_school=[[Bishop Miege High School|Bishop Miege HS]]}}\n{{CBB roster/Player|first=Ryan|last=Gendron|num=23|pos=G|ft=6|in=4|lbs=200|class=SO|rs=|home=[[Tulsa, Oklahoma|Tulsa, OK]]|high_school=[[Bishop Kelley High School|Bishop Kelley HS]]<br>[[Oklahoma State Cowboys baseball|Oklahoma State]]|note=W}}\n{{CBB roster/Player|first=Curtis|last=Haywood|num=33|pos=G|ft=6|in=5|lbs=200|class=JR|rs=y|home=[[Oklahoma City|Oklahoma City, OK]]|high_school=[[Brewster Academy]]<br>[[Georgia Tech Yellow Jackets men's basketball|Georgia Tech]]}}\n{{CBB roster/Footer\n|head_c