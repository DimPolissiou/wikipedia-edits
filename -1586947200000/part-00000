F=2\u20130\n|match_LSK_TIL=0\u20133\n|match_LSK_VIK=4\u20131\n|match_LSK_VIF=0\u20131\n|match_LSK_AAL=7\u20130\n\n|name_LYN=[[Lyn Fotball|Lyn]]\n|match_LYN_SKB=6\u20130\n|match_LYN_FFK=0\u20130\n|match_LYN_LSK=0\u20134\n|match_LYN_OG=3\u20131\n|match_LYN_RBK=2\u20131\n|match_LYN_SF=3\u20130\n|match_LYN_STB=3\u20132\n|match_LYN_IKS=4\u20131\n|match_LYN_SIF=1\u20130\n|match_LYN_TIL=4\u20133\n|match_LYN_VIK=2\u20133\n|match_LYN_VIF=[[Battle of Oslo (football)|3\u20131]]\n|match_LYN_AAL=0\u20130\n\n|name_OG=[[Odd Grenland]]\n|match_OG_SKB=0\u20132\n|match_OG_FFK=2\u20130\n|match_OG_LSK=0\u20131\n|match_OG_LYN=2\u20130\n|match_OG_RBK=1\u20132\n|match_OG_SF=4\u20130\n|match_OG_STB=1\u20135\n|match_OG_IKS=2\u20130\n|match_OG_SIF=0\u20131\n|match_OG_TIL=0\u20131\n|match_OG_VIK=1\u20133\n|match_OG_VIF=1\u20134\n|match_OG_AAL=3\u20130\n\n|name_RBK=[[Rosenborg BK|Rosenborg]]\n|match_RBK_SKB=3\u20130\n|match_RBK_FFK=1\u20131\n|match_RBK_LSK=1\u20131\n|match_RBK_LYN=3\u20130\n|match_RBK_OG=4\u20131\n|match_RBK_SF=2\u20130\n|match_RBK_STB=3\u20130\n|match_RBK_IKS=4\u20131\n|match_RBK_SIF=1\u20132\n|match_RBK_TIL=4\u20133\n|match_RBK_VIK=4\u20132\n|match_RBK_VIF=2\u20130\n|match_RBK_AAL=2\u20132\n\n|name_SF=[[Sandefjord Fotball|Sandefjord]]\n|match_SF_SKB=2\u20133\n|match_SF_FFK=2\u20132\n|match_SF_LSK=0\u20132\n|match_SF_LYN=3\u20131\n|match_SF_OG=0\u20132\n|match_SF_RBK=1\u20132\n|match_SF_STB=2\u20133\n|match_SF_IKS=1\u20133\n|match_SF_SIF=0\u20131\n|match_SF_TIL=2\u20131\n|match_SF_VIK=4\u20131\n|match_SF_VIF=1\u20133\n|match_SF_AAL=3\u20130\n\n|name_STB=[[Stab\u00e6k Fotball|Stab\u00e6k]]\n|match_STB_SKB=0\u20131\n|match_STB_FFK=0\u20132\n|match_STB_LSK=3\u20131\n|match_STB_LYN=3\u20132\n|match_STB_OG=1\u20130\n|match_STB_RBK=2\u20131\n|match_STB_SF=1\u20131\n|match_STB_IKS=1\u20131\n|match_STB_SIF=3\u20132\n|match_STB_TIL=5\u20130\n|match_STB_VIK=2\u20131\n|match_STB_VIF=2\u20132\n|match_STB_AAL=4\u20132\n\n|name_IKS=[[IK Start|Start]]\n|match_IKS_SKB=1\u20131\n|match_IKS_FFK=3\u20131\n|match_IKS_LSK=0\u20130\n|match_IKS_LYN=0\u20131\n|match_IKS_OG=1\u20132\n|match_IKS_RBK=2\u20131\n|match_IKS_SF=3\u20131\n|match_IKS_STB=1\u20131\n|match_IKS_SIF=2\u20133\n|match_IKS_TIL=1\u20131\n|match_IKS_VIK=1\u20131\n|match_IKS_VIF=1\u20130\n|match_IKS_AAL=1\u20132\n\n|name_SIF=[[Str\u00f8msgodset IF|Str\u00f8msgodset]]\n|match_SIF_SKB=2\u20134\n|match_SIF_FFK=2\u20131\n|match_SIF_LSK=1\u20131\n|match_SIF_LYN=1\u20131\n|match_SIF_OG=2\u20131\n|match_SIF_RBK=1\u20131\n|match_SIF_SF=4\u20130\n|match_SIF_STB=0\u20135\n|match_SIF_IKS=3\u20132\n|match_SIF_TIL=1\u20132\n|match_SIF_VIK=1\u20132\n|match_SIF_VIF=1\u20132\n|match_SIF_AAL=0\u20134\n\n|name_TIL=[[Troms\u00f8 IL|Troms\u00f8]]\n|match_TIL_SKB=3\u20130\n|match_TIL_FFK=4\u20131\n|match_TIL_LSK=2\u20131\n|match_TIL_LYN=2\u20131\n|match_TIL_OG=2\u20134\n|match_TIL_RBK=2\u20131\n|match_TIL_SF=2\u20131\n|match_TIL_STB=0\u20131\n|match_TIL_IKS=1\u20131\n|match_TIL_SIF=2\u20132\n|match_TIL_VIK=2\u20131\n|match_TIL_VIF=1\u20130\n|match_TIL_AAL=0\u20132\n\n|name_VIK=[[Viking FK|Viking]]\n|match_VIK_SKB=3\u20131\n|match_VIK_FFK=2\u20132\n|match_VIK_LSK=1\u20131\n|match_VIK_LYN=4\u20132\n|match_VIK_OG=2\u20131\n|match_VIK_RBK=1\u20131\n|match_VIK_SF=0\u20130\n|match_VIK_STB=2\u20131\n|match_VIK_IKS=2\u20130\n|match_VIK_SIF=3\u20130\n|match_VIK_TIL=3\u20131\n|match_VIK_VIF=2\u20131\n|match_VIK_AAL=3\u20130\n\n|name_VIF=[[V\u00e5lerenga Fotball|V\u00e5lerenga]]\n|match_VIF_SKB=2\u20130\n|match_VIF_FFK=2\u20130\n|match_VIF_LSK=1\u20133\n|match_VIF_LYN=[[Battle of Oslo (football)|0\u20130]]\n|match_VIF_OG=1\u20131\n|match_VIF_RBK=2\u20131\n|match_VIF_SF=2\u20130\n|match_VIF_STB=1\u20131\n|match_VIF_IKS=3\u20132\n|match_VIF_SIF=1\u20131\n|match_VIF_TIL=2\u20132\n|match_VIF_VIK=1\u20133\n|match_VIF_AAL=2\u20131\n\n|name_AAL=[[Aalesunds FK|Aalesund]]\n|match_AAL_SKB=2\u20131\n|match_AAL_FFK=3\u20133\n|match_AAL_LSK=1\u20133\n|match_AAL_LYN=3\u20131\n|match_AAL_OG=2\u20131\n|match_AAL_RBK=1\u20132\n|match_AAL_SF=4\