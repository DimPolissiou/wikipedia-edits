0130\n\n|name_VIF=[[V\u00e5lerenga Fotball|V\u00e5lerenga]]\n|match_VIF_SKB=2\u20130\n|match_VIF_FFK=2\u20130\n|match_VIF_LSK=1\u20133\n|match_VIF_LYN=[[Battle of Oslo (football)|0\u20130]]\n|match_VIF_OG=1\u20131\n|match_VIF_RBK=2\u20131\n|match_VIF_SF=2\u20130\n|match_VIF_STB=1\u20131\n|match_VIF_IKS=3\u20132\n|match_VIF_SIF=1\u20131\n|match_VIF_TIL=2\u20132\n|match_VIF_VIK=1\u20133\n|match_VIF_AAL=2\u20131\n\n|name_AAL=[[Aalesunds FK|Aalesund]]\n|match_AAL_SKB=2\u20131\n|match_AAL_FFK=3\u20133\n|match_AAL_LSK=1\u20133\n|match_AAL_LYN=3\u20131\n|match_AAL_OG=2\u20131\n|match_AAL_RBK=1\u20132\n|match_AAL_SF=4\u20131\n|match_AAL_STB=1\u20134\n|match_AAL_IKS=2\u20134\n|match_AAL_SIF=3\u20132\n|match_AAL_TIL=2\u20133\n|match_AAL_VIK=0\u20132\n|match_AAL_VIF=1\u20130\n\n|update=complete|source=[http://idrett.speaker.no/07/ResultatService.aspx?0=7&1=Fotball&2=Norge&3=2008%20-%202008&4=277300&5=&6=&7=&8= speaker.no]\n}}\n\n==Season statistics==\n===Top scorers===\n{|bgcolor=\"#f7f8ff\" cellpadding=\"3\" cellspacing=\"0\" border=\"1\" style=\"font-size: 95%; width: 35em; border: gray solid 1px; border-collapse: collapse;\"\n|- bgcolor=\"#CCCCCC\"\n! width=\"50px\" |Position\n! Player\n! Club\n! width=\"50px\" | Goals\n|- bgcolor=\"#EFEFEF\"\n|align=\"center\"|1\n| {{flagicon|NOR}} [[Thorstein Helstad]]\n| [[SK Brann|Brann]]\n|align=\"center\"|'''22'''\n|- bgcolor=\"#EFEFEF\"\n|align=\"center\"|2\n| {{flagicon|SWE}} [[Daniel Nannskog]] \n| [[Stab\u00e6k Fotball|Stab\u00e6k]]\n|align=\"center\"|'''19'''\n|- bgcolor=\"#EFEFEF\"\n|align=\"center\"|3\n| {{flagicon|NGA}} [[Peter Ijeh]] \n| [[Viking FK|Viking]]\n|align=\"center\"|'''18'''\n|- bgcolor=\"#EFEFEF\"\n|align=\"center\"|4\n| {{flagicon|ISL}} [[Veigar P\u00e1ll Gunnarsson]] \n| [[Stab\u00e6k Fotball|Stab\u00e6k]]\n|align=\"center\"|'''15'''\n|- bgcolor=\"#EFEFEF\"\n|align=\"center\"|5\n| {{flagicon|NOR}} [[Steffen Iversen]]\n| [[Rosenborg BK|Rosenborg]]\n|align=\"center\"|'''13'''\n|- bgcolor=\"#EFEFEF\"\n|align=\"cent