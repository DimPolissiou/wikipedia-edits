32\n|match_BHO_STE=1\u20133\n|match_BHO_UCL=3\u20131\n|match_BHO_UCR=0\u20132\n\n|name_BRA=[[FC Bra\u0219ov|Bra\u0219ov]]\n|match_BRA_COR=2\u20130\n|match_BRA_DUB=3\u20130\n|match_BRA_DIN=1\u20131\n|match_BRA_ARG=1\u20130\n|match_BRA_BAC=2\u20130\n|match_BRA_BHO=2\u20131\n|match_BRA_FAR=4\u20130\n|match_BRA_GBI=2\u20131\n|match_BRA_INT=3\u20130\n|match_BRA_JIU=4\u20131\n|match_BRA_PET=1\u20130\n|match_BRA_POL=4\u20131\n|match_BRA_RAP=1\u20131\n|match_BRA_SPO=3\u20130\n|match_BRA_STE=1\u20131\n|match_BRA_UCL=2\u20131\n|match_BRA_UCR=1\u20131\n\n|name_FAR=[[FC Farul Constan\u021ba|Farul Constan\u021ba]]\n|match_FAR_COR=3\u20130\n|match_FAR_DUB=3\u20131\n|match_FAR_DIN=0\u20130\n|match_FAR_ARG=3\u20131\n|match_FAR_BAC=0\u20130\n|match_FAR_BHO=3\u20131\n|match_FAR_BRA=0\u20130\n|match_FAR_GBI=1\u20131\n|match_FAR_INT=2\u20130\n|match_FAR_JIU=0\u20130\n|match_FAR_PET=3\u20130\n|match_FAR_POL=1\u20130\n|match_FAR_RAP=2\u20131\n|match_FAR_SPO=2\u20131\n|match_FAR_STE=0\u20130\n|match_FAR_UCL=1\u20130\n|match_FAR_UCR=1\u20132\n\n|name_GBI=[[ACF Gloria Bistri\u021ba|Gloria Bistri\u021ba]]\n|match_GBI_COR=4\u20131\n|match_GBI_DUB=1\u20130\n|match_GBI_DIN=1\u20131\n|match_GBI_ARG=0\u20130\n|match_GBI_BAC=2\u20131\n|match_GBI_BHO=3\u20131\n|match_GBI_BRA=3\u20132\n|match_GBI_FAR=2\u20130\n|match_GBI_INT=1\u20130\n|match_GBI_JIU=5\u20130\n|match_GBI_PET=2\u20131\n|match_GBI_POL=3\u20132\n|match_GBI_RAP=5\u20130\n|match_GBI_SPO=0\u20130\n|match_GBI_STE=1\u20132\n|match_GBI_UCL=2\u20131\n|match_GBI_UCR=1\u20130\n\n|name_INT=[[FC Inter Sibiu|Inter Sibiu]]\n|match_INT_COR=5\u20132\n|match_INT_DUB=4\u20130\n|match_INT_DIN=2\u20130\n|match_INT_ARG=3\u20131\n|match_INT_BAC=2\u20130\n|match_INT_BHO=2\u20133\n|match_INT_BRA=2\u20130\n|match_INT_FAR=2\u20131\n|match_INT_GBI=2\u20131\n|match_INT_JIU=3\u20132\n|match_INT_PET=4\u20132\n|match_INT_POL=3\u20130\n|match_INT_RAP=1\u20130\n|match_INT_SPO=0\u20130\n|match_INT_STE=1\u20132\n|match_INT_UCL=2\u20130\n|match_INT_UCR=3\u20130\n\n|name_