orom || 12 || 8 || 20 ||rowspan=3| 40.25 ||rowspan=3| 27 ||rowspan=3| '''67.25'''\n|-\n|align=left| {{sortname|Manuel|Ordovas}} ||align=left| Bohemio || 8 || 12 || 20\n|-\n|align=left| {{sortname|Marcelino|Gavil\u00e1n}} ||align=left| Quoniam || 20.25 || 7 || 27.25\n|-\n|rowspan=3| 11 ||rowspan=3 align=left| {{flagIOC|SWE|1952 Summer}} ||align=left| {{sortname|Gunnar|Palm|dab=equestrian}} ||align=left| Lurifax || 12 || 8 || 20 ||rowspan=3| 36 ||rowspan=3| 44 ||rowspan=3| '''80'''\n|-\n|align=left| {{sortname|B\u00f6rje|Jeppson}} ||align=left| Spitfire || 12 || 16 || 28\n|-\n|align=left| {{sortname|Carl-Jan|Hamilton}} ||align=left| Halali || 12 || 20 || 32\n|-\n|rowspan=3| 12 ||rowspan=3 align=left| {{flagIOC|EGY|1952 Summer}} ||align=left| {{sortname|Mohamed|Khairy|dab=equestrian}} ||align=left| Inch'Allah || 8 || 8 || 16 ||rowspan=3| 44.25 ||rowspan=3| 36 ||rowspan=3| '''80.25'''\n|-\n|align=left| {{sortname|Gamal|Haress}} ||align=left| Sakr || 8 || 16 || 24\n|-\n|align=left| {{sortname|Mohamed Selim|Zaki}} ||align=left| Sali al Nabi || 28.25 || 12 || 40.25\n|-\n|rowspan=3| 13 ||rowspan=3 align=left| {{flagIOC|ROU|1952 Summer}} ||align=left| {{sortname|Gheorghe|Antohi}} ||align=left| Haimana || 16 || 22.25 || 38.25 ||rowspan=3| 94 ||rowspan=3| 86.25 ||rowspan=3| '''180.25'''\n|-\n|align=left| {{sortname|Ion|Jipa}} ||align=left| Troika || 8 || 31 || 39\n|-\n|align=left| {{sortname|Ion|Constantin}} ||align=left| Vagabond || 70 || 33 ||103\n|-\n|rowspan=3| 14 ||rowspan=3 align=left| {{flagIOC|URS|1952 Summer}} ||align=left| {{sortname|Mikhail|Vlasov}} ||align=left| Rota || 40 || 16 || 56 ||rowspan=3| 104.75 ||rowspan=3| 97 ||rowspan=3| '''201.75'''\n|-\n|align=left| {{sortname|Nikolay|Shelenkov}} ||align=left| Atiger || 41.75 || 20.75 || 62.5\n|-\n|align=left| {{sortname|Gavriil|Budyonny}} ||align=left| Yeger || 23 || 60.25 || 83.25\n|-\n|rowspan=3| \u2013 ||rowspan=3 align=left| {{flagIOC|FIN|1952 Summer}} ||align=left| {{sortname|Mauno|Roiha}} ||align=left| Roa || 3