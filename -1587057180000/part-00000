allas\n}}\n{{football box collapsible\n|result = W\n|nobars = 1\n|date = <span style=\"font-size:93%\">25 February 2017</span>\n|time = 15:00\n|round = 26\n|score = 0\u20132\n|report = [http://spfl.co.uk/premiership/fixture/3638039/ SPFL Report]\n|team1 = [[St Johnstone F.C.|St Johnstone]]\n|team2 = [[Kilmarnock F.C.|Kilmarnock]]\n|goals1 =\n|goals2 = [[Rory McKenzie|McKenzie]] {{goal|11}}<br />[[Conor Sammon|Sammon]] {{goal|71}}\n|stadium = [[McDiarmid Park]], [[Perth, Scotland|Perth]]\n|attendance = 2,933\n|referee = Don Robertson\n}}\n{{football box collapsible\n|result = L\n|nobars = 1\n|date = <span style=\"font-size:93%\">4 March 2017</span>\n|time = 15:00\n|round = 27\n|score = 1\u20132\n|report = [http://spfl.co.uk/premiership/fixture/3638044/ SPFL Report]\n|team1 = [[Kilmarnock F.C.|Kilmarnock]]\n|team2 = [[Motherwell F.C.|Motherwell]]\n|goals1 = [[Kris Boyd|K Boyd]] {{goal|32}}\n|goals2 = [[Carl McHugh|McHugh]] {{goal|51}}<br />[[Zak Jules|Jules]] {{goal|76}}\n|stadium = [[Rugby Park]], [[Kilmarnock]]\n|attendance = 4,726\n|referee = [[Bobby Madden]]\n}}\n{{football box collapsible\n|result = W\n|nobars = 1\n|date = <span style=\"font-size:93%\">11 March 2017</span>\n|time = 15:00\n|round = 28\n|score = 1\u20132\n|report = [http://spfl.co.uk/premiership/fixture/3638050/ SPFL Report]\n|team1 = [[Ross County F.C.|Ross County]]\n|team2 = [[Kilmarnock F.C.|Kilmarnock]]\n|goals1 = [[Liam Boyce|Boyce]] {{goal|58}}\n|goals2 = [[Conor Sammon|Sammon]] {{goal|46}}<br />[[Kris Boyd|K Boyd]] {{goal|85}}\n|stadium = [[Victoria Park, Dingwall]]\n|attendance = 3,380\n|referee = Nick Walsh\n}}\n{{football box collapsible\n|result = D\n|nobars = 1\n|date = <span style=\"font-size:93%\">18 March 2017</span>\n|time = 15:00\n|round = 29\n|score = 1\u20131\n|report = [http://spfl.co.uk/premiership/fixture/3638055/ SPFL Report]\n|team1 = [[Kilmarnock F.C.|Kilmarnock]]\n|team2 = [[Partick Thistle F.C.|Partick Thistle]]\n|goals1 = [[Conor Sammon|Sammon]] {{goal|87}}\n|goals2 = [[