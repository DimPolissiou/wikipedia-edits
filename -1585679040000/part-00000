ar \u0916\u093e\u0928, \u0928\u0947  \u0935\u093f\u0926\u094d\u0930\u094b\u0939 \u0915\u093f\u092f\u093e \u0925\u093e, \u0914\u0930 \u0907\u0938 \u0915\u0947 \u0924\u0939\u0924 \u0930\u093e\u091c\u093e Baram \u0938\u093f\u0902\u0939 \u092c\u093e\u0932\u0940, \u091c\u094b\u0915\u093f \u0938\u092e\u094d\u0930\u093e\u091f \u0915\u0940 \u0938\u0947\u0935\u093e \u092e\u0947\u0902 \u0925\u0947,  \u090f\u0915 \u092c\u0948\u0938 \u0930\u093f\u0938\u093e\u0932\u0926\u093e\u0930 \u0925\u0947 \u090f\u0915 \u0936\u093e\u0939\u0940 \u092c\u0932 \u0915\u0947 \u0938\u093e\u0925 awarkhan \u092a\u0930 \u0939\u092e\u0932\u093e \u0915\u093f\u092f\u093e \u0925\u093e\u0964 \u0930\u093e\u091c\u093e \u0938\u092b\u0932 \u0930\u0939\u093e \u0925\u093e; \u0915\u093f\u0932\u0947 \u0932\u093f\u092f\u093e \u0917\u092f\u093e \u0925\u093e, Awar \u0916\u093e\u0928 \u0915\u0940 \u092e\u094c\u0924 \u0939\u094b \u0917\u0908, \u0914\u0930 \u092a\u0920\u093e\u0928 \u0915\u094b \u0928\u093f\u0937\u094d\u0915\u093e\u0938\u093f\u0924 \u0915\u0930 \u0926\u093f\u092f\u093e\u0964 \u0907\u0938 \u091c\u0940\u0924 \u0915\u0947 \u0932\u093f\u090f \u0907\u0928\u093e\u092e \u092e\u0947\u0902 \u0909\u0928\u094d\u0939\u094b\u0902\u0928\u0947 \u0907\u0915\u0939\u0924\u094d\u0924\u0930 \u0917\u093e\u0902\u0935\u094b\u0902 \u0938\u0947 \u092e\u093f\u0932\u0915\u0930 \u090f\u0938\u094d\u091f\u0947\u091f \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0915\u093f\u092f\u093e\u0964 \u0935\u0939 \u0905\u092a\u0928\u0947 \u092c\u0947\u091f\u0947, Bhikam \u0938\u093f\u0902\u0939 \u0915\u094b \u0930\u093e\u091c\u093e \u0928\u093f\u092f\u0941\u0915\u094d\u0924 \u0915\u093f\u092f\u093e \u091c\u094b\u0915\u093f \u0935\u0930\u094d\u0924\u092e\u093e\u0928 taluqdar \u0915\u0947 \u092a\u0942\u0930\u094d\u0935\u091c \u0939\u0948, Bahrelias \u0938\u0902\u0916\u094d\u092f\u093e \u092e\u0947\u0902 \u0935\u0943\u0926\u094d\u0927\u093f \u0914\u0930 \u0938\u092e\u0943\u0926\u094d\u0927\u093f \u0939\u0941\u0908 \u0914\u0930 \u0928\u0935\u093e\u