{{chset-cell|0038|[[8 (number)|8]]|56}}\n|{{chset-color-digit}}|{{chset-cell|0039|[[9 (number)|9]]|57}}\n|{{chset-color-punct}}|{{chset-cell|003A|[[colon (punctuation)|&#x3A;]]|58}}\n|{{chset-color-punct}}|{{chset-cell|003B|[[semicolon|&#x3B;]]|59}}\n|{{chset-color-graph}}|{{chset-cell|003C|[[less-than sign|&lt;]]|60}}\n|{{chset-color-graph}}|{{chset-cell|003D|[[equal sign|&#x3D;]]|61}}\n|{{chset-color-graph}}|{{chset-cell|003E|[[greater-than sign|&gt;]]|62}}\n|{{chset-color-punct}}|{{chset-cell|003F|[[question mark|&#x3F;]]|63}}\n|-\n!{{chset-left2|4_<br/>64}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|010C/0427|[[\u010c]]/[[\u0427]]|64}}\n|{{chset-color-alpha}}|{{chset-cell|0041/0410|[[A]]/[[A (Cyrillic)|\u0410]]|65}}\n|{{chset-color-alpha}}|{{chset-cell|0042/0411|[[B]]/[[Be (Cyrillic)|\u0411]]|66}}\n|{{chset-color-alpha}}|{{chset-cell|0043/0426|[[C]]/[[Tse (Cyrillic)|\u0426]]|67}}\n|{{chset-color-alpha}}|{{chset-cell|0044/0414|[[D]]/[[De (Cyrillic)|\u0414]]|68}}\n|{{chset-color-alpha}}|{{chset-cell|0045/0415|[[E]]/[[Ye (Cyrillic)|\u0415]]|69}}\n|{{chset-color-alpha}}|{{chset-cell|0046/0424|[[F]]/[[Ef (Cyrillic)|\u0424]]|70}}\n|{{chset-color-alpha}}|{{chset-cell|0047/0413|[[G]]/[[Ge (Cyrillic)|\u0413]]|71}}\n|{{chset-color-alpha}}|{{chset-cell|0048/0425|[[H]]/[[Kha (Cyrillic)|\u0425]]|72}}\n|{{chset-color-alpha}}|{{chset-cell|0049/0418|[[I]]/[[I (Cyrillic)|\u0418]]|73}}\n|{{chset-color-alpha}}|{{chset-cell|004A/0408|[[J]]/[[Je (Cyrillic)|\u0408]]|74}}\n|{{chset-color-alpha}}|{{chset-cell|004B/041A|[[K]]/[[Ka (Cyrillic)|\u041a]]|75}}\n|{{chset-color-alpha}}|{{chset-cell|004C/041B|[[L]]/[[El (Cyrillic)|\u041b]]|76}}\n|{{chset-color-alpha}}|{{chset-cell|004D/041C|[[M]]/[[Em (Cyrillic)|\u041c]]|77}}\n|{{chset-color-alpha}}|{{chset-cell|004E/041D|[[N]]/[[En (Cyrillic)|\u041d]]|78}}\n|{{chset-color-alpha}}|{{chset-cell|004F/041E|[[O]]/[[O (Cyrillic)|\u041e]]|79}}\n|-\n!{{chset-left2|5_<br/>80}}\n|{{chset-color-alpha}}|{{chset-cell|0050/041F|[[P]]/[[Pe (Cyrillic)|\u041f]]|80}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0051/040C|[[Q]]/[[\u040c]]|81}}\n|{{chset-color-alpha}}|{{chset-cell|0052/0420|[[R]]/[[Er (Cyrillic)|\u0420]]|82}}\n|{{chset-color-alpha}}|{{chset-cell|0053/0421|[[S]]/[[Es (Cyrillic)|\u0421]]|83}}\n|{{chset-color-alpha}}|{{chset-cell|0054/0422|[[T]]/[[Te (Cyrillic)|\u0422]]|84}}\n|{{chset-color-alpha}}|{{chset-cell|0055/0423|[[U]]/[[U (Cyrillic)|\u0423]]|85}}\n|{{chset-color-alpha}}|{{chset-cell|0056/0412|[[V]]/[[Ve (Cyrillic)|\u0412]]|86}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0057/0403|[[W]]/[[\u0403]]|87}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0058/0409|[[X]]/[[\u0409]]|88}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0059/040A|[[Y]]/[[\u040a]]|89}}\n|{{chset-color-alpha}}|{{chset-cell|005A/0417|[[Z]]/[[Ze (Cyrillic)|\u0417]]|90}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0106/040B|[[\u0106]]/[[\u040b]]|91}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|017D/0416|[[\u017d]]/[[\u0416]]|92}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0110/0402|[[\u0110]]/[[\u0402]]|93}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0160/0428|[[\u0160]]/[[\u0428]]|94}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|00EB/040F|[[\u00eb]]/[[\u040f]]|95}}\n|-\n!{{chset-left2|6_<br/>96}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|010D/0447|[[\u010d]]/[[\u0447]]|96}}\n|{{chset-color-alpha}}|{{chset-cell|0061/0430|[[a]]/[[A (Cyrillic)|\u0430]]|97}}\n|{{chset-color-alpha}}|{{chset-cell|0062/0431|[[b]]/[[Be (Cyrillic)|\u0431]]|98}}\n|{{chset-color-alpha}}|{{chset-cell|0063/0446|[[c]]/[[Tse (Cyrillic)|\u0446]]|99}}\n|{{chset-color-alpha}}|{{chset-cell|0064/0434|[[d]]/[[De (Cyrillic)|\u0434]]|100}}\n|{{chset-color-alpha}}|{{chset-cell|0065/0435|[[e]]/[[Ye (Cyrillic)|\u0435]]|101}}\n|{{chset-color-alpha}}|{{chset-cell|0066/0444|[[f]]/[[Ef (Cyrillic)|\u0444]]|102}}\n|{{chset-color-alpha}}|{{chset-cell|0067/0433|[[g]]/[[Ge (Cyrillic)|\u0433]]|103}}\n|{{chset-color-alpha}}|{{chset-cell|0068/0445|[[h]]/[[Kha (Cyrillic)|\u0445]]|104}}\n|{{chset-color-alpha}}|{{chset-cell|0069/0438|[[i]]/[[I (Cyrillic)|\u0438]]|105}}\n|{{chset-color-alpha}}|{{chset-cell|006A/0458|[[j]]/[[Je (Cyrillic)|\u0458]]|106}}\n|{{chset-color-alpha}}|{{chset-cell|006B/043A|[[k]]/[[Ka (Cyrillic)|\u043a]]|107}}\n|{{chset-color-alpha}}|{{chset-cell|006C/043B|[[l]]/[[El (Cyrillic)|\u043b]]|108}}\n|{{chset-color-alpha}}|{{chset-cell|006D/043C|[[m]]/[[Em (Cyrillic)|\u043c]]|109}}\n|{{chset-color-alpha}}|{{chset-cell|006E/043D|[[n]]/[[En (Cyrillic)|\u043d]]|110}}\n|{{chset-color-alpha}}|{{chset-cell|006F/043E|[[o]]/[[O (Cyrillic)|\u043e]]|111}}\n|-\n!{{chset-left2|7_<br/>112}}\n|{{chset-color-alpha}}|{{chset-cell|0070/043F|[[p]]/[[Pe (Cyrillic)|\u043f]]|112}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0071/045C|[[q]]/[[\u045c]]|113}}\n|{{chset-color-alpha}}|{{chset-cell|0072/0440|[[r]]/[[Er (Cyrillic)|\u0440]]|114}}\n|{{chset-color-alpha}}|{{chset-cell|0073/0441|[[s]]/[[Es (Cyrillic)|\u0441]]|115}}\n|{{chset-color-alpha}}|{{chset-cell|0074/0442|[[t]]/[[Te (Cyrillic)|\u0442]]|116}}\n|{{chset-color-alpha}}|{{chset-cell|0075/0443|[[u]]/[[U (Cyrillic)|\u0443]]|117}}\n|{{chset-color-alpha}}|{{chset-cell|0076/0432|[[v]]/[[Ve (Cyrillic)|\u0432]]|118}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0077/0453|[[w]]/[[\u0453]]|119}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0078/0459|[[x]]/[[\u0459]]|120}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0079/045A|[[y]]/[[\u045a]]|121}}\n|{{chset-color-alpha}}|{{chset-cell|007A/0437|[[z]]/[[Ze (Cyrillic)|\u0437]]|122}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0107/045B|[[\u0107]]/[[\u045b]]|123}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|017E/0436|[[\u017e]]/[[\u0436]]|124}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0111/0452|[[\u0111]]/[[Dje|\u0452]]|125}}\n|{{chset-color-alpha-box|green}}|{{chset-cell|0161/0448|[[\u0161]]/[[Sha (Cyrillic)|\u0448]]|126}}\n|{{chset-color-graph-box|green}}|{{chset-ctrl|25A0|[[\u25a0]]|127}}\n|}\n{{chset-leg