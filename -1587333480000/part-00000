da, Tokyo|Sumida]]\n|3\n|5\n| bgcolor=\"#CCFFCC\" |1/2<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |1/1<sup>Tomin</sup>\n|0/1\n|\n| bgcolor=\"#00A4E1\" |1/1\n|\n|\n|\n|\n|\n|- align=\"right\"\n| align=\"left \" |[[K\u014dt\u014d]]\n|4\n|9\n| bgcolor=\"#CCFFCC\" |1/2<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |1/1<sup>Tomin</sup>\n| bgcolor=\"#FF0000\" |1/1\n|0/1\n| bgcolor=\"#00A4E1\" |1/1\n|\n|\n|\n|0/1\n|0/2<sup>1\u00d7Tomin</sup>\n|- align=\"right\"\n| align=\"left \" |[[Shinagawa]]\n|4\n|7\n|0/2<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |1/1<sup>Tomin</sup>\n| bgcolor=\"#FF0000\" |1/1\n|0/1<sup>[[Liberale Partei (Japan, 2016\u2013)|LP]]&Net</sup>\n| bgcolor=\"#00A4E1\" |2/2\n|\n|\n|\n|\n|\n|- align=\"right\"\n| align=\"left \" |[[Meguro]]\n|3\n|5\n|0/2<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |1/1<sup>Tomin</sup>\n| bgcolor=\"#FF0000\" |1/1\n|\n| bgcolor=\"#00A4E1\" |1/1<sup>Net</sup>\n|\n|\n|\n|\n|\n|- align=\"right\"\n| align=\"left \" |[[\u014cta, Tokyo|\u014cta]]\n|8\n|15\n| bgcolor=\"#CCFFCC\" |2/3<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |2/2<sup>Tomin</sup>\n| bgcolor=\"#FF0000\" |1/2\n|0/1<sup>LP&Net</sup>\n| bgcolor=\"#00A4E1\" |2/2\n|\n| bgcolor=\"#A0C0E0\" |1/1\n|\n|0/1\n|0/3\n|- align=\"right\"\n| align=\"left \" |[[Setagaya]]\n|8\n|18\n| bgcolor=\"#CCFFCC\" |3/3<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |1/1<sup>Tomin</sup>\n| bgcolor=\"#FF0000\" |1/1\n| bgcolor=\"#FFEECC\" |1/1<sup>LP</sup>\n| bgcolor=\"#00A4E1\" |2/2\n|0/1<sup>DP</sup>\n|0/1\n|0/1<sup>LP</sup>\n|0/2\n|0/5\n|- align=\"right\"\n| align=\"left \" |[[Shibuya]]\n|2\n|5\n|0/1<sup>Kokoro</sup>\n|\n|0/1\n|0/1<sup>LP</sup>\n| bgcolor=\"#00A4E1\" |1/1<sup>K\u014dmei</sup>\n|\n|\n|\n|\n| bgcolor=\"#00A4E1\" |1/1<sup>K\u014dmei&Tomin</sup>\n|- align=\"right\"\n| align=\"left \" |[[Nakano, Tokyo|Nakano]]\n|3 (-1)\n|6\n|0/1<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |1/1<sup>Tomin</sup>\n|0/1\n| bgcolor=\"#FFEECC\" |1/1<sup>LP&Net</sup>\n| bgcolor=\"#00A4E1\" |1/1\n|\n|\n|\n|0/1\n|\n|- align=\"right\"\n| align