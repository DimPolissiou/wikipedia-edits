/1\n|0/1<sup>LP&Net</sup>\n| bgcolor=\"#00A4E1\" |1/1\n|\n|\n|\n|0/2\n|\n|- align=\"right\"\n| align=\"left \" |[[Edogawa, Tokyo|Edogawa]]\n|5\n|6\n| bgcolor=\"#CCFFCC\" |1/2<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |1/1<sup>Tomin</sup>\n| bgcolor=\"#FF0000\" |1/1\n|\n| bgcolor=\"#00A4E1\" |2/2\n|\n|\n|\n|\n|\n|- align=\"right\"\n| align=\"left \" |[[Hachi\u014dji]]\n|5\n|9\n| bgcolor=\"#CCFFCC\" |1/2<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |1/1<sup>Tomin</sup>\n| bgcolor=\"#FF0000\" |1/1\n|0/1<sup>LP&Net</sup>\n| bgcolor=\"#00A4E1\" |2/2\n|\n|\n|\n|0/1\n|0/1\n|- align=\"right\"\n| align=\"left \" |[[Tachikawa]]\n|2\n|4\n| bgcolor=\"#CCFFCC\" |1/1<sup>Kokoro</sup>\n|\n|0/1\n|\n| bgcolor=\"#00A4E1\" |1/1<sup>K\u014dmei</sup>\n|\n|\n|\n|\n|0/1<sup>Tomin</sup>\n|- align=\"right\"\n| align=\"left \" |[[Musashino, Tokyo|Musashino]]\n|1\n|3\n|0/1<sup>Kokoro</sup>\n|\n|\n|0/1<sup>LP&Net</sup>\n| bgcolor=\"#00A4E1\" |1/1<sup>K\u014dmei</sup>\n|\n|\n|\n|\n|\n|- align=\"right\"\n| align=\"left \" |[[Mitaka, Tokyo|Mitaka]]\n|2\n|4\n|0/1<sup>Kokoro</sup>\n|\n|0/1\n| bgcolor=\"#FFEECC\" |1/1<sup>LP&Net</sup>\n| bgcolor=\"#00A4E1\" |1/1<sup>K\u014dmei</sup>\n|\n|\n|\n|\n|\n|- align=\"right\"\n| align=\"left \" |[[\u014cme]]\n|1\n|3\n|0/1<sup>Kokoro</sup>\n|\n|\n|\n| bgcolor=\"#00A4E1\" |1/1<sup>K\u014dmei&Net</sup>\n|\n|\n|\n|\n|0/1<sup>JCP</sup>\n|- align=\"right\"\n| align=\"left \" |[[Fuch\u016b, Tokyo|Fuch\u016b]]\n|2\n|4\n|0/1<sup>Kokoro</sup>\n|\n|0/1\n|\n| bgcolor=\"#00A4E1\" |2/2<sup>K\u014dmei&1\u00d7Net</sup>\n|\n|\n|\n|\n|\n|- align=\"right\"\n| align=\"left \" |[[Akishima]]\n|1\n|3\n|0/1<sup>Kokoro</sup>\n|\n|0/1\n|\n| bgcolor=\"#00A4E1\" |1/1<sup>K\u014dmei&Net</sup>\n|\n|\n|\n|\n|\n|- align=\"right\"\n| align=\"left \" |[[Machida, Tokyo|Machida]]\n|4 (+1)\n|8\n| bgcolor=\"#CCFFCC\" |1/2<sup>Kokoro</sup>\n| bgcolor=\"#CCCCFF\" |1/1<sup>Tomin</sup>\n| bgcolor=\"#FF0000\" |1/1\n|0/1<sup>LP&Net</sup>\n| bgcolor=\"#00A4E1\" |1/1\n|\n|0/1\n|\n|0/1\n|\n|- align=\"right\"\n|