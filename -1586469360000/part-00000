en|pos=OL}}\n{{American football roster/Player|num=71|class=FR|first=Jared|last=Griffith|pos=OL}}\n{{American football roster/Player|num=57|class=SO|first=Kai-Leon|last=Herbert|pos=OL}}\n{{American football roster/Player|num=75|class=FR|first=Zalon'tae|last=Hillery|pos=OL|rs=y}}\n{{American football roster/Player|num=77|class=SR|first=Jahair|last=Jones|pos=OL|rs=y}}\n{{American football roster/Player|num=68|class=FR|first=Zachary|last=Lerman|pos=OL|rs=y}}\n{{American football roster/Player|num=62|class=JR|first=Hayden|last=Mahoney|pos=OL|rs=y}}\n{{American football roster/Player|num=79|class=JR|first=Bar|last=Milo|pos=OL|rs=y}}\n{{American football roster/Player|num=51|class=FR|first=DJ|last=Scaife Jr.|pos=OL}}\n{{American football roster/Player|num=78|class=SR|first=Tyree|last=St. Louis|pos=OL}}\n{{American football roster/Player|num=52|class=FR|first=Cleveland|last=Reed Jr.|pos=OL}}\n\n|defensive_players=\n{{American football roster/Player|num=93|class=JR|first=Pat|last=Bethel|pos=DL}}\n{{American football roster/Player|num=96|class=SO|first=Jonathan|last=Ford|pos=DL}}\n{{American football roster/Player|num=97|class=SO|first=Jonathan|last=Garvin|pos=DL}}\n{{American football roster/Player|num=31|class=SR|first=Demetrius|last=Jackson|pos=DL|rs=y}}\n{{American football roster/Player|num=99|class=JR|first=Joe|last=Jackson|pos=DL|link=y|dab= Joe Jackson (defensive end, born 1996)}}\n{{American football roster/Player|num=90|class=SO|first=Tyreic|last=Martin|pos=DL|rs=y}}\n{{American football roster/Player|num=91|class=FR|first=Jordan|last=Miller|pos=DL}}\n{{American football roster/Player|num=95|class=SR|first=John|last=O'Boyle|pos=DL|rs=y}}\n{{American football roster/Player|num=94|class=SR|first=Tito|last=Odenigbo|pos=DL|rs=y}}\n{{American football roster/Player|num=19|class=JR|first=Scott|last=Patchan|pos=DL|rs=y}}\n{{American football roster/Player|num=15|class=FR|first=Gregory|last=Rousseau|link=y|pos=DL}}\n{{American football roster/Player|num=68|class=FR|first=M