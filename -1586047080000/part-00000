\n|align=\"center\"|''3,481''\n|align=\"center\"|'''2.07%'''\n|align=\"center\"|''102''\n|align=\"center\"|'''0.02%'''\n|align=\"center\"|''1''\n|-\n|align=\"center\"  {{party shading/Democratic}}|[[Rowan County, Kentucky|Rowan]] \n|align=\"center\"|'''58.47%'''\n|align=\"center\"|''4,045''\n|align=\"center\"|'''39.66%'''\n|align=\"center\"|''2,744''\n|align=\"center\"|'''1.86%'''\n|align=\"center\"|''129''\n|align=\"center\"|'''0.00%'''\n|align=\"center\"|''0''\n|-\n|align=\"center\"  {{party shading/Republican}}|[[Russell County, Kentucky|Russell]] \n|align=\"center\"|'''26.50%'''\n|align=\"center\"|''1,571''\n|align=\"center\"|'''71.73%'''\n|align=\"center\"|''4,252''\n|align=\"center\"|'''1.77%'''\n|align=\"center\"|''105''\n|align=\"center\"|'''0.00%'''\n|align=\"center\"|''0''\n|-\n|align=\"center\"  {{party shading/Democratic}}|[[Scott County, Kentucky|Scott]] \n|align=\"center\"|'''49.10%'''\n|align=\"center\"|''9,827''\n|align=\"center\"|'''48.61%'''\n|align=\"center\"|''9,730''\n|align=\"center\"|'''2.29%'''\n|align=\"center\"|''458''\n|align=\"center\"|'''0.00%'''\n|align=\"center\"|''0''\n|-\n|align=\"center\"  {{party shading/Republican}}|[[Shelby County, Kentucky|Shelby]] \n|align=\"center\"|'''43.96%'''\n|align=\"center\"|''7,380''\n|align=\"center\"|'''53.98%'''\n|align=\"center\"|''9,062''\n|align=\"center\"|'''2.04%'''\n|align=\"center\"|''343''\n|align=\"center\"|'''0.01%'''\n|align=\"center\"|''2''\n|-\n|align=\"center\"  {{party shading/Republican}}|[[Simpson County, Kentucky|Simpson]] \n|align=\"center\"|'''46.48%'''\n|align=\"center\"|''2,203''\n|align=\"center\"|'''51.84%'''\n|align=\"center\"|''2,457''\n|align=\"center\"|'''1.69%'''\n|align=\"center\"|''80''\n|align=\"center\"|'''0.00%'''\n|align=\"center\"|''0''\n|-\n|align=\"center\"  {{party shading/Republican}}|[[Spencer County, Kentucky|Spencer]] \n|align=\"center\"|'''35.59%'''\n|align=\"center\"|''2,667''\n|align=\"center\"|'''62.24%'''\n|align=\"center\"|''4,664''\n|align=\"center\"|