ieval Cross, Stringston.jpg\n| upload_desc = Churchyard Cross, 5 Metres South Of Porch, Church of St Mary\n| commonscat = St Mary the Virgin's Church, Stringston | show_ref = yes\n| reference = <ref>{{National Heritage List for England |num=1308144 |desc=Churchyard Cross, 5 metres south of porch, Church of St Mary|accessdate=3 April 2015|fewer-links=yes}}</ref>\n}}\n{{EH listed building row\n| name = [[Combe, Dulverton|Combe]]\n| location = [[Dulverton]]\n| type = Manor house\n| completed = {{sort|1575|Late 16th century}}\n| grid_ref = SS912266\n| lat = 51.0293\n| lon = -3.5528\n| uid = 1247793\n| show_img = yes\n| image = CombeHouse Dulverton Somerset SouthFront.PNG\n| upload_desc = Combe\n| commonscat = Combe, Dulverton | show_ref = yes\n| reference = <ref>{{National Heritage List for England |num=1247793 |desc=Combe|accessdate=3 April 2015|fewer-links=yes}}</ref>\n}}\n{{EH listed building row\n| name = [[St Decuman's Church, Watchet|Cross in Churchyard South of Church Porch]]\n| location = [[Watchet]]\n| type = Cross\n| completed = {{sort|1425|Probably early 15th century}}\n| grid_ref = ST064426\n| lat = 51.1757\n| lon = -3.3390\n| uid = 1057663\n| show_img = yes\n| image = Geograph 2372613 Old stone cross, St. Decuman's churchyard.jpg\n| upload_desc = Cross In Churchyard South Of Church Porch\n| commonscat = Church of Saint Decuman in Watchet | show_ref = yes\n| reference = <ref>{{National Heritage List for England |num=1057663 |desc=Cross In Churchyard South Of Church Porch|accessdate=3 April 2015|fewer-links=yes}}</ref>\n}}\n{{EH listed building row\n| name = [[Croydon House]]\n| location = [[Timberscombe]]\n| type = House\n| completed =  {{sort|1700|c. 1700}}\n| grid_ref = SS962406\n| lat =  51.1559\n| lon =  -3.4846\n| uid = 1057345\n| show_img = yes\n| image =\n| upload_desc = Croydon House\n| commonscat = | show_ref = yes\n| reference = <ref>{{National Heritage List for England |num=1057345 |desc=Croydon House|accessdate=3 April 2015|fewer-links=yes}}</ref