7  = FYL\n|team8  = LYN\n|team9  = LYS\n|team10 = OST\n|team11 = REA\n|team12 = SAN\n|team13 = SOG\n|team14 = STO\n\n|result1=2D \n|result12=4D |result13=4D |result14=4D\n\n|res_col_header=PR\n|col_2D=green1 |text_2D=Promotion to [[2021 2. divisjon|2. divisjon]]\n|col_4D=red1   |text_4D=Relegation to [[2021 4. divisjon|4. divisjon]]\n\n|name_\u00c5RD=[[\u00c5rdal FK|\u00c5rdal]]\n|win_\u00c5RD=0 |draw_\u00c5RD=0 |loss_\u00c5RD=0 |gf_\u00c5RD=0 |ga_\u00c5RD=0\n\n|name_BJA=[[IL Bjarg|Bjarg]]\n|win_BJA=0 |draw_BJA=0 |loss_BJA=0 |gf_BJA=0 |ga_BJA=0\n\n|name_BRA=[[SK Brann 2|Brann 2]]\n|win_BRA=0 |draw_BRA=0 |loss_BRA=0 |gf_BRA=0 |ga_BRA=0\n\n|name_FAN=[[Fana IL|Fana]]\n|win_FAN=0 |draw_FAN=0 |loss_FAN=0 |gf_FAN=0 |ga_FAN=0\n\n|name_FJ\u00d8=[[Fj\u00f8ra FK|Fj\u00f8ra]]\n|win_FJ\u00d8=0 |draw_FJ\u00d8=0 |loss_FJ\u00d8=0 |gf_FJ\u00d8=0 |ga_FJ\u00d8=0\n\n|name_FRI=[[Frigg Oslo FK|Frigg]]\n|win_FRI=0 |draw_FRI=0 |loss_FRI=0 |gf_FRI=0 |ga_FRI=0\n\n|name_FYL=[[FK Fyllingsdalen|Fyllingsdalen]]\n|win_FYL=0 |draw_FYL=0 |loss_FYL=0 |gf_FYL=0 |ga_FYL=0\n\n|name_LYN=[[Lyn Fotball|Lyn]]\n|win_LYN=0 |draw_LYN=0 |loss_LYN=0 |gf_LYN=0 |ga_LYN=0\n\n|name_LYS=[[Lysekloster IL|Lysekloster]]\n|win_LYS=0 |draw_LYS=0 |loss_LYS=0 |gf_LYS=0 |ga_LYS=0\n\n|name_OST=[[Os TF|Os]]\n|win_OST=0 |draw_OST=0 |loss_OST=0 |gf_OST=0 |ga_OST=0\n\n|name_REA=[[IF Ready|Ready]]\n|win_REA=0 |draw_REA=0 |loss_REA=0 |gf_REA=0 |ga_REA=0\n\n|name_SAN=[[IL Sandviken|Sandviken]]\n|win_SAN=0 |draw_SAN=0 |loss_SAN=0 |gf_SAN=0 |ga_SAN=0\n\n|name_SOG=[[Sogndal Fotball 2|Sogndal 2]]\n|win_SOG=0 |draw_SOG=0 |loss_SOG=0 |gf_SOG=0 |ga_SOG=0\n\n|name_STO=[[Stord IL|Stord]]\n|win_STO=0 |draw_STO=0 |loss_STO=0 |gf_STO=0 |ga_STO=0\n\n<!--Status-->\n\n<!--Table settings and rules-->\n|show_limit=5\n|class_rules = 1) Points; 2) Goal difference; 3) Goals scored; 4) Head-to-head points; 5) Head-to-head goal difference; 6) Head-to-head away goals scored (only if between two teams); 7) Head-to-head goals scored; 8) Play-off (only used to decide champions or relegation).\n}}</onlyinclude>\n\n===Group 5===\n<onlyinclude>{{#invoke:Sports table|main|style=WDL\n|update=29 January 2020|start_date=13 April 2020\n|source=[https://www.fotball.no/fotballdata/turnering/tabell/?fiksId=169282 fotball.no] {{in lang|no}}\n|section=Group 5\n\n|team1  = AL2\n|team2  = BY\u00c5\n|team3  = KOL\n|team4  = MEL\n|team5  = MO2\n|team6  = NTN\n|team7  = ORK\n|team8  = RAN\n|team9  = SPJ\n|team10 = SDM\n|team11 = TIL\n|team12 = TRF\n|team13 = TYN\n|team14 = VOL\n\n|result1=2D \n|result12=4D |result13=4D |result14=4D\n\n|res_col_header=PR\n|col_2D=green1 |text_2D=Promotion to [[2021 2. divisjon|2. divisjon]]\n|col_4D=red1   |text_4D=Relegation to [[2021 4. divisjon|4. divisjon]]\n\n|name_AL2=[[Aalesunds FK 2|Aalesund 2]]\n|win_AL2=0 |draw_AL2=0 |loss_AL2=0 |gf_AL2=0 |ga_AL2=0\n\n|name_BY\u00c5=[[By\u00e5sen Toppfotball|By\u00e5sen]]\n|win_BY\u00c5=0 |draw_BY\u00c5=0 |loss_BY\u00c5=0 |gf_BY\u00c5=0 |ga_BY\u00c5=0\n\n|name_KOL=[[Kolstad IL|Kolstad]]\n|win_KOL=0 |draw_KOL=0 |loss_KOL=0 |gf_KOL=0 |ga_KOL=0\n\n|name_MEL=[[Melhus IL|Melhus]]\n|win_MEL=0 |draw_MEL=0 |loss_MEL=0 |gf_MEL=0 |ga_MEL=0\n\n|name_MO2=[[Molde FK 2|Molde 2]]\n|win_MO2=0 |draw_MO2=0 |loss_MO2=0 |gf_MO2=0 |ga_MO2=0\n\n|name_NTN=[[NTNUI]]\n|win_NTN=0 |draw_NTN=0 |loss_NTN=0 |gf_NTN=0 |ga_NTN=0\n\n|name_ORK=[[Orkla FK|Orkla]]\n|win_ORK=0 |draw_ORK=0 |loss_ORK=0 |gf_ORK=0 |ga_ORK=0\n\n|name_RAN=[[Ranheim Fotball|Ranheim 2]]\n|win_RAN=0 |draw_RAN=0 |loss_RAN=0 |gf_RAN=0 |ga_RAN=0\n\n|name_SPJ=[[Spjelkavik IL|Spjelkavik]]\n|win_SPJ=0 |draw_SPJ=0 |loss_SPJ=0 |gf_SPJ=0 |ga_SPJ=0\n\n|name_SDM=[[Strindheim IL|Strindheim]]\n|win_SDM=0 |draw_SDM=0 |loss_SDM=0 |gf_SDM=0 |ga_SDM=0\n\n|name_TIL=[[Tiller IL|Tiller]]\n|win_TIL=0 |draw_TIL=0 |loss_TIL=0 |gf_TIL=0 |ga_TIL=0\n\n|name_TRF=[[SK Tr\u00e6ff|Tr\u00e6ff]]\n|win_TRF=0 |draw_TRF=0 |loss_TRF=0 |gf_TRF=0 |ga_TRF=0\n\n|name_TYN=[[Tynset IF|Tynset]]\n|win_TYN=0 |draw_TYN=0 |loss_TYN=0 |gf_TYN=0 |ga_TYN=0\n\n|name_VOL=