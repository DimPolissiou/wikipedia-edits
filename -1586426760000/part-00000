s<ref>{{cite web |title=Player Game Finder |url=https://www.basketball-reference.com/play-index/tiny.fcgi?id=HJZNy |website=basketball-reference.com |accessdate=May 8, 2019}}</ref>\n|-\n!Rank\n!Name\n!Total\n!class=\"unsortable\"|{{Tooltip|Ref.|Reference}} \n|-\n|1 || bgcolor=\"#FFFF99\"|{{sortname|Magic|Johnson}}* || 30 ||<ref>{{cite web |title=Magic Johnson Game Finder |url=http://bkref.com/tiny/s7JX7 |website=basketball-reference.com |accessdate=May 8, 2019}}</ref>\n|-\n|2 ||bgcolor=\"#CFECEC\"|{{sortname|LeBron|James}}^ || 23 || <ref>{{cite web |title=LeBron James Game Finder |url=http://bkref.com/tiny/LUw5a  |website=basketball-reference.com |accessdate=8 May  2019}}</ref>\n|-\n|3 || bgcolor=\"#FFFF99\"|{{sortname|Jason|Kidd}}* || 11 || <ref>{{cite web |title=Jason Kidd Game Finder |url=http://bkref.com/tiny/FxWUc  |website=basketball-reference.com |accessdate=8 May  2019}}</ref>\n|-\n|rowspan=\"4\" |4 || bgcolor=\"#FFFF99\"|{{sortname|Larry|Bird}}* || 10 || <ref>{{cite web |title=Larry Bird Game Finder |url=http://bkref.com/tiny/Inpp3 |website=basketball-reference.com |accessdate=May 8, 2019}}</ref>\n|-\n| bgcolor=\"#CFECEC\" |{{sortname|Draymond|Green}}^ || 10 || <ref>{{cite web |title=Draymond Green Game Finder |url=http://bkref.com/tiny/pJuXo |website=basketball-reference.com |accessdate=May 8, 2019}}</ref>\n|-\n| bgcolor=\"#CFECEC\" |{{sortname|Rajon|Rondo}}^ || 10 || <ref>{{cite web |title=Rajon Rondo Game Finder |url= http://bkref.com/tiny/R0Isp |website=basketball-reference.com |accessdate=May 8, 2019}}</ref>\n|-\n| bgcolor=\"#CFECEC\" |{{sortname|Russell|Westbrook}}^ || 10 ||<ref>{{cite web|url=http://bkref.com/tiny/sKC9c |title=Russell Westbrook Game Finder|work=basketball-reference.com|accessdate=May 8, 2019}}</ref>\n|-\n|8 || bgcolor=\"#FFFF99\"|{{sortname|Wilt|Chamberlain}}* || 9 || <ref>{{cite web |title=Wilt Chamberlain Game Finder |url=http://bkref.com/tiny/Vy070  |website=basketball-reference.com |accessdate=May 8, 2019}}</ref>\n|-\n|9|| bgcolo