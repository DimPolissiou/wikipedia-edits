}}\n| {{lang|bn|\u09a2\u09bc}} || || || \u0b22\u0b3c \n| || || || ||  || 5\n|-\n! {{sort|Nna|\u1e47}}\n! .n\n| \u0923 \n|{{Nastaliq|\u06b9}}\n| {{lang|bn|\u09a3}} || \u0a23 || \u0aa3 || \u0b23 \n| \u0ba3 || \u0c23 || \u0ca3 || \u0d23 || \u0dab || 11\n|-\n! {{sort|Ndda|n\u0306\u1e0d}}\n! ^n.d\n| \n|\n| || || || \n| || || || || \u0dac || 1\n|-\n! {{sort|Ta|t}}\n! t\n| \u0924 \n|{{Nastaliq|\u062a}}\n| {{lang|bn|\u09a4}} || \u0a24 || \u0aa4 || \u0b24 \n| \u0ba4 || \u0c24 || \u0ca4 || \u0d24 || \u0dad || 11\n|-\n! {{sort|Tha|th}}\n! th\n| \u0925 \n|{{Nastaliq|\u062a\u06be}}\n| {{lang|bn|\u09a5}} || \u0a25 || \u0aa5 || \u0b25 \n| || \u0c25 || \u0ca5 || \u0d25 || \u0dae || 10\n|-\n! {{sort|Da|d}}\n! d\n| \u0926 \n|{{Nastaliq|\u062f}}\n| {{lang|bn|\u09a6}} || \u0a26 || \u0aa6 || \u0b26 \n| || \u0c26 || \u0ca6 || \u0d26 || \u0daf || 10\n|-\n! {{sort|Dha|dh}}\n! dh\n| \u0927 \n|{{Nastaliq|\u062f\u06be}}\n| {{lang|bn|\u09a7}} || \u0a27 || \u0aa7 || \u0b27 \n| || \u0c27 || \u0ca7 || \u0d27 || \u0db0 || 10\n|-\n! {{sort|Na|n}}\n! n\n| \u0928 \n|{{Nastaliq|\u0646}}\n| {{lang|bn|\u09a8}} || \u0a28 || \u0aa8 || \u0b28 \n| \u0ba8 || \u0c28 || \u0ca8 || \u0d28 || \u0db1 || 11\n|-\n! {{sort|Nda|n\u0306d}}\n! ^nd\n| \n|\n| || || || \n| || || || || \u0db3 || 1\n|-\n! {{sort|Pa|p}}\n! p\n| \u092a \n|{{Nastaliq|\u067e}}\n| {{lang|bn|\u09aa}} || \u0a2a || \u0aaa || \u0b2a \n| \u0baa || \u0c2a || \u0caa || \u0d2a || \u0db4 || 11\n|-\n! {{sort|Pha|ph}}\n! ph\n| \u092b \n|{{Nastaliq|\u067e\u06be}}\n| {{lang|bn|\u09ab}} || \u0a2b || \u0aab || \u0b2b \n| || \u0c2b ||\u0cab\n| \u0d2b || \u0db5 || 10\n|-\n! {{sort|Ba|b}}\n! b\n| \u092c \n|{{Nastaliq|\u0628}}\n| {{lang|bn|\u09ac}} || \u0a2c || \u0aac || \u0b2c \n| || \u0c2c || \u0cac || \u0d2c || \u0db6 || 10\n|-\n! {{sort|Bha|bh}}\n! bh\n| \u092d \n|{{Nastaliq|\u0628\u06be}}\n| {{lang|bn|\u09ad}} || \u0a2d || \u0aad || \u0b2d \n| || \u0c2d || \u0cad || \u0d2d || \u0db7 || 10\n|-\n! {{sort|Ma|m}}\n! m\n| \u092e \n|{{Nastaliq|\u0645}}\n| {{lang|bn|