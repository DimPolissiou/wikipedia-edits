pr, 9 Apr, 10 Apr, 11 Apr, 12 Apr, 13 Apr, 14 Apr\n|y=0, 0, 0, 2, 0, 1, 2, 0, 3, 4, 1, 0, 0, 8, 11, 16, 8, 31, 28, 35, 49, 55, 72, 64, 66, 128, 149, 140, 145, 141, 184, 92, 173, 254, 246, 266, 171, 129, 126, 170\n|yGrid= |xGrid=\n}}\n\n{{Graph:Chart\n|type=line\n|linewidth=1\n|showSymbols=1\n|width=700\n|colors=#009233, #FFCD00, #0099D5, #FF0000, #8800ff\n|showValues=\n|xAxisTitle=Date\n|xAxisAngle=-40\n|x=24 Feb, 25 Feb, 26 Feb, 27 Feb, 28 Feb, 29 Feb, 1 Mar, 2 Mar, 3 Mar, 4 Mar, 5 Mar, 6 Mar, 7 Mar, 8 Mar, 9 Mar, 10 Mar, 11 Mar, 12 Mar, 13 Mar, 14 Mar, 15 Mar, 16 Mar, 17 Mar, 18 Mar, 19 Mar, 20 Mar, 21 Mar, 22 Mar, 23 Mar, 24 Mar, 25 Mar, 26 Mar, 27 Mar, 28 Mar, 29 Mar, 30 Mar, 31 Mar, 1 Apr, 2 Apr, 3 Apr, 4 Apr, 5 Apr, 6 Apr, 7 Apr, 8 Apr, 9 Apr, 10 Apr, 11 Apr, 12 Apr, 13 Apr, 14 Apr\n|yAxisTitle=No. of cases by region\n|legend=Legend\n|y1= 0, 2, 2, 2, 3, 7, 17, 19, 23, 31, 48, 68, 81, 97, 133, 213, 350, 586, 940, 1175, 1453, 1999, 2118, 2583, 3546, 4582, 5626, 6584, 7277, 8796, 10302, 11652, 13456, 14883, 16112, 17724, 18969, 20354, 21967, 23592, 25500, 26906, 27968, 28856, 30027, 31609, 33352, 34486, 35285, 35947, 36521\n|y1Title=Others\n|y2=0, 1, 2, 4, 25, 30, 66, 86, 101, 111, 175, 281, 346, 392, 484, 648, 801, 688, 936, 1154, 1407, 1541, 2105, 2372, 3033, 3497, 3542, 3545, 5615, 6318, 7197, 7924, 9235, 10607, 11400, 12178, 13225, 14351, 15427, 16606, 17885, 18735, 19384, 20220, 20929, 21961, 23028, 23644, 24267, 24883, 25300\n|y2Title=North Rhine-Westphalia\n|y3=14, 14, 14, 14, 15, 15, 19, 25, 35, 48, 52, 79, 117, 148, 256, 314, 366, 500, 558, 681, 886, 1067, 1109, 1243, 1692, 2401, 2960, 3650, 4892, 5754, 6558, 7993, 9481, 11150, 12881, 13989, 14810, 16497, 18496, 20237, 21908, 23846, 24974, 26163, 27564, 28827, 30363, 31453, 32282, 33015, 33569\n|y3Title=Bavaria\n|y4=0, 1, 3, 6, 10, 14, 15, 19, 26, 44, 65, 91, 116, 182, 199, 237, 277, 454, 454, 569, 827, 1105, 1479, 1609, 2155, 2746, 3668, 3807, 3811, 5348, 6069, 7283, 8161, 9781, 9794, 10943, 12334, 1341