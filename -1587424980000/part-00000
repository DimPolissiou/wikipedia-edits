77/liga-primer-indonesia goal.com]<!--[http://www.ligaprimerindonesia.co.id Liga Primer Indonesia]-->\n}}\n\n==Results==\n{{#invoke:sports results|main\n|matches_style=FBR|solid_cell=grey\n|team1=ACH|team2=BDV|team3=BFC|team4=BTV|team5=BRY\n|team6=CEN|team7=JKT|team8=MDU|team9=MDB|team10=MDC\n|team11=MNK|team12=SBY|team13=PSMA|team14=PSBO|team15=PSM\n|team16=RLM|team17=SMU|team18=SFC|team19=TWV\n\n|name_ACH=[[Aceh United F.C.|Aceh United]]\n|match_ACH_BDV=null\n|match_ACH_BFC=2\u20130\n|match_ACH_BTV=null\n|match_ACH_BRY=1\u20130\n|match_ACH_CEN=4\u20131\n|match_ACH_JKT=null\n|match_ACH_MDU=null\n|match_ACH_MDB=null\n|match_ACH_MDC=0\u20131\n|match_ACH_MNK=null\n|match_ACH_SBY=null\n|match_ACH_PSMA=1\u20130\n|match_ACH_PSBO=1\u20131\n|match_ACH_PSM=null\n|match_ACH_RLM=2\u20131\n|match_ACH_SMU=2\u20130\n|match_ACH_SFC=null\n|match_ACH_TWV=null\n\n|name_BDV=[[Bali Devata F.C.|Bali Devata]]\n|match_BDV_ACH=2\u20130\n|match_BDV_BFC=null\n|match_BDV_BTV=0\u20131\n|match_BDV_BRY=null\n|match_BDV_CEN=3\u20131\n|match_BDV_JKT=1\u20131\n|match_BDV_MDU=1\u20130\n|match_BDV_MDB=null\n|match_BDV_MDC=null\n|match_BDV_MNK=1\u20131\n|match_BDV_SBY=2\u20131\n|match_BDV_PSMA=null\n|match_BDV_PSBO=null\n|match_BDV_PSM=2\u20133\n|match_BDV_RLM=null\n|match_BDV_SMU=null\n|match_BDV_SFC=1\u20130\n|match_BDV_TWV=2\u20132\n\n|name_BFC=[[Bandung F.C.]]\n|match_BFC_ACH=null\n|match_BFC_BDV=0\u20131\n|match_BFC_BTV=null\n|match_BFC_BRY=null\n|match_BFC_CEN=null\n|match_BFC_JKT=null\n|match_BFC_MDU=null\n|match_BFC_MDB=2\u20132\n|match_BFC_MDC=1\u20131\n|match_BFC_MNK=1\u20130\n|match_BFC_SBY=null\n|match_BFC_PSMA=0\u20131\n|match_BFC_PSBO=0\u20131\n|match_BFC_PSM=1\u20131\n|match_BFC_RLM=5\u20133\n|match_BFC_SMU=3\u20131\n|match_BFC_SFC=null\n|match_BFC_TWV=1\u20132\n\n|name_BTV=[[Batavia Union]]\n|match_BTV_ACH=2\u20130\n|match_BTV_BDV=null\n|match_BTV_BFC=3\u20132\n|match_BTV_BRY=2\u20131\n|match_BTV_CEN=3\u20132\n|match_BTV_JKT=1\u20131\n|match_BTV_MDU=4\u20131\n|match_BTV_MDB=null\n|mat