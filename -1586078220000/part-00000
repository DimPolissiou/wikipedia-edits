 March 2020\n|source=[https://www.goalzz.com/?c=17583&stage=1 Goalzz]\n\n<!--Update team positions here-->\n|team1=DIR |team2=ARA |team3=KHO |team4=ARB |team5=ORO |team6=RIY |team7=SHR |team8=SAF |team9=HEJ |team10=QAI |team11=ANW |team12=DER\n\n<!--Update team qualifications here (defined below)-->\n|result1=PLC |result2=PL |result11=REL |result12=REL\n\n<!--Update team results here and then (if needed positions above). Don't forget to update the date above (update parameter)-->\n|win_ANW=5 |draw_ANW=3 |loss_ANW=10|gf_ANW=19|ga_ANW=28<!-- Al-Anwar -->\n|win_ARB=8 |draw_ARB=3 |loss_ARB=7 |gf_ARB=28|ga_ARB=21<!-- Al-Arabi -->\n|win_DER=2 |draw_DER=5 |loss_DER=11|gf_DER=18|ga_DER=37<!-- Al-Dera'a -->\n|win_DIR=9 |draw_DIR=8 |loss_DIR=1 |gf_DIR=24|ga_DIR=13<!-- Al-Diriyah -->\n|win_HEJ=6 |draw_HEJ=4 |loss_HEJ=8 |gf_HEJ=19|ga_HEJ=26<!-- Al-Hejaz -->\n|win_KHO=7 |draw_KHO=8 |loss_KHO=3 |gf_KHO=29|ga_KHO=23<!-- Al-Kholood -->\n|win_ORO=7 |draw_ORO=4 |loss_ORO=7 |gf_ORO=26|ga_ORO=20<!-- Al-Orobah -->\n|win_QAI=4 |draw_QAI=9 |loss_QAI=5 |gf_QAI=19|ga_QAI=19<!-- Al-Qaisumah -->\n|win_RIY=6 |draw_RIY=7 |loss_RIY=5 |gf_RIY=18|ga_RIY=15<!-- Al-Riyadh -->\n|win_SAF=5 |draw_SAF=7 |loss_SAF=6 |gf_SAF=19|ga_SAF=22<!-- Al-Safa -->\n|win_SHR=6 |draw_SHR=5 |loss_SHR=7 |gf_SHR=23|ga_SHR=28<!-- Al-Sharq -->\n|win_ARA=7 |draw_ARA=9 |loss_ARA=2 |gf_ARA=24|ga_ARA=14<!-- Arar -->\n\n<!--Team definitions (wikilinks in table)-->\n|name_ANW = [[Al-Anwar Club|Al-Anwar]]\n|name_ARB = [[Al-Arabi (Saudi Arabia)|Al-Arabi]]\n|name_DER = [[Al-Dera'a FC|Al-Dera'a]]\n|name_DIR = [[Al-Diriyah Club|Al-Diriyah]]\n|name_HEJ = [[Al-Hejaz Club|Al-Hejaz]]\n|name_KHO = [[Al-Kholood Club|Al-Kholood]]\n|name_ORO = [[Al-Orobah F.C.|Al-Orobah]]\n|name_QAI = [[Al-Qaisumah FC|Al-Qaisumah]]\n|name_RIY = [[Al-Riyadh SC|Al-Riyadh]]\n|name_SAF = [[Al Safa FC|Al-Safa]]\n|name_SHR = [[Sharq COD|Al-Sharq]]\n|name_ARA = [[Arar F.C.|Arar]]\n\n<!-- Status -->\n\n<!--Table settings and rules-->\n|show_limit=5\n|class_rules =1)