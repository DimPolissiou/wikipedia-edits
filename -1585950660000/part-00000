art|GAON]])<ref>{{cite web|title=South Korea Gaon International Chart (Gaon Chart|url=http://www.gaonchart.co.kr/main/section/total/list.gaon?mainGbn=CHART&mainCate=1&nationGbn=E&termGbn=week|publisher=[[Gaon Chart]]|accessdate=May 6, 2012}}</ref>\n|style=\"text-align:center;\"|5\n|-\n|{{singlechart|Spain|8|artist=Flo Rida|song=Whistle|date=2012-08-01|accessdate=Aug 12, 2012|refname=http://www.promusicae.org/files/listastonos/top%2050%20canciones_w38.2012.pdf}}\n|-\n|{{singlechart|Sweden|1|artist=Flo Rida|song=Whistle|accessdate=May 12, 2012}}\n|-\n{{singlechart|Switzerland|1|artist=Flo Rida|song=Whistle|accessdate=May 12, 2012}}\n|-\n{{singlechart|UK|2|artist=Flo Rida|song=Whistle|date=2012-06-16|accessdate=May 12, 2012|refname=ukdebut}}\n|-\n{{singlechart|UKrandb|1|artist=Flo Rida|song=Whistle|date=2012-06-16|accessdate=May 12, 2012|refname=ukrdebut}}\n|-\n|{{singlechart|Billboardhot100|1|artist=Flo Rida|artistid={{BillboardID|Flo Rida}}|accessdate=May 3, 2012}}\n|-\n|{{singlechart|Billboardadultpopsongs|25|artist=Flo Rida|artistid={{BillboardID|Flo Rida}}|accessdate=September 3, 2012}}\n|-\n|{{singlechart|Billboarddanceclubplay|1|artist=Flo Rida|artistid={{BillboardID|Flo Rida}}|accessdate=August 6, 2012}}\n|-\n|{{singlechart|Billboardlatinsongs|30|artist=Flo Rida|artistid={{BillboardID|Flo Rida}}|accessdate=January 2, 2015}}\n|-\n|{{singlechart|Billboardrapsongs|2|artist=Flo Rida|artistid={{BillboardID|Flo Rida}}|accessdate=June 19, 2012}}\n|-\n|{{singlechart|Billboardpopsongs|1|artist=Flo Rida|artistid={{BillboardID|Flo Rida}}|accessdate=June 19, 2012}}\n|-\n|{{singlechart|Billboardrhythmic|1|artist=Flo Rida|artistid={{BillboardID|Flo Rida}}|accessdate=June 19, 2012}}\n|-\n|Venezuela Pop Rock General ([[Record Report]])<ref name=\"venezuela\">{{cite web|url=http://www.recordreport.com.ve/publico/?i=poprock|title=Pop Rock General|publisher=Record Report|date=29 September 2012|archiveurl=https://web.archive.org/web/20120919190129/http://recordreport.com.ve/public