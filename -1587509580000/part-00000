ITA}}\n{{WDL|2|0|0|2|for=1|against=4|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|JPN}}\n{{WDL|1|0|0|1|for=0|against=2|diff=yes}}\n|- bgcolor=\"#bbf3bb\"\n|scope=row|{{Fb|KAZ}}\n{{WDL|3|3|0|0|for=11|against=0|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|KVX}}\n{{WDL|2|0|1|1|for=1|against=3|diff=yes}}\n|- bgcolor=\"#bbf3bb\"\n|scope=row|{{Fb|LVA}}\n{{WDL|1|1|0|0|for=2|against=0|diff=yes}}\n|- bgcolor=\"#bbf3bb\"\n|scope=row|{{Fb|LIE}}\n{{WDL|2|1|1|0|for=2|against=0|diff=yes}}\n|- bgcolor=\"#bbf3bb\"\n|scope=row|{{Fb|LIT}}\n{{WDL|2|2|0|0|for=6|against=1|diff=yes}}\n|- bgcolor=\"#bbf3bb\"\n|scope=row|{{Fb|LUX}}\n{{WDL|1|1|0|0|for=4|against=1|diff=yes}}\n|- bgcolor=\"#bbf3bb\"\n|scope=row|{{Fb|MDA}}\n{{WDL|4|3|0|1|for=7|against=5|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|MKD}}\n{{WDL|3|1|0|2|for=4|against=7|diff=yes}}\n|- bgcolor=\"#bbf3bb\"\n|scope=row|{{Fb|NIR}}\n{{WDL|1|1|0|0|for=2|against=0|diff=yes}}\n|- bgcolor=\"#ffffbb\"\n|scope=row|{{Fb|NOR}}\n{{WDL|2|1|0|1|for=4|against=3|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|POL}}\n{{WDL|4|0|2|2|for=6|against=9|diff=yes}}\n|- bgcolor=\"#ffffbb\"\n|scope=row|{{Fb|IRL}}\n{{WDL|2|0|2|0|for=0|against=0|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|ROM}}\n{{WDL|5|1|2|2|for=2|against=6|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|RUS}}\n{{WDL|2|0|0|2|for=0|against=5|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|SER}}\n{{WDL|2|0|0|2|for=1|against=4|diff=yes}}\n|- bgcolor=\"#bbf3bb\"\n|scope=row|{{Fb|SMR}}\n{{WDL|2|2|0|0|for=9|against=0|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|SVK}}\n{{WDL|1|0|0|1|for=0|against=2|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|SLO}}\n{{WDL|2|0|1|1|for=1|against=3|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|SWE}}\n{{WDL|3|0|1|2|for=3|against=6|diff=yes}}\n|- bgcolor=\"#ffffbb\"\n|scope=row|{{Fb|SWI}}\n{{WDL|2|1|0|1|for=1|against=2|diff=yes}}\n|- bgcolor=\"#ffbbbb\"\n|scope=row|{{Fb|TUR}}\n{{WDL|2|0|1|1|for=2|against=3|diff