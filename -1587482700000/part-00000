o]] || {{change|63597|67798}} || align=\"left\" | {{flag|Lazio}}\n|-\n|align=center| 89 || align=\"left\" | [[Cosenza]] || {{change|70068|67239}} || align=\"left\" | {{flag|Calabria}}\n|-\n|align=center| 90 || align=\"left\" | [[Potenza]] || {{change|68297|67211}} || align=\"left\" | {{flag|Basilicata}}\n|-\n|align=center| 91 || align=\"left\" | [[Castellammare di Stabia]] || {{change|64506|65922}} || align=\"left\" | {{flag|Campania}}\n|-\n|align=center| 92 || align=\"left\" | [[Afragola]] || {{change|63981|64817}} || align=\"left\" | {{flag|Campania}}\n|-\n|align=center| 93 || align=\"left\" | [[Vittoria, Sicily|Vittoria]] || {{change|63332|64212}} || align=\"left\" | {{flag|Sicily}}\n|-\n|align=center| 94 || align=\"left\" | [[Crotone]] || {{change|61798|63941}} || align=\"left\" | {{flag|Calabria}}\n|-\n|align=center| 95 || align=\"left\" | [[Pomezia]] || {{change|61106|63641}} || align=\"left\" | {{flag|Lazio}}\n|-\n|align=center| 96 || align=\"left\" | [[Vigevano]] || {{change|63700|63153}} || align=\"left\" | {{flag|Lombardy}}\n|-\n|align=center| 97 || align=\"left\" | [[Carrara]] || {{change|65573|62537}} || align=\"left\" | {{flag|Tuscany}}\n|-\n|align=center| 98 || align=\"left\" | [[Caltanissetta]] || {{change|60267|62317}} || align=\"left\" | {{flag|Sicily}}\n|-\n|align=center| 99 || align=\"left\" | [[Viareggio]] || {{change|64503|62079}} || align=\"left\" | {{flag|Tuscany}}\n|-\n|align=center| 100 || align=\"left\" | [[Fano]] || {{change|64100|60978}} || align=\"left\" | {{flag|Marche}}\n|-\n|align=center| 101 || align=\"left\" | [[Savona]] || {{change|62553|60632}} || align=\"left\" | {{flag|Liguria}}\n|-\n|align=center| 102 || align=\"left\" | [[Matera]] || {{change|60818|60403}} || align=\"left\" | {{flag|Basilicata}}\n|-\n|align=center| 103 || align=\"left\" | [[Olbia]] || {{change|56066|60261}} || align=\"left\" | {{flag|Sardinia}}\n|-\n|align=center| 104 || align=\"left\" | [[Legnano]] || {{change|59147|60177}} || align=\"left\" | {{flag|Lombardy