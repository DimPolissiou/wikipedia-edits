|match_OHL_MIR=0\u20132\n|match_OHL_OSO=4\u20132\n|match_OHL_PEL=1\u20133\n|match_OHL_POB=3\u20132\n|match_OHL_RIN=1\u20135\n|match_OHL_RUF=0\u20132\n|match_OHL_SKO=1\u20134\n|match_OHL_TIK=1\u20137\n|match_OHL_VLZ=0\u20131\n\n|name_OSO=[[FK Osogovo|Osogovo]]\n|match_OSO_BEL=3\u20130\n|match_OSO_DRI=0\u20130\n|match_OSO_EGL=2\u20133\n|match_OSO_KOR=2\u20130\n|match_OSO_LOK=0\u20130\n|match_OSO_MGP=0\u20133\n|match_OSO_MIR=0\u20132\n|match_OSO_OHL=1\u20130\n|match_OSO_PEL=1\u20132\n|match_OSO_POB=1\u20132\n|match_OSO_RIN=3\u20132\n|match_OSO_RUF=1\u20135\n|match_OSO_SKO=1\u20130\n|match_OSO_TIK=1\u20130\n|match_OSO_VLZ=0\u20130\n\n|name_PEL=[[FK Pelister|Pelister]]\n|match_PEL_BEL=4\u20131\n|match_PEL_DRI=3\u20132\n|match_PEL_EGL=0\u20131\n|match_PEL_KOR=2\u20130\n|match_PEL_LOK=2\u20130\n|match_PEL_MGP=3\u20131\n|match_PEL_MIR=2\u20131\n|match_PEL_OHL=3\u20130\n|match_PEL_OSO=1\u20131\n|match_PEL_POB=2\u20130\n|match_PEL_RIN=3\u20132\n|match_PEL_RUF=1\u20130\n|match_PEL_SKO=1\u20130\n|match_PEL_TIK=1\u20130\n|match_PEL_VLZ=4\u20132\n\n|name_POB=[[FK Pobeda (2010)|Pobeda Junior]]\n|match_POB_BEL=2\u20130\n|match_POB_DRI=1\u20131\n|match_POB_EGL=1\u20130\n|match_POB_KOR=1\u20132\n|match_POB_LOK=1\u20132\n|match_POB_MGP=4\u20131\n|match_POB_MIR=2\u20130\n|match_POB_OHL=3\u20130\n|match_POB_OSO=2\u20131\n|match_POB_PEL=0\u20130\n|match_POB_RIN=1\u20130\n|match_POB_RUF=2\u20130\n|match_POB_SKO=0\u20131\n|match_POB_TIK=1\u20131\n|match_POB_VLZ=3\u20130\n\n|name_RIN=[[FK Gostivar|Rinia Gostivar]]\n|match_RIN_BEL=1\u20130\n|match_RIN_DRI=1\u20131\n|match_RIN_EGL=3\u20130\n|match_RIN_KOR=2\u20130\n|match_RIN_LOK=3\u20132\n|match_RIN_MGP=1\u20131\n|match_RIN_MIR=0\u20130\n|match_RIN_OHL=1\u20131\n|match_RIN_OSO=3\u20131\n|match_RIN_PEL=1\u20130\n|match_RIN_POB=0\u20131\n|match_RIN_RUF=1\u20130\n|match_RIN_SKO=3\u20131\n|match_RIN_TIK=0\u20130\n|match_RIN_VLZ=0\u20130\n\n|name_RUF=[[KF Rrufeja|Rufeja]]\n|match_RUF_BEL=4\u20131\n|match_RUF_DRI=0\u20133\n|match_RUF_EGL=2\u20130\