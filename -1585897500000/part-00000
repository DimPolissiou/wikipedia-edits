508|name=Alex Venters}}</ref>\n|1933\n|1939\n|3\n|0\n|-\n|align=left|{{sortname|Hugh|Wales}}\n|<ref>{{SFA Profile|id=113511|name=Hugh Wales}}</ref>\n|colspan=2|1932\n|1\n|0\n|-\n|align=left|{{sortname|Andy|Walker|dab=footballer, born 1965}}\n|<ref>{{SFA Profile|id=2505|name=Andy Walker}}</ref>\n|1988\n|1994\n|3\n|0\n|-\n|align=left|{{sortname|Frank|Walker|dab=Scottish footballer}}\n|<ref>{{SFA Profile|id=113512|name=Frank Walker}}</ref>\n|colspan=2|1922\n|1\n|0\n|-\n|align=left|{{sortname|George|Walker|dab=footballer born 1909}}\n|<ref>{{SFA Profile|id=113005|name=George Walker}}</ref>\n|1930\n|1931\n|4\n|0\n|-\n|align=left|{{sortname|Jimmy|Walker|dab=footballer, born 1925}}\n|<ref>{{SFA Profile|id=113513|name=Jimmy Walker}}</ref>\n|colspan=2|1946\n|1\n|0\n|-\n|align=left|{{sortname|Nicky|Walker}}\n|<ref>{{SFA Profile|id=1346|name=Nicky Walker}}</ref>\n|1993\n|1996\n|2\n|0\n|-\n|align=left|{{sortname|William|Walker|dab=footballer, born 1884}}\n|<ref>{{SFA Profile|id=113517|name=William Walker}}</ref>\n|1909\n|1910\n|2\n|0\n|-\n|align=left|{{sortname|Ian|Wallace|dab=footballer}}\n|<ref>{{SFA Profile|id=113831|name=Ian Wallace}}</ref>\n|1978\n|1979\n|3\n|1\n|-\n|align=left|{{sortname|Ross|Wallace}}\n|<ref>{{SFA Profile|id=24867|name=Ross Wallace}}</ref>\n|colspan=2|2009\n|1\n|0\n|-\n|align=left|{{sortname|Jimmy|Wardhaugh}}\n|<ref>{{SFA Profile|id=113376|name=Jimmy Wardhaugh}}</ref>\n|1954\n|1956\n|2\n|0\n|-\n|align=left|{{sortname|Andrew|Watson|dab=footballer, born 1856}}\n|<ref>{{SFA Profile|id=113377|name=Andrew Watson}}</ref>\n|1881\n|1882\n|3\n|0\n|-\n|align=left|{{sortname|Bobby|Watson|dab=footballer}}\n|<ref>{{SFA Profile|id=113744|name=Bobby Watson}}</ref>\n|colspan=2|1971\n|1\n|0\n|-\n|align=left|{{sortname|James|Watson|dab=Rangers footballer}}\n|<ref>{{SFA Profile|id=113380|name=James Watson}}</ref>\n|colspan=2|1878\n|1\n|1\n|-\n|align=left|{{sortname|Jimmy|Watson|dab=footballer, born 1924}}\n|<ref>{{SFA Profile|id=113379|name=James Watson}}</ref>\n|1947\n|19