enter\"|0 / 0\n|align=\"center\"|0 / 3\n|align=\"center\"|0 / 4\n|align=\"center\"|0 / 3\n|align=\"center\"|0 / 4\n|align=\"center\"|0 / 4\n|align=\"center\"|0 / 3\n|align=\"center\"|0 / 0\n|align=\"center\"|0 / 21\n|align=\"center\" style=\"background:#EFEFEF;\" |N/A\n|-\n|style=\"background:#EFEFEF;\" |Annual Win-Loss\n|align=\"center\" style=\"background:#EFEFEF;\"|0\u20130\n|align=\"center\" style=\"background:#EFEFEF;\"|0\u20130\n|align=\"center\" style=\"background:#EFEFEF;\"|0\u20130\n|align=\"center\" style=\"background:#EFEFEF;\"|0\u20130\n|align=\"center\" style=\"background:#EFEFEF;\"|0\u20130\n|align=\"center\" style=\"background:#EFEFEF;\"|2\u20133\n|align=\"center\" style=\"background:#EFEFEF;\"|3\u20134\n|align=\"center\" style=\"background:#EFEFEF;\"|1\u20133\n|align=\"center\" style=\"background:#EFEFEF;\"|4\u20134\n|align=\"center\" style=\"background:#EFEFEF;\"|6\u20134\n|align=\"center\" style=\"background:#EFEFEF;\"|3\u20133\n|align=\"center\" style=\"background:#EFEFEF;\"|0\u20130\n|align=\"center\" style=\"background:#EFEFEF;\"|N/A\n|align=\"center\" style=\"background:#EFEFEF;\"|19\u201321\n|-\n| colspan=\"16\" | '''ATP Masters Series'''\n|-\n|style=\"background:#EFEFEF;\" |[[Indian Wells Masters|Indian Wells]]\n|align=\"center\" colspan=\"3\" rowspan=\"9\" style=\"color:#ccc;\"|NM1<br><br>Before<br><br>1990\n|align=\"center\"|A\n|align=\"center\"|A\n|align=\"center\"|A\n|align=\"center\"|A\n|align=\"center\"|A\n|align=\"center\" style=\"background:#afeeee;\"|2R\n|align=\"center\" style=\"background:yellow;\"|SF\n|align=\"center\" style=\"background:#afeeee;\"|1R\n|align=\"center\" style=\"background:#afeeee;\"|1R\n|align=\"center\" style=\"background:#EFEFEF;\" |0 / 4\n|align=\"center\" style=\"background:#EFEFEF;\"|4\u20134\n|-\n|style=\"background:#EFEFEF;\" |[[Miami Masters|Miami]]\n|align=\"center\"|A\n|align=\"center\"|A\n|align=\"center\"|A\n|align=\"center\" style=\"background:#afeeee;\"|2R\n|align=\"center\" style=\"background:#afeee