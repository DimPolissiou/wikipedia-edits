| || || || || || || \n|-\n!Teams\n!5 || 8\t|| 10 || 5 || 7 || 7 || 9 || 10 || 9 || 10 || 10 || 9 || 11 || 7 || 11 || 12 || 14 || 9 || 13 || 9\n|}\n\n{| class=\"wikitable\" style=\"text-align:center; font-size:90%\"\n! width=170|Country\n! {{flagicon|CHN}}<br />[[2005 FIBA Asia Championship for Women|2005]] !! {{flagicon|KOR|1997}}<br />[[2007 FIBA Asia Championship for Women|2007]] !! {{flagicon|IND}}<br />[[2009 FIBA Asia Championship for Women|2009]] !! {{flagicon|JPN}}<br />[[2011 FIBA Asia Championship for Women|2011]] !! {{flagicon|THA}}<br />[[2013 FIBA Asia Championship for Women|2013]] !! {{flagicon|CHN}}<br />[[2015 FIBA Asia Championship for Women|2015]] !! {{flagicon|IND}}<br />[[2017 FIBA Women's Asia Cup|2017]] !! {{flagicon|IND}}<br />[[2019 FIBA Women's Asia Cup|2019]]|| Years\n|-\n|align=left|{{bkw|AUS|size=20px}}\n| || || || || || || bgcolor=silver|'''2nd''' || bgcolor=#cc9966|'''3rd''' ||'''2'''\n|-\n|align=left|{{bkw|CAM|size=20px}}\n| || || || || || || ||  ||'''1'''\n|-\n|align=left|{{bkw|CHN|size=20px}}\n| bgcolor=gold|'''1st''' || bgcolor=silver|'''2nd''' || bgcolor=gold|'''1st''' ||bgcolor=gold|'''1st'''|| bgcolor=#cc9966|'''3rd''' || bgcolor=silver|'''2nd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=silver|'''2nd''' ||'''23'''\n|-\n|align=left|{{bkw|TPE|size=20px}}\n| bgcolor=#cc9966|'''3rd''' || 4th || 4th ||4th|| 4th || 4th || 5th || 6th ||'''24'''\n|-\n|align=left|{{bkw|FIJ|size=20px}}\n| || || || || || || 14th || q ||'''2'''\n|-\n|align=left|{{bkw|HKG|size=20px}}\n|9th||8th|| || ||12th|| 11th || ||  ||'''22'''\n|-\n|align=left|{{bkw|IND|size=20px}}\n|10th||7th||6th||6th|| 5th || 6th || 9th || 8th ||'''19'''\n|-\n|align=left|{{bkw|INA|size=20px}}\n| || || ||9th|| 9th || || ||  ||'''11'''\n|-\n|align=left|{{bkw|IRI|size=20px}}\n| || || || || || || ||  ||'''1'''\n|-\n|align=left|{{bkw|JPN|size=20px}}\n| 4th || bgcolor=#cc9966|'''3rd'''|| bgcolor=#cc9966|'''3rd''' ||bgcolor=#cc9966|'''3rd'''|| bgcolor=gold|'''1st''' || bgcolor=gold|'''1st''' ||