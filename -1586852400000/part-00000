 || || ||6th|| || || || || || || || || || || || || || || \n|-\n|align=left|{{bkw|CHN|size=20px}}\n| || || || || || bgcolor=gold|'''1st''' || bgcolor=silver|'''2nd''' || bgcolor=silver|'''2nd''' || bgcolor=silver|'''2nd''' || bgcolor=silver|'''2nd''' || bgcolor=gold|'''1st''' || bgcolor=silver|'''2nd''' || bgcolor=gold|'''1st''' || bgcolor=gold|'''1st''' || bgcolor=gold|'''1st''' || bgcolor=gold|'''1st''' || bgcolor=#cc9966|'''3rd''' || 4th || bgcolor=gold|'''1st''' || bgcolor=gold|'''1st'''\n|-\n|align=left|{{bkw|TPE|size=20px}}\n|bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=silver|'''2nd''' || bgcolor=#cc9966|'''3rd''' || || || || || || bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || 4th || 4th || 4th || 4th || 4th || bgcolor=#cc9966|'''3rd''' || 4th || 4th\n|-\n|align=left|{{bkw|HKG|size=20px}}\n| ||7th||8th||5th||5th||7th||5th||6th||7th||9th|| ||6th||8th||5th||5th||8th||10th||8th||10th|| 9th\n|-\n|align=left|{{bkw|IND|size=20px}}\n| || ||10th|| || || || ||7th||9th||7th||7th||8th||7th||6th|| || ||9th|| ||8th||7th\n|-\n|align=left|{{bkw|INA|size=20px}}\n| || ||5th||4th|| || ||7th||9th|| || ||10th|| ||9th|| ||11th||12th||11th|| || || \n|-\n|align=left|{{bkw|IRI|size=20px}}\n| || || || ||4th|| || || || || || || || || || || || || || || \n|-\n|align=left|{{bkw|JPN|size=20px}}\n|bgcolor=silver|'''2nd''' ||bgcolor=silver|'''2nd''' || bgcolor=gold|'''1st''' || || bgcolor=silver|'''2nd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || 4th || 4th || bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=#cc9966|'''3rd''' || bgcolor=silver|'''2nd''' || bgcolor=silver|'''2nd''' || bgcolor=silver|'''2nd''' || bgcolor=silver|'''2nd'''\n|-\n|align=left|{{bkw|JOR|size=20px}}\n| || || || || || || || || || || || || || || || 11th || || || || \n|-\n|align=left|{{bkw|KAZ|size=20px}}\n| |