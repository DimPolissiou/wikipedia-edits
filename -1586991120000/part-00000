nel, Russia and Ukraine could not be drawn in the same group.\n\n{{#invoke:Sports table|main|style=WDL\n\n|source=[https://www.uefa.com/under19/standings/2020/2001066/ UEFA]\n|show_groups=T\n\n<!--UPDATE standings below (including date)-->\n|update=complete\n<!--Elite round bye-->\n|group_POR=\u2014 |name_POR={{fbu|19|POR}}\n<!--Group 1, replace with country code after teams qualify-->\n|group_TUR='''[[#Group 1|1]]''' |name_TUR={{fbu|19|TUR}}\n|group_BUL='''[[#Group 1|1]]''' |name_BUL={{fbu|19|BUL}}\n|win_TUR=3 |draw_TUR=0 |loss_TUR=0 |gf_TUR=8 |ga_TUR=3 |status_TUR=\n|win_BUL=2 |draw_BUL=0 |loss_BUL=1 |gf_BUL=7 |ga_BUL=3 |status_BUL=\n<!--Group 2, replace with country code after teams qualify-->\n|group_ESP='''[[#Group 2|2]]''' |name_ESP={{fbu|19|ESP}}\n|group_SRB='''[[#Group 2|2]]''' |name_SRB={{fbu|19|SRB}}\n|win_ESP=3 |draw_ESP=0 |loss_ESP=0 |gf_ESP=7 |ga_ESP=0 |status_ESP=\n|win_SRB=1 |draw_SRB=1 |loss_SRB=1 |gf_SRB=9 |ga_SRB=5 |status_SRB=\n<!--Group 3, replace with country code after teams qualify-->\n|group_SCO='''[[#Group 3|3]]''' |name_SCO={{fbu|19|SCO}}\n|group_GER='''[[#Group 3|3]]''' |name_GER={{fbu|19|GER}}\n|win_SCO=2 |draw_SCO=1 |loss_SCO=0 |gf_SCO=5 |ga_SCO=2 |status_SCO=\n|win_GER=2 |draw_GER=0 |loss_GER=1 |gf_GER=12|ga_GER=3 |status_GER=\n<!--Group 4, replace with country code after teams qualify-->\n|group_BEL='''[[#Group 4|4]]''' |name_BEL={{fbu|19|BEL}}\n|group_ISL='''[[#Group 4|4]]''' |name_ISL={{fbu|19|ISL}}\n|win_BEL=3 |draw_BEL=0 |loss_BEL=0 |gf_BEL=6 |ga_BEL=1 |status_BEL=\n|win_ISL=2 |draw_ISL=0 |loss_ISL=1 |gf_ISL=9 |ga_ISL=7 |status_ISL=\n<!--Group 5, replace with country code after teams qualify-->\n|group_WAL='''[[#Group 5|5]]''' |name_WAL={{fbu|19|WAL}}\n|group_RUS='''[[#Group 5|5]]''' |name_RUS={{fbu|19|RUS}}\n|win_WAL=2 |draw_WAL=1 |loss_WAL=0 |gf_WAL=7 |ga_WAL=2 |status_WAL=\n|win_RUS=1 |draw_RUS=2 |loss_RUS=0 |gf_RUS=7 |ga_RUS=3 |status_RUS=\n<!--Group 6, replace with country code after teams qualify-->\n|group_ITA='''[[#Group 6|