erican football roster/Player|first=Malik|last=Jackson|num=88|pos=WR|class=RSO}}\n|defensive_players=\n{{American football roster/Player|first=Cristian|last=Williams|num=3|pos=S|class=RSR}}\n{{American football roster/Player|first=Robert|last=Revels III|num=4|pos=LB|class=RJR}}\n{{American football roster/Player|first=Diamon|last=Cannon|num=6|pos=LB|class=RSR}}\n{{American football roster/Player|first=TieNeal|last=Martin|num=7|pos=S|class=SR}}\n{{American football roster/Player|first=Brandon|last=Johnson|num=8|pos=S|class=SR}}\n{{American football roster/Player|first=Reggie|last=Robinson II|num=9|pos=CB|class=RSR|link=y}}\n{{American football roster/Player|first=Manny|last=Bunch|num=10|pos=S|class=SR}}\n{{American football roster/Player|first=Keidrien|last=Wadley|num=11|pos=S|class=RSR}}\n{{American football roster/Player|first=Allie|last=Green IV|num=12|pos=CB|class=JR}}\n{{American football roster/Player|first=Lamar|last=Mullins|num=13|pos=S|class=JR}}\n{{American football roster/Player|first=Trevis|last=Gipson|num=15|pos=DE|class=RSR|link=y}}\n{{American football roster/Player|first=Reggie|last=Ellis|num=16|pos=CB|class=FR}}\n{{American football roster/Player|first=Tyon|last=Davis|num=18|pos=CB|class=RSO}}\n{{American football roster/Player|first=Grant|last=Sawyer|num=19|pos=LB|class=SO}}\n{{American football roster/Player|first=Kendarin|last=Ray|num=20|pos=S|class=RFR}}\n{{American football roster/Player|first=Bryson|last=Powers|num=21|pos=S|class=RSO}}\n{{American football roster/Player|first=Marcus|last=Mays|num=22|pos=S|class=RSO}}\n{{American football roster/Player|first=Zaven|last=Collins|num=23|pos=LB|class=RSO}}\n{{American football roster/Player|first=Treyvon|last=Reeves|num=24|pos=LB|class=RJR}}\n{{American football roster/Player|first=Jaise|last=Oliver|num=25|pos=S|class=FR}}\n{{American football roster/Player|first=Akayleb|last=Evans|num=26|pos=CB|class=JR}}\n{{American football roster/Player|first=Daiquain|last=Jackson|num=27|pos=S|class=RSO}}\n{{American football roster/Player|first=Jett|last=Hendrix|num=28|pos=S|class=RFR}}\n{{American football roster/Player|first=Ryan|last=Nixon|num=29|pos=CB|class=SO}}\n{{American football roster/Player|first=Justin|last=Wright|num=30|pos=LB|class=RFR}}\n{{American football roster/Player|first=Josiah|last=Wilson|num=33|pos=CB|class=RFR}}\n{{American football roster/Player|first=Yohance|last=Burnett|num=35|pos=LB|class=RJR}}\n{{American football roster/Player|first=Mike|last=Garrett Jr.|num=39|pos=DB|class=JR}}\n{{American football roster/Player|first=Mitchell|last=Kulkin|num=40|pos=LB|class=RFR}}\n{{American football roster/Player|first=Dorian|last=Hopkins|num=41|pos=LB|class=FR}}\n{{American football roster/Player|first=Cooper|last=Edmiston|num=42|pos=LB|class=SR}}\n{{American football roster/Player|first=Cade|last=Baumann|num=44|pos=DT|class=RJR}}\n{{American football roster/Player|first=Bershard|last=Glaspie|num=46|pos=LB|class=FR}}\n{{American football roster/Player|first=Joseph|last=Anderson|num=49|pos=LB|class=RFR}}\n{{American football roster/Player|first=Shemarr|last=Robinson|num=54|pos=DT|class=SR}}\n{{American football roster/Player|first=Deven|last=Lamp|num=58|pos=DE|class=RSO}}\n{{American football roster/Player|first=RaQuan|last=Thompson|num=88|pos=DL|class=FR}}\n{{American football roster/Player|first=Jaxon|last=Player|num=90|pos=DT|class=SO}}\n{{American football roster/Player|first=Cullen|last=Wick|num=91|pos=DE|class=JR}}\n{{American football roster/Player|first=Darrias|last=Murdock|num=92|pos=DL|class=FR}}\n{{American football roster/Player|first=JaJuan|last=Blankenship|num=93|pos=DT|class=RJR}}\n{{American football roster/Player|first=Anthony|last=Goodlow|num=94|pos=LB|class=RFR}}\n{{American football roster/Player|first=Cooper|last=Laake|num=95|pos=DL|class=FR}}\n{{American football roster/Player|first=Tyarise|last=Stevenson|num=97|pos=DT|class=RJR}}\n{{American football roster/Player|first=Lazarus|last=May|num=98|pos=DE|class=FR}}\n|special_teams_player