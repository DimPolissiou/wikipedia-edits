istEntry/VG\n| title = [[Grand Theft Auto (video game)|Grand Theft Auto]]\n| microsoft = MS-DOS<br />[[Microsoft Windows|Windows]]\n| nintendo = [[Game Boy Color|GBC]]\n| sega = no\n| sony = [[PlayStation (console)|PlayStation]]\n| other = no\n| release = 1997<ref name=\"NowGamer: History Of DMA\" />\n| publisher = BMG Interactive, [[Take-Two Interactive]]\n| genre = [[Action-adventure game|Action-adventure]]\n}}\n{{ListEntry/VG\n| title = [[Body Harvest]]\n| microsoft = no\n| nintendo = [[Nintendo 64]]\n| sega = no\n| sony = no\n| other = no\n| release = 1998<ref name=\"NowGamer: History Of DMA\" />\n| publisher = [[Gremlin Interactive]]\n| genre = Action-adventure\n}}\n{{ListEntry/VG\n| title = [[Space Station Silicon Valley]]\n| microsoft = no\n| nintendo = GBC<br />Nintendo 64\n| sega = no\n| sony = PlayStation{{efn|''Space Station Silicon Valley'' was renamed ''Evo's Space Adventures'' on PlayStation.<ref name=\"Jeuxvideo: Evo's Space Adventures\" />}}\n| other = no\n| release = 1998<ref name=\"NowGamer: History Of DMA\" />\n| publisher = Gremlin Interactive\n| genre = Action-adventure\n}}\n{{ListEntry/VG\n| title = [[Tanktics (1999 video game)|Tanktics]]\n| microsoft = MS-DOS<br />Windows\n| nintendo = no\n| sega = no\n| sony = no\n| other = no\n| release = 1998<ref name=\"NowGamer: History Of DMA\" />\n| publisher = Gremlin Interactive\n| genre = Real-time Strategy\n}}\n{{ListEntry/VG\n| title = [[Wild Metal Country]]\n| microsoft = Windows\n| nintendo = no\n| sega = [[Dreamcast]]{{efn|''Wild Metal Country'' was released as ''Wild Metal'' for Dreamcast.<ref name=\"GameSpot: Wild Metal\" />}}\n| sony = no\n| other = no\n| release = 1999<ref name=\"NowGamer: History Of DMA\" />\n| publisher = Gremlin Interactive\n| genre = Action\n}}\n{{ListEntry/VG\n| title = [[Grand Theft Auto 2]]\n| microsoft = Windows\n| nintendo = GBC\n| sega = Dreamcast\n| sony = PlayStation\n| other = no\n| release = 1999<ref name=\"MCV: Sam Houser\" />\n| publisher = [[Rockstar Games]]\