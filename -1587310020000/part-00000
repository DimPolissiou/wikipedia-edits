lete\n|source=[http://slfl.co.uk/lowland-league-table/ SLFL] [https://www.bbc.co.uk/sport/football/lowland-league/table BBC Sport] [https://int.soccerway.com/national/scotland/scottish-football-league/20192020/lowland-league/r52820/ Soccerway]\n\n<!--Update team positions here-->\n|team1=BER |team2=BON|team3=BSC |team4=CAL |team5=CSS|team6=CUM |team7=DAL |team8=EKB |team9=EAS|team10=EDU|team11=GFR |team12=GRE|team13=KEL|team14=SPA |team15=STI |team16=VOL\n\n<!--Update team results here and then (if needed positions above). Don't forget to update the date above (update parameter)-->\n|win_BER=0 |draw_BER=0|loss_BER=0|gf_BER=0|ga_BER=0<!--Berwick Rangers-->\n|win_BON=0|draw_BON=0|loss_BON=0 |gf_BON=0|ga_BON=0<!--Bonnyrigg Rose Athletic-->\n|win_BSC=0|draw_BSC=0|loss_BSC=0 |gf_BSC=0|ga_BSC=0<!--BSC Glasgow-->\n|win_CAL=0|draw_CAL=0|loss_CAL=0|gf_CAL=0|ga_CAL=0<!--Caledonian Braves-->\n|win_CSS=0|draw_CSS=0|loss_CSS=0 |gf_CSS=0|ga_CSS=0<!--Civil Service Strollers-->\n|win_CUM=0|draw_CUM=0|loss_CUM=0|gf_CUM=0|ga_CUM=0<!--Cumbernauld Colts-->\n|win_DAL=0 |draw_DAL=0|loss_DAL=0|gf_DAL=0|ga_DAL=0<!--Dalbeattie Star-->\n|win_EKB=0|draw_EKB=0|loss_EKB=0|gf_EKB=0|ga_EKB=0<!--East Kilbride-->\n|win_EAS=0|draw_EAS=0|loss_EAS=0 |gf_EAS=0|ga_EAS=0<!--East Stirlingshire-->\n|win_EDU=0 |draw_EDU=0|loss_EDU=0|gf_EDU=0|ga_EDU=0<!--Edinburgh University-->\n|win_GFR=0 |draw_GFR=0|loss_GFR=0|gf_GFR=0|ga_GFR=0<!--Gala Fairydean Rovers-->\n|win_GRE=0|draw_GRE=0|loss_GRE=0|gf_GRE=0|ga_GRE=0<!--Gretna 2008-->\n|win_KEL=0|draw_KEL=0|loss_KEL=0 |gf_KEL=0|ga_KEL=0<!--Kelty Hearts-->\n|win_SPA=0|draw_SPA=0|loss_SPA=0 |gf_SPA=0|ga_SPA=0<!--The Spartans-->\n|win_STI=0 |draw_STI=0|loss_STI=0|gf_STI=0|ga_STI=0<!--Stirling University-->\n|win_VOL=0|draw_VOL=0|loss_VOL=0|gf_VOL=0|ga_VOL=0<!--Vale of Leithen-->\n\n<!--Team definitions (wikilinks in table)-->\n|name_BER=[[Berwick Rangers F.C.|Berwick Rangers]]\n|name_BON=[[Bonnyrigg Rose Athletic F.C.|Bonnyrigg Rose Athletic]]\n|name_BSC=[[BSC Glasgow F