l players ==\n{|class=\"sortable wikitable\" style=text-align:center\n!width=130|Player\n!Highest singles ranking\n!Highest doubles ranking\n!First year played\n!Years played\n!Ties played\n!Total W\u2013L\n!Singles W\u2013L\n!Doubles W\u2013L\n|-\n|align=left|{{sortname|Aivaras|Bal\u017eekas}}\n|{{nts|926}}\n|{{nts|939}}\n|1999\n|5\n|19\n|{{sort|1013|10\u201313}}\n|{{sort|0912|9\u201312}}\n|{{sort|0101|1\u20131}}\n|-\n|align=left|{{sortname|Ri\u010dardas|Berankis}}\n|{{nts|67}}\n|{{nts|285}}\n|2007\n|6\n|13\n|{{sort|1709|17\u20139}}\n|{{sort|1405|14\u20135}}\n|{{sort|0304|3\u20134}}\n|-\n|align=left|{{sortname|Mantas|Bugaili\u0161kis}}\n|style=color:#999|{{sort|99999|N/A}}\n|style=color:#999|{{sort|99999|N/A}}\n|2013\n|1\n|1\n|{{sort|0001|0\u20131}}\n|{{sort|0000|0\u20130}}\n|{{sort|0001|0\u20131}}\n|-\n|align=left|{{sortname|Eugenijus|Cariovas}}\n|style=color:#999|{{sort|99999|N/A}}\n|style=color:#999|{{sort|99999|N/A}}\n|2006\n|2\n|8\n|{{sort|0805|8\u20135}}\n|{{sort|0203|2\u20133}}\n|{{sort|0602|6\u20132}}\n|-\n|align=left|{{sort|Celedinas|[[Mindaugas \u010celedinas]]}}\n|style=color:#999|{{sort|99999|N/A}}\n|style=color:#999|{{sort|99999|N/A}}\n|2006\n|2\n|3\n|{{sort|0004|0\u20134}}\n|{{sort|0001|0\u20131}}\n|{{sort|0003|0\u20133}}\n|-\n|align=left|{{sortname|Art\u016bras|Gotovskis}}\n|style=color:#999|{{sort|99999|N/A}}\n|style=color:#999|{{sort|99999|N/A}}\n|2005\n|1\n|2\n|{{sort|0101|1\u20131}}\n|{{sort|0001|0\u20131}}\n|{{sort|0100|1\u20130}}\n|-\n|align=left|{{sortname|Julius|Gotovskis}}\n|style=color:#999|{{sort|99999|N/A}}\n|style=color:#999|{{sort|99999|N/A}}\n|2011\n|1\n|1\n|{{sort|0001|0\u20131}}\n|{{sort|0001|0\u20131}}\n|{{sort|0000|0\u20130}}\n|-\n|align=left|{{sortname|Laurynas|Grigelis}}\n|{{nts|183}}\n|{{nts|132}}\n|2008\n|4\n|9\n|{{sort|0709|7\u20139}}\n|{{sort|0407|4\u20137}}\n|{{sort|0302|3\u20132}}\n|-\n|align=left|{{sortname|Denis|Ivancovas}}\n|style=color:#999|{{sort|99999|N/A}}\n|style=color:#999|{{sort|99999|N/A}}\n|1994\n|3\n|8\n|{{sort