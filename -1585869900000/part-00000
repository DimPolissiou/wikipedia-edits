ete\n|source=[http://www.nifs.no/tabell.php?land=1&t=17&u=667426 nifs.no]\n}}</onlyinclude>\n\n===Group 2===\n\n<onlyinclude>{{#invoke:sports table|main|style=WDL\n|section=Group 2\n|res_col_header=PQR\n\n|team1=NES|name_NES=[[Nest-Sotra Fotball|Nest-Sotra]]\n|team2=NOT|name_NOT=[[Notodden FK|Notodden]]\n|team3=BRY|name_BRY=[[Bryne FK|Bryne]]\n|team4=VID|name_VID=[[FK Vidar|Vidar]]\n|team5=FRL|name_FRL=[[IF Fram Larvik|Fram Larvik]]\n|team6=H\u00d8D|name_H\u00d8D=[[IL H\u00f8dd|H\u00f8dd]]\n|team7=NAR|name_NAR=[[Nardo FK|Nardo]]\n|team8=VAH|name_VAH=[[SK Vard Haugesund|Vard Haugesund]]\n|team9=EGE|name_EGE=[[Egersunds IK|Egersund]]\n|team10=H\u00d8N|name_H\u00d8N=[[H\u00f8nefoss BK|H\u00f8nefoss]]\n|team11=ODD2|name_ODD2=[[Odds BK 2|Odd 2]]\n|team12=VIN|name_VIN=[[Vindbjart FK|Vindbjart]]\n|team13=FAN|name_FAN=[[Fana IL|Fana ]]\n|team14=BY\u00c5|name_BY\u00c5=[[By\u00e5sen Toppfotball|By\u00e5sen]]\n|win_NES=15|draw_NES=9|loss_NES=2|gf_NES=60|ga_NES=22|status_NES=P\n|win_NOT=17|draw_NOT=3|loss_NOT=6|gf_NOT=53|ga_NOT=26|status_NOT=P\n|win_BRY=13|draw_BRY=8|loss_BRY=5|gf_BRY=52|ga_BRY=37\n|win_VID=13|draw_VID=3|loss_VID=10|gf_VID=56|ga_VID=49\n|win_FRL=12|draw_FRL=6|loss_FRL=8|gf_FRL=49|ga_FRL=43\n|win_H\u00d8D=11|draw_H\u00d8D=7|loss_H\u00d8D=8|gf_H\u00d8D=44|ga_H\u00d8D=37\n|win_NAR=12|draw_NAR=4|loss_NAR=10|gf_NAR=29|ga_NAR=35\n|win_VAH=12|draw_VAH=3|loss_VAH=11|gf_VAH=33|ga_VAH=36\n|win_EGE=8|draw_EGE=10|loss_EGE=8|gf_EGE=40|ga_EGE=29\n|win_H\u00d8N=9|draw_H\u00d8N=5|loss_H\u00d8N=12|gf_H\u00d8N=35|ga_H\u00d8N=41\n|win_ODD2=7|draw_ODD2=5|loss_ODD2=14|gf_ODD2=51|ga_ODD2=69\n|win_VIN=6|draw_VIN=4|loss_VIN=16|gf_VIN=44|ga_VIN=58|status_VIN=R\n|win_FAN=5|draw_FAN=7|loss_FAN=14|gf_FAN=30|ga_FAN=64|status_FAN=R\n|win_BY\u00c5=3|draw_BY\u00c5=4|loss_BY\u00c5=19|gf_BY\u00c5=22|ga_BY\u00c5=52|status_BY\u00c5=R\n|col_P=green1|text_P=Promotion to [[2018 1. divisjon|1. divisjon]]\n|result1=P\n|col_QP=green3|text_QP=Qualification for the [[2017 2. divisjon#Promotion_play-offs|