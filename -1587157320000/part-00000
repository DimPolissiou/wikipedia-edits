lay''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |082\n|''Mike the Knight and the Tour of the Castle''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |083\n|''Mike the Knight and the Mr. Cuddles\u2019 Bath Time''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |084\n|''Mike the Knight and the Court Jester''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |085\n|''Mike the Knight and the Great Waldorfini''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |086\n|''Mike the Knight and the Special Knightly Things''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |087\n|''Mike the Knight and the Troll Trail Adventure''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |088\n|''Mike the Knight and the Snoring Dragon''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |089\n|''Mike the Knight and the Evie\u2019s Magical Mistake''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |090\n|''Mike the Knight and the Three Missions''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |091\n|''Mike the Knight and the Super Troll Ball''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |092\n|''Mike the Knight and the Best Lookout''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |093\n|''Mike the Knight and the Knight School''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |094\n|''Mike the Knight and the Galahad\u2019s Birthday''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |095\n|''Mike the Knight and Squirt Sticking to the Plan''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |096\n|''Mike the Knight and the Stolen Staff''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |097\n|''Mike the Knight and the Evie\u2019s Pet Puppies''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |098\n|''Mike the Knight and the Magical Flower''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |099\n|''Mike the Knight and the Size Spell''\n|-\n| colspan=\"2\" |\n|-\n| bgcolor=\"#DFEEEF\" |100\n|''Mike the Knight and Fernando\u2019s Mission''\n|-\n| colspan=\"2\" |\